meMA.Ode3C11.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C11.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C11.incipit = "You made yourself…"
meMA.Ode3C11.text = "You made yourself into a veritable vessel of virtues and a receptacle of the gifts of grace of the Holy Spirit, when you purged yourself of all impurity of passions."

meMA.Ode3C12.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C12.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C12.incipit = "Blessed Saint…"
meMA.Ode3C12.text = "Blessed Saint, you gave no sleep to your eyes, until you became a house of the Holy Trinity and a treasury of wisdom. As such, you enriched the world with your golden teachings."

meMA.Ode3C13.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C13.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C13.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C13.incipit = "O St. Ephraim…"
meMA.Ode3C13.text = "O St. Ephraim, the holy Church of Christ sees in you a second Euphrates River, overflowing with golden streams of wise teachings and irrigating the entire world."

meMA.Ode3C1T.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C1T.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C1T.incipit = "O all-pure Lady…"
meMA.Ode3C1T.text = "O all-pure Lady, one of the Holy Trinity came forth from your pure body, incarnate and in two natures, in a manner fitting God, in His goodness rescuing the descendants of Adam."

