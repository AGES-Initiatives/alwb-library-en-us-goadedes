meMA.Ode6C11.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode6C11.melody = he.h.m6_en_US_goadedes.TouViouTinThalassan.notmetered
meMA.Ode6C11.incipit = "The God-bearing father…"
meMA.Ode6C11.text = "The God-bearing father was built not on a foundation of sand but on unbreakable faith. So he remained unshaken by any of the enemy's assaults, because he was governed by invincible power."

meMA.Ode6C12.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode6C12.melody = he.h.m6_en_US_goadedes.TouViouTinThalassan.notmetered
meMA.Ode6C12.incipit = "O father, your tongue…"
meMA.Ode6C12.text = "O father, your tongue became like the pen of a swift-writing scribe, teaching completely Orthodox knowledge, and inscribing the law of the Spirit on hearts as if on tablets."

meMA.Ode6C13.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C13.mode = misc_en_US_goadedes.Mode6
meMA.Ode6C13.melody = he.h.m6_en_US_goadedes.TouViouTinThalassan.notmetered
meMA.Ode6C13.incipit = "O godly-minded St. Ephraim…"
meMA.Ode6C13.text = "O godly-minded St. Ephraim, those who follow your teachings are like those who sail on a calm sea. They are delivered from soul destroying waves, they rise above the storm of heresies, and they are saved by faith."

meMA.Ode6C1T.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode6C1T.melody = he.h.m6_en_US_goadedes.TouViouTinThalassan.notmetered
meMA.Ode6C1T.incipit = "O Bride of God…"
meMA.Ode6C1T.text = "O Bride of God, He who as God is above all visible and invisible creation assumed flesh and came forth from your womb; and He kept you a virgin, as you were before childbirth."

meMA.Ode6H.ode = misc_en_US_goadedes.Ode6
meMA.Ode6H.mode = misc_en_US_goadedes.Mode6
meMA.Ode6H.melody = he.h.m6_en_US_goadedes.TouViouTinThalassan.melody
