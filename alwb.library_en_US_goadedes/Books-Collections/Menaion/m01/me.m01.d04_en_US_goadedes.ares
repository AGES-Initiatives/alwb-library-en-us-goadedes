meMA.Ode7C11.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_goadedes.misc.Mode6
meMA.Ode7C11.greekmelody = he.h.m6_en_US_goadedes.heHE.DrosovolonMenTinKaminon.greekname
meMA.Ode7C11.melody = he.h.m6_en_US_goadedes.heHE.DrosovolonMenTinKaminon.notmetered
meMA.Ode7C11.incipit = "O Christ, the River…"
meMA.Ode7C11.text = "O Christ, the River of delight, how shall the Jordan River receive You? How shall the Forerunner put his hand on You, who with Your own hand formed him, and also delivered us from the hand of the evil one?"

meMA.Ode7C12.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_goadedes.misc.Mode6
meMA.Ode7C12.melody = he.h.m6_en_US_goadedes.heHE.DrosovolonMenTinKaminon.notmetered
meMA.Ode7C12.incipit = "O Lord, the River…"
meMA.Ode7C12.text = "O Lord, the River of goodness, how can You come to River Jordan, requesting the waters of incorruption? You do so in order to give drink to those who succumbed to the serpent's envy and sank into corruption through wrong eating."

meMA.Ode7C13.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C13.mode = miscellanea_en_US_goadedes.misc.Mode6
meMA.Ode7C13.melody = he.h.m6_en_US_goadedes.heHE.DrosovolonMenTinKaminon.notmetered
meMA.Ode7C13.incipit = "Let the mountains…"
meMA.Ode7C13.text = "Let the mountains drip with gladness, and let the hills leap for joy! Let the rivers clap their hands! Christ has appeared and is coming to bury human failure in the river."

