meMA.Ode4C11.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C11.melody = he.h.m6_en_US_goadedes.ChristosMouDynamis.notmetered
meMA.Ode4C11.incipit = "O all-blessed medaled Martyr…"
meMA.Ode4C11.text = "O all-blessed medaled Martyr, you circled the entire earth, and you held out the branch of contest, which dripped with gladness and sweet fragrance."

meMA.Ode4C12.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C12.melody = he.h.m6_en_US_goadedes.ChristosMouDynamis.notmetered
meMA.Ode4C12.incipit = "…"
meMA.Ode4C12.text = "O blessed Clement, we consider your contest, and it is like a meadow, O glorious Martyr, from which we pluck cures like flowers, and our senses are cheered by your various miracles."

meMA.Ode4C13.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C13.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C13.melody = he.h.m6_en_US_goadedes.ChristosMouDynamis.notmetered
meMA.Ode4C13.incipit = "…"
meMA.Ode4C13.text = "O marvelous Clement, you became a son of the day and of the never-setting light, and you continually shine with tri-solar light and the splendor of your preaching."

meMA.Ode4C1T.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C1T.melody = he.h.m6_en_US_goadedes.ChristosMouDynamis.notmetered
meMA.Ode4C1T.incipit = "…"
meMA.Ode4C1T.text = "Ever-virgin Mary, who were the most pure vessel able to contain God in your body. Please expunge all defilement and every stain from my soul."

