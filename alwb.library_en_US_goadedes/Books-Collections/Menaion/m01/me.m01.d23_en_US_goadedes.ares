meMA.Ode7C11.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode7C11.melody = he.h.m6_en_US_goadedes.OiPaidesEnVavyloni.notmetered
meMA.Ode7C11.incipit = "O Saint, you brightened…"
meMA.Ode7C11.text = "O Saint, you brightened the whole world with the splendor of your martyrdom, and you sang to Christ with a pure mind and soul: \"Blessed are You, O Lord, the God of our fathers.\""

meMA.Ode7C12.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode7C12.melody = he.h.m6_en_US_goadedes.OiPaidesEnVavyloni.notmetered
meMA.Ode7C12.incipit = "…"
meMA.Ode7C12.text = "O Martyr, the sacred celebration of your accomplishments glows with heavenly light, and it shines on us who sing: \"Blessed are You, O Lord, the God of our fathers.\""

meMA.Ode7C1T.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode7C1T.melody = he.h.m6_en_US_goadedes.OiPaidesEnVavyloni.notmetered
meMA.Ode7C1T.incipit = "…"
meMA.Ode7C1T.text = "O Virgin, you became the Mother of the Wisdom that transcends wisdom, who wisely governs the universe with His natural goodness. Blessed is the fruit of your womb, O all-pure Lady."

