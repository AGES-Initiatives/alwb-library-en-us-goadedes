meMA.Ode3C11.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C11.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C11.incipit = "You were a branch…"
meMA.Ode3C11.text = "You were a branch of the life-giving vine, and you brought forth ripe fruits. You garnished them with your splendid martyrdom, and you dedicated them to the Savior of all."

meMA.Ode3C12.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C12.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C12.incipit = "…"
meMA.Ode3C12.text = "O all-blessed Saint, you were an imitator of Paul. You traveled throughout the world and sanctified it with deifying baptism and sprinkling of blood of your confession."

meMA.Ode3C1T.ode = misc_en_US_goadedes.Ode3
meMA.Ode3C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode3C1T.melody = he.h.m6_en_US_goadedes.OukEstinAgios.notmetered
meMA.Ode3C1T.incipit = "…"
meMA.Ode3C1T.text = "No one is free of blame like you, O all-pure Theotokos. You alone of all women in your womb ever conceived and gave birth to the true God, God the Logos, the Creator of all."

