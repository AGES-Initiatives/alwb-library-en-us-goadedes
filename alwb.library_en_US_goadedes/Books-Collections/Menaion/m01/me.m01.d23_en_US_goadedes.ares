meMA.Ode1C11.ode = misc_en_US_goadedes.Ode1
meMA.Ode1C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode1C11.melody = he.h.m6_en_US_goadedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C11.incipit = "O Clement, as you stand…"
meMA.Ode1C11.text = "O Clement, as you stand near Christ with the chorus on high, as a priest and as a glorious Martyr, fervently entreat Him that we who praise you may attain to your illumination."

meMA.Ode1C12.ode = misc_en_US_goadedes.Ode1
meMA.Ode1C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode1C12.melody = he.h.m6_en_US_goadedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C12.incipit = "…"
meMA.Ode1C12.text = "O devout Saint, when you concluded protracted struggles on earth, you were counted worthy to receive the crown of the kingdom of heaven and perpetual and eternal life."

meMA.Ode1C13.ode = misc_en_US_goadedes.Ode1
meMA.Ode1C13.mode = misc_en_US_goadedes.Mode6
meMA.Ode1C13.melody = he.h.m6_en_US_goadedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C13.incipit = "…"
meMA.Ode1C13.text = "O God-bearing Saint and companion of the Lord's Angels, you were redeemed by the Passion of the dispassionate Savior, and you hastened to ascend to Him by your suffering, by emulating His suffering."

meMA.Ode1C1T.ode = misc_en_US_goadedes.Ode1
meMA.Ode1C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode1C1T.melody = he.h.m6_en_US_goadedes.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C1T.incipit = "…"
meMA.Ode1C1T.text = "O immaculate Mother of God, adorned and embroidered with beauty of virtue, you conceived the true God in a manner past understanding, and you made the fountain of blessings spring up for us."

