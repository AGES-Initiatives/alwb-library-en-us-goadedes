meVE.Aposticha1.mode = miscellanea_en_US_goadedes.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_en_US_goadedes.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_en_US_goadedes.heAU.OikosTouEfratha.name
meVE.Aposticha1.incipit = "Let all creation sing…"
meVE.Aposticha1.text = "Let all creation sing * a new song to the Savior, * born of the holy Virgin, * to Christ who in the Jordan River has been baptized today."

meVE.Aposticha2.mode = miscellanea_en_US_goadedes.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_goadedes.heAU.OikosTouEfratha.name
meVE.Aposticha2.incipit = "Let the Forerunner shout…"
meVE.Aposticha2.text = "Let the Forerunner shout * like a prophetic trumpet, * “Produce the fruits of good works * for Jesus Christ the Lord, who is baptized in the streams today.”"

meVE.Aposticha3.mode = miscellanea_en_US_goadedes.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_goadedes.heAU.OikosTouEfratha.name
meVE.Aposticha3.incipit = "From heaven…"
meVE.Aposticha3.text = "From heaven the divine * connatural Spirit shone forth, * descended in the form of * a dove, and came to Christ, and was visible ineffably."

