meMA.Ode6C11.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C11.mode = misc_en_US_goadedes.Mode4
meMA.Ode6C11.melody = he.h.m4_en_US_goadedes.IlthonEisTaVathi.notmetered
meMA.Ode6C11.incipit = "O Peter, the hosts of heaven…"
meMA.Ode6C11.text = "O Peter, the hosts of heaven and the Apostles all celebrate with us on earth, seeing the symbols of your sufferings venerated today."

meMA.Ode6C12.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C12.mode = misc_en_US_goadedes.Mode4
meMA.Ode6C12.melody = he.h.m4_en_US_goadedes.IlthonEisTaVathi.notmetered
meMA.Ode6C12.incipit = "…"
meMA.Ode6C12.text = "O Peter, the saints all applaud and the demons wail aghast, seeing the symbols of your sufferings venerated today."

meMA.Ode6C13.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C13.mode = misc_en_US_goadedes.Mode4
meMA.Ode6C13.melody = he.h.m4_en_US_goadedes.IlthonEisTaVathi.notmetered
meMA.Ode6C13.incipit = "…"
meMA.Ode6C13.text = "O Peter, the soldiers guarding you fell asleep the Angel came. Suddenly, from your hands the the chains fell off. And we joyously venerate them today."

meMA.Ode6C14.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C14.mode = misc_en_US_goadedes.Mode4
meMA.Ode6C14.melody = he.h.m4_en_US_goadedes.IlthonEisTaVathi.notmetered
meMA.Ode6C14.incipit = "…"
meMA.Ode6C14.text = "O Apostle, you raised Tabitha when she was dead; but you deadened the guards and shook off the chains. Everything about you transcends reason. Therefore, with faith we honor you."

meMA.Ode6C1T.ode = misc_en_US_goadedes.Ode6
meMA.Ode6C1T.mode = misc_en_US_goadedes.Mode4
meMA.Ode6C1T.melody = he.h.m4_en_US_goadedes.IlthonEisTaVathi.notmetered
meMA.Ode6C1T.incipit = "…"
meMA.Ode6C1T.text = "O Lady, I am in the winter of perdition, and rivers of countless faults trouble me. Take pity on me, and guide me to a safe place of God."

