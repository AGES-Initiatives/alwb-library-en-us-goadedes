meMA.Ode8C11.ode = misc_en_US_goadedes.Ode8
meMA.Ode8C11.mode = misc_en_US_goadedes.Mode4
meMA.Ode8C11.melody = he.h.m4_en_US_goadedes.LytrotaTouPantos.notmetered
meMA.Ode8C11.incipit = "Law-giver, shepherd…"
meMA.Ode8C11.text = "Law-giver, shepherd, and teacher of the flock of Christ, glorious St. Peter, we who solemnly extol you and faithfully venerate your chains pray you to free us from the bonds of sin."

meMA.Ode8C12.ode = misc_en_US_goadedes.Ode8
meMA.Ode8C12.mode = misc_en_US_goadedes.Mode4
meMA.Ode8C12.melody = he.h.m4_en_US_goadedes.LytrotaTouPantos.notmetered
meMA.Ode8C12.incipit = "…"
meMA.Ode8C12.text = "O Peter, we who honor your chains pray you to dispel demonic disturbances, calm the winter storm of sin, and expel sickness, peril, tribulation, and foreign attacks."

meMA.Ode8C13.ode = misc_en_US_goadedes.Ode8
meMA.Ode8C13.mode = misc_en_US_goadedes.Mode4
meMA.Ode8C13.melody = he.h.m4_en_US_goadedes.LytrotaTouPantos.notmetered
meMA.Ode8C13.incipit = "…"
meMA.Ode8C13.text = "O Apostle, all who touch your precious chain are blessed with grace and filled with holiness, and they sing with joy, \"Bless the Lord and sing a hymn to Him, O all you works of the Lord.\""

meMA.Ode8C14.ode = misc_en_US_goadedes.Ode8
meMA.Ode8C14.mode = misc_en_US_goadedes.Mode4
meMA.Ode8C14.melody = he.h.m4_en_US_goadedes.LytrotaTouPantos.notmetered
meMA.Ode8C14.incipit = "…"
meMA.Ode8C14.text = "The solemn veneration of your holy chains is better than the enjoyment of any party, O Apostle. And you are the host of this godly celebration."

meMA.Ode8C1T.ode = misc_en_US_goadedes.Ode8
meMA.Ode8C1T.mode = misc_en_US_goadedes.Mode4
meMA.Ode8C1T.melody = he.h.m4_en_US_goadedes.LytrotaTouPantos.notmetered
meMA.Ode8C1T.incipit = "…"
meMA.Ode8C1T.text = "O Lady, the deceitful serpent crept up to me, and through bad counsel he got me exiled from the Garden of Eden. But the Angel of Great Counsel, who became human in your womb, made me once again a resident of Paradise."

