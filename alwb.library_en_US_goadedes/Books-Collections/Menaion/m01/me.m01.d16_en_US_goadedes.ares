meMA.Ode7C11.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C11.mode = misc_en_US_goadedes.Mode4
meMA.Ode7C11.melody = he.h.m4_en_US_goadedes.ODiasosasEnPyri.notmetered
meMA.Ode7C11.incipit = "O Apostle, Herod thought he ruled…"
meMA.Ode7C11.text = "O Apostle, Herod thought he ruled but was instead ruled by pleasure. He did what would please the Jews and sentenced you to have your hands bound with chains, which we today honor, as we extol you."

meMA.Ode7C12.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C12.mode = misc_en_US_goadedes.Mode4
meMA.Ode7C12.melody = he.h.m4_en_US_goadedes.ODiasosasEnPyri.notmetered
meMA.Ode7C12.incipit = "…"
meMA.Ode7C12.text = "O Disciple of Christ, all creation yields to you. The sea became walkable, the chains became fibrous, and iron gates were like avenues, all by the power of the Holy Spirit."

meMA.Ode7C13.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C13.mode = misc_en_US_goadedes.Mode4
meMA.Ode7C13.melody = he.h.m4_en_US_goadedes.ODiasosasEnPyri.notmetered
meMA.Ode7C13.incipit = "…"
meMA.Ode7C13.text = "O Peter, you put a stop to false magicians, you raised the dead and the disabled, you cured diseases and healed the infirm by your shadow, you passed through locked gates, and you escaped from the chains, which are now venerated."

meMA.Ode7C14.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C14.mode = misc_en_US_goadedes.Mode4
meMA.Ode7C14.melody = he.h.m4_en_US_goadedes.ODiasosasEnPyri.notmetered
meMA.Ode7C14.incipit = "…"
meMA.Ode7C14.text = "Peter, the horse and Apostle of Christ, shot out from Palestine as out of a starting-gate, and preached to the whole world. He reposed in Old Rome, but he gave his chain to New Rome to be venerated."

meMA.Ode7C1T.ode = misc_en_US_goadedes.Ode7
meMA.Ode7C1T.mode = misc_en_US_goadedes.Mode4
meMA.Ode7C1T.melody = he.h.m4_en_US_goadedes.ODiasosasEnPyri.notmetered
meMA.Ode7C1T.incipit = "…"
meMA.Ode7C1T.text = "O Lady, the Chaldean flame, which did not harm the Servants, signified the novelty of your conception. Please rescue me with the dew of your intercession, for I am inflamed by many transgressions."

