meMA.C2.acrostic.title = "The Iambic Canon, by St. John of Damascus."
meMA.C2.acrostic.notice1 = "The Acrostic formed by the first letter of each line is as follows:"
meMA.C2.acrostic.text = "Today, the Almighty’s good Son, with the pure Spirit’s divine lightning, strikes, burns, buries sin in water. He gives us grace to translate and sing ethereal hymns."
meMA.C2.acrostic.line1.text = "Today, the Almighty’s good Son,"
meMA.C2.acrostic.line2.text = "With the pure Spirit’s divine lightning,"
meMA.C2.acrostic.line3.text = "Strikes, burns, buries sin in water. He gives us grace"
meMA.C2.acrostic.line4.text = "To translate and sing ethereal hymns."
