meMA.Ode4C12.incipit = "The Unmercenaries…"
meMA.Ode4C12.text = "The Unmercenaries sparkle with divine miracles; the impart grace for the benefit of all."

meMA.Ode4C13.ode = miscellanea_en_US_goadedes.misc.Ode4
