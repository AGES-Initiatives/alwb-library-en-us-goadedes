meMA.Ode1C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C11.incipit = "O Porphyrius…"
meMA.Ode1C11.text = "O Porphyrius, I celebrate your luminous memory today, and I entreat you to pray that heavenly illumination be granted to me."

meMA.Ode1C12.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C12.incipit = "You fully dedicated…"
meMA.Ode1C12.text = "You fully dedicated yourself to the Lord from an early age, O Porphyrius, and with heart and soul you cleaved to Him."

meMA.Ode1C13.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C13.incipit = "O Porphyrius…"
meMA.Ode1C13.text = "O Porphyrius, first you mortified all the passions through asceticism; then you became a priest of the Lord of life and death."

meMA.Ode1C1T.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C1T.incipit = "O Virgin Theotokos…"
meMA.Ode1C1T.text = "O Virgin Theotokos, when you received the Rain from heaven in your womb, you stopped the downpour of sin."

meMA.Ode3C1H.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_en_US_goadedes.misc.Mode8
