meMA.Ode4C1H.incipit = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.incipit
meMA.Ode4C1H.text = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.text

meMA.Ode4C11.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C11.greekmelody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.greekname
meMA.Ode4C11.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C11.incipit = "O Saint, you preached…"
meMA.Ode4C11.text = "O Saint, you preached to the deluded that the Logos is equal to the Father, and you drew them to Orthodoxy and dedicated them to the God of all."

meMA.Ode4C12.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C12.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C12.incipit = "Most sacred Porphyrius…"
meMA.Ode4C12.text = "Most sacred Porphyrius, with your vivifying breath you revived a girl who was near death, and this brought about her regeneration and that of many others."

meMA.Ode4C13.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C13.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C13.incipit = "O devout Saint…"
meMA.Ode4C13.text = "O devout Saint, you made of your heart a place for the God of all to dwell, by your praying all night and standing all day."

meMA.Ode4C1T.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C1T.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C1T.incipit = "O all-pure Virgin…"
meMA.Ode4C1T.text = "O all-pure Virgin, you became the mother of Life personified, the Father's Son and Logos, and thus you revive me had been dead."

meMA.Ode5C1H.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C1H.greekmelody = he.h.m8_en_US_goadedes.heHE.FotisonImas.greekname
meMA.Ode5C1H.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.melody
meMA.Ode5C1H.incipit = he.h.m8_en_US_goadedes.heHE.FotisonImas.incipit
meMA.Ode5C1H.text = he.h.m8_en_US_goadedes.heHE.FotisonImas.text

meMA.Ode5C11.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C11.greekmelody = he.h.m8_en_US_goadedes.heHE.FotisonImas.greekname
meMA.Ode5C11.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C11.incipit = "All-venerable Hierarch…"
meMA.Ode5C11.text = "All-venerable Hierarch, you cut off thorny hedonism at the root, and you produced the wheat of good religion."

meMA.Ode5C12.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C12.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C12.incipit = "O wise Porphyrius…"
meMA.Ode5C12.text = "O wise Porphyrius, inundated heaven with your prayers, and it opened and rain watered the earth, but suffocated the weeds of the heretics."

meMA.Ode5C13.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C13.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C13.incipit = "O Saint, by the power…"
meMA.Ode5C13.text = "O Saint, by the power of the Holy Spirit, you piloted to the harbor of life a whole city and its population, and you avoided the stormy waters of impiety."

meMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C1T.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C1T.incipit = "Our Creator chose…"
meMA.Ode5C1T.text = "Our Creator chose you alone, O ever-virgin Theotokos, from all generations; He dwelt in you and refashioned us."

meMA.Ode6C1H.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C1H.greekmelody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C1H.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.melody
meMA.Ode6C1H.incipit = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.incipit
meMA.Ode6C1H.text = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.text

meMA.Ode6C11.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C11.greekmelody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C11.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C11.incipit = "O Porphyrius…"
meMA.Ode6C11.text = "O Porphyrius, you are great example for hierarchs and pastors. Once you mortified the passions through self-control, you obtained what you truly desired. Full of joy, you departed to God. Now you stand near Him, the supreme object of desire. "

meMA.Ode6C12.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C12.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C12.incipit = "Christ, who was…"
meMA.Ode6C12.text = "Christ, who was voluntarily lifted up on the Cross, saw the beauty of your soul, O Saint. He appeared to you, and you saw Him descending from the Cross and telling you to be the custodian of His Cross, showing to you the affection and familiarity you had toward Him."

meMA.Ode6C13.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C13.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C13.incipit = "O preacher of God…"
meMA.Ode6C13.text = "O preacher of God, rich in a zeal and faith like that of Peter the Chief Apostle, with your cutting word of Orthodoxy, you eliminated the wicked woman who had secretly and deceitfully sowed the destructive teachings of Mani."

meMA.Ode6C1T.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C1T.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C1T.incipit = "O immaculate Virgin…"
meMA.Ode6C1T.text = "O immaculate Virgin Mary, you gave birth to the Slayer of death, and to immortal life you transferred Adam, who had become subject to death and decay. By giving a body to God, you made us who were born on earth to be heavenly."

meMA.Kontakion1.mode = miscellanea_en_US_goadedes.misc.Mode2
meMA.Kontakion1.greekmelody = he.a.m2_en_US_goadedes.heAU.ToisTonAimatonSou.greekname
meMA.Kontakion1.melody = he.a.m2_en_US_goadedes.heAU.ToisTonAimatonSou.name
meMA.Kontakion1.incipit = "Being adorned…"
meMA.Kontakion1.text = "Being adorned by your most sacred way of life, * attired in clerical vestments resplendently, * godly-minded, all-blessed Porphyrius, * you are renowned for your healings and miracles, * unceasingly praying on behalf of us all."

meMA.Oikos1.text = "With lightning bolts of miracles, the memory of wise Porphyrius shines brighter than the sun. It brings light to all creation and drives out the deceptive error of idolatry. It illumines the faithful and brings joy to all people. While on earth he was pleasing to God, and so he received the grace to work wonders and to heal everyone. Now he stands near the Trinity, full of light, unceasingly praying on behalf of us all."

meMA.Ode7C1H.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode7C1H.greekmelody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.greekname
meMA.Ode7C1H.melody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.melody
meMA.Ode7C1H.incipit = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.incipit
meMA.Ode7C1H.text = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.text

meMA.Ode7C11.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode7C11.greekmelody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.greekname
meMA.Ode7C11.melody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.notmetered
meMA.Ode7C11.incipit = "O Porphyrius…"
meMA.Ode7C11.text = "O Porphyrius, you shepherded lambs on the pastures of truth, being yourself offered as a lamb to Christ the Shepherd, as you sang, “O Lord our God, You are blessed unto the ages.”"

meMA.Ode7C12.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode7C12.melody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.notmetered
meMA.Ode7C12.incipit = "St. Porphyrius…"
meMA.Ode7C12.text = "St. Porphyrius, Jesus Christ rewards your labors, and He grants you to dwell in the eternal mansions, as you sang, “O Lord our God, You are blessed unto the ages.”"

meMA.Ode7C13.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode7C13.melody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.notmetered
meMA.Ode7C13.incipit = "Thrice-blessed Porphyrius…"
meMA.Ode7C13.text = "Thrice-blessed Porphyrius, Christ adorned your life with many signs and miracles, because you longed for Him, as you sang, “O Lord our God, You are blessed unto the ages.”"

meMA.Ode7C1T.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode7C1T.melody = he.h.m8_en_US_goadedes.heHE.PaidesEvraion.notmetered
meMA.Ode7C1T.incipit = "O Mother…"
meMA.Ode7C1T.text = "O Mother of compassionate God the Logos, pity me and save me. With the light that dwells in you, illuminate me, as I sing, “O all-pure Virgin, blessed is the fruit of your womb.”"

