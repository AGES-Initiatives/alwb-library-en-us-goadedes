meMA.Ode4C1H.greekmelody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.greekname
meMA.Ode4C1H.melody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.melody
meMA.Ode4C1H.incipit = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.incipit
meMA.Ode4C1H.text = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.text

meMA.Ode4C11.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.greekname
meMA.Ode4C11.melody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.notmetered
meMA.Ode4C11.incipit = "You were like…"
meMA.Ode4C11.text = "You were like a dining table generously laden with food for those in need. You fed the hungry with spiritual bread, your God-inspired words.  "

meMA.Ode4C12.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.notmetered
meMA.Ode4C12.incipit = "Wise Father…"
meMA.Ode4C12.text = "Wise Father Tarasius, by vigilance and prayer you removed the debris covering the eyes of the mind, and you attain noetic contemplation of God."

meMA.Ode4C13.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.notmetered
meMA.Ode4C13.incipit = "O wise Father…"
meMA.Ode4C13.text = "O wise Father, mountains of heresy were shaken, hills of ungodliness were cast down, and the foundations of the Church were immovably established, on the strength of your doctrines."

meMA.Ode4C1T.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_en_US_goadedes.heHE.TinAnexichniaston.notmetered
meMA.Ode4C1T.incipit = "Human nature…"
meMA.Ode4C1T.text = "Human nature became weak because of transgression. It is now enveloped in strength because of your childbirth, O pure Virgin, and the enemy's tyrannical strength has become completely weak."

meMA.Ode5C1H.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.greekname
meMA.Ode5C1H.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.melody
meMA.Ode5C1H.incipit = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.incipit
meMA.Ode5C1H.text = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.text

meMA.Ode5C11.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.greekname
meMA.Ode5C11.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C11.incipit = "St. Tarasius…"
meMA.Ode5C11.text = "St. Tarasius, using the whistle of the words, you guided your flock to the pastures of Orthodoxy, to the water of the knowledge of God, and into the fold of Christ. And now, by your intercessions, you preserve it unharmed."

meMA.Ode5C12.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C12.incipit = "O Tarasius…"
meMA.Ode5C12.text = "O Tarasius, you ascended on the chariot of virtue to the height of divine vision; and with your mind's eye you saw your Master's invisible beauty, which intelligibly delighted you."

meMA.Ode5C13.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C13.incipit = "O Hierarch…"
meMA.Ode5C13.text = "O Hierarch, having the reputation of being an inexhaustible river of mercy, you poured out ever-flowing streams of charity to those in need. Therefore Christ has resettled you in a place of delight near the ever-living water."

meMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C1T.incipit = "O pure Virgin…"
meMA.Ode5C1T.text = "O pure Virgin, because of sin the human race was stripped of its original beauty and majesty in the image of God. Now because you conceived God, it is more divinely clothed again."

meMA.Ode6C1H.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.greekname
meMA.Ode6C1H.melody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.melody
meMA.Ode6C1H.incipit = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.incipit
meMA.Ode6C1H.text = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.text

meMA.Ode6C11.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.greekname
meMA.Ode6C11.melody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C11.incipit = "O devout…"
meMA.Ode6C11.text = "O devout and godly-minded St. Tarasius, you removed the veil of ignorance from the hearts of heretics, and to all you revealed the knowledge of true doctrines."

meMA.Ode6C12.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C12.incipit = "O Hierarch…"
meMA.Ode6C12.text = "O Hierarch, you incinerated the thorny prevarications of the heretics, and you spread the light of your correct doctrines to the ends of the earth."

meMA.Ode6C13.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C13.incipit = "As an initiate…"
meMA.Ode6C13.text = "As an initiate of sacred mysteries, you were girded with the strength that comes of God. You proclaimed the truth, and using the spiritual sword of your doctrines, you cut off the heads of falsehood."

meMA.Ode6C1T.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_en_US_goadedes.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C1T.incipit = "O Mother of God…"
meMA.Ode6C1T.text = "O Mother of God, grant that we who venerate the sacred symbols of your virginal conception of God may see your intelligible glory with our mind's eye."

meMA.Kontakion1.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Kontakion1.greekmelody = he.a.m4_en_US_goadedes.heAU.EpefanisSimeron.greekname
meMA.Kontakion1.melody = he.a.m4_en_US_goadedes.heAU.EpefanisSimeron.name
meMA.Kontakion1.incipit = "Like a great…"
meMA.Kontakion1.text = "Like a great and brilliant sun, * shining with dogmas * and aglow with miracles, * you are a guiding light, O Saint, * for Orthodoxy throughout the world, * mystic of heaven, all-blessed Tarasius."

meMA.Oikos1.text = "O Savior, as my soul is darkened by the murk of my offenses, I pray You illumine it with the light of Your mercy. Purify me of the haze of sinful recollections, and turn my mind to good thoughts, O my Christ. Then I will be able to extol Your Hierarch worthily, and to relate the story of his life, recount his illustrious exploits, and describe his inspired faith and the zeal he possessed for the sake of Your Church, which dutifully now extols him and cries out, “Mystic of heaven, all-blessed Tarasius!”"

