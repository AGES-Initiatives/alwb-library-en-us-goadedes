meMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_en_US_goadedes.heHE.EnKyrioTheoMou.greekname
meMA.Ode3C11.melody = he.h.m4_en_US_goadedes.heHE.EnKyrioTheoMou.notmetered
meMA.Ode3C11.incipit = "Open, Temple…"
meMA.Ode3C11.text = "Open, Temple! The Temple of God has come, in order to make us temples of His Holy Spirit."

meMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_goadedes.heHE.EnKyrioTheoMou.notmetered
meMA.Ode3C12.incipit = "The shadow…"
meMA.Ode3C12.text = "The shadow has passed now, and with the coming of grace, the truth is brought in. So receive the Christ now, O Symeon."

meMA.Ode3C13.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_goadedes.heHE.EnKyrioTheoMou.notmetered
meMA.Ode3C13.incipit = "Chaste widow Anna…"
meMA.Ode3C13.text = "Chaste widow Anna, devoted and known to God, give thanks now to Him, who for us has physically became an infant."

meMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_goadedes.heHE.EnKyrioTheoMou.notmetered
meMA.Ode3C1T.incipit = "Let us religiously…"
meMA.Ode3C1T.text = "Let us religiously extol the virgin Mother of God; for she unceasingly intercedes with the Lord on our behalf."

