meMA.Ode8C11.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_en_US_goadedes.heHE.TonVasileaChriston.greekname
meMA.Ode8C11.melody = he.h.m4_en_US_goadedes.heHE.TonVasileaChriston.notmetered
meMA.Ode8C11.incipit = "He, who for us…"
meMA.Ode8C11.text = "He, who for us became an infant like us, submitted to the Law, to free us all from the letter of the Law. O all you works, to the Lord sing a hymn of praise!"

meMA.Ode8C12.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_en_US_goadedes.heHE.TonVasileaChriston.notmetered
meMA.Ode8C12.incipit = "Symeon the Elder…"
meMA.Ode8C12.text = "Symeon the Elder saw You, the noetic Light, and he received You in his arms and said, “O Logos, now You let me depart from this temporal life for the life in the Spirit.”"

meMA.Ode8C13.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_en_US_goadedes.heHE.TonVasileaChriston.notmetered
meMA.Ode8C13.incipit = "Pre-eternal God…"
meMA.Ode8C13.text = "Pre-eternal God was well pleased to become an infant for us and to be held in the arms of the Elder Symeon. O all you works, to the Lord sing a hymn of praise!"

meMA.Ode8C1T.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_en_US_goadedes.heHE.TonVasileaChriston.notmetered
meMA.Ode8C1T.incipit = "Christ the King…"
meMA.Ode8C1T.text = "Christ the King was for us born of the Virgin Mary, who remained a pure virgin after childbirth. O all you works, to the Lord sing a hymn of praise!"

