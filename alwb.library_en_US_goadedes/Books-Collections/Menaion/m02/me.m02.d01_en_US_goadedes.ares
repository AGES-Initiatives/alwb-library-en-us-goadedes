meMA.Ode1C11.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_en_US_goadedes.heHE.AsomaiSoiKyrie.greekname
meMA.Ode1C11.melody = he.h.m4_en_US_goadedes.heHE.AsomaiSoiKyrie.notmetered
meMA.Ode1C11.incipit = "I will sing to You…"
meMA.Ode1C11.text = "I will sing to You, O Lord my God, who physically became an infant, complied with the Law, and saved humanity, for whom You became human."

meMA.Ode1C12.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_en_US_goadedes.heHE.AsomaiSoiKyrie.notmetered
meMA.Ode1C12.incipit = "In his arms…"
meMA.Ode1C12.text = "In his arms the Elder embraced God, the Giver of life, and he was released from life, saying, “Now You let me depart, for today I have seen You.”"

meMA.Ode1C13.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_en_US_goadedes.heHE.AsomaiSoiKyrie.notmetered
meMA.Ode1C13.incipit = "O Christ, long ago…"
meMA.Ode1C13.text = "O Christ, long ago on Mount Sinai You dictated the Law for my sake. Now You yourself submit to the laws of nature, and You follow the regulations to the letter of the Law."

meMA.Ode1C1T.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_en_US_goadedes.heHE.AsomaiSoiKyrie.notmetered
meMA.Ode1C1T.incipit = "I will sing…"
meMA.Ode1C1T.text = "I will sing to You, O Lord my God, for You were born of a holy and virgin Mother, whom You designated to be the hope of our souls."

