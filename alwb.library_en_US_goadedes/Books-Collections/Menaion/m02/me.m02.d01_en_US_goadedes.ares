meMA.Ode5C11.melody = he.h.m4_en_US_goadedes.heHE.OAnateilasToFos.notmetered
meMA.Ode5C11.incipit = "In the shadow…"
meMA.Ode5C11.text = "In the shadow of the Law, to righteous Symeon was revealed the word of grace, as he physically received the Christ."

meMA.Ode5C12.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_en_US_goadedes.heHE.OAnateilasToFos.notmetered
meMA.Ode5C12.incipit = "Then Symeon…"
meMA.Ode5C12.text = "Then Symeon prophesied, “This Child is set for the fall and the rising of many, the fall of unbelievers and the rising of those who believe.”"

meMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_en_US_goadedes.heHE.OAnateilasToFos.notmetered
meMA.Ode5C1T.incipit = "O pure Mother…"
meMA.Ode5C1T.text = "O pure Mother of God, we pray you importune your Son to deliver from every danger and disaster those who glorify you, O immaculate Theotokos."

