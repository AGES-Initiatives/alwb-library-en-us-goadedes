meCO.Ode3C1H.ode = misc_en_US_goadedes.Ode3
meCO.Ode3C1H.mode = misc_en_US_goadedes.Mode2
meCO.Ode3C1H.melody = misc_en_US_goadedes.Heirmos
meCO.Ode3C1H.incipit = he.h.m2_en_US_goadedes.TisPisteosEnPetra.incipit
meCO.Ode3C1H.text = he.h.m2_en_US_goadedes.TisPisteosEnPetra.text

meCO.Ode3C11.ode = misc_en_US_goadedes.Ode3
meCO.Ode3C11.mode = misc_en_US_goadedes.Mode2
meCO.Ode3C11.melody = he.h.m2_en_US_goadedes.TisPisteosEnPetra.name
meCO.Ode3C11.incipit = "In vain does the council…"
meCO.Ode3C11.text = "In vain does the council of unlawful Herod * assemble to plot with murderous intentions * to kill the Master who was born as an infant. * O Christ. We sing to You * and say, \"You are our God, * and no one is holy but You, O Lord.\""

meCO.Ode3C12.ode = misc_en_US_goadedes.Ode3
meCO.Ode3C12.mode = misc_en_US_goadedes.Mode2
meCO.Ode3C12.melody = he.h.m2_en_US_goadedes.TisPisteosEnPetra.name
meCO.Ode3C12.incipit = "Today, the wicked council…"
meCO.Ode3C12.text = "Today, the wicked council of lawless Herod, * whose soul opposes God, confers in order * to kill the Master together with the infants. * O Christ, we sing to You * and say, \"You are our God, * and no one is holy but You, O Lord.\""

meCO.Ode8C1H.ode = misc_en_US_goadedes.Ode8
meCO.Ode8C1H.mode = misc_en_US_goadedes.Mode2
meCO.Ode8C1H.melody = misc_en_US_goadedes.Heirmos
meCO.Ode8C1H.incipit = he.h.m2_en_US_goadedes.RimaTyrannou.incipit
meCO.Ode8C1H.text = he.h.m2_en_US_goadedes.RimaTyrannou.text

meCO.Ode8C11.ode = misc_en_US_goadedes.Ode8
meCO.Ode8C11.mode = misc_en_US_goadedes.Mode2
meCO.Ode8C11.melody = he.h.m2_en_US_goadedes.RimaTyrannou.name
meCO.Ode8C11.incipit = "Come let us pour out…"
meCO.Ode8C11.text = "Come let us pour out our tears like costly fragrant oil * to Christ who is born in the flesh for our sake, * Let us purify our flesh of every stain * and purely approach the immaculate One, * as we cry out and sing, \"All you works of the Lord, sing a hymn to the Lord, * and exalt Him beyond measure unto all the ages.\""

meCO.Ode8C12.ode = misc_en_US_goadedes.Ode8
meCO.Ode8C12.mode = misc_en_US_goadedes.Mode2
meCO.Ode8C12.melody = he.h.m2_en_US_goadedes.RimaTyrannou.name
meCO.Ode8C12.incipit = "Come, fellow sinners…"
meCO.Ode8C12.text = "Come, fellow sinners, repent with compunctious tears * that we escape the tears of hell and perdition to come. * Let us follow in the steps of Jesus Christ, * who is wrapped in swaddling clothes as an infant, * as we cry out and sing, \"All you works of the Lord, sing a hymn to the Lord, * and exalt Him beyond measure unto all the ages.\""

meCO.Ode8C13.ode = misc_en_US_goadedes.Ode8
meCO.Ode8C13.mode = misc_en_US_goadedes.Mode2
meCO.Ode8C13.melody = he.h.m2_en_US_goadedes.RimaTyrannou.name
meCO.Ode8C13.incipit = "Knowing that the sacrament…"
meCO.Ode8C13.text = "Knowing that the sacrament of our ransom * is accomplished through repentance and an abundance of tears, * and also through confession, come, O believers, * let us cleanse ourselves and thus go to Christ, who is born in the flesh, * as we cry out and sing, \"All you works of the Lord, sing a hymn to the Lord, * and exalt Him beyond measure unto all the ages.\""

meCO.Ode9C1H.ode = misc_en_US_goadedes.Ode9
meCO.Ode9C1H.mode = misc_en_US_goadedes.Mode2
meCO.Ode9C1H.melody = misc_en_US_goadedes.Heirmos
meCO.Ode9C1H.incipit = he.h.m2_en_US_goadedes.PsychaisKatharais.incipit
meCO.Ode9C1H.text = he.h.m2_en_US_goadedes.PsychaisKatharais.text

meCO.Ode9C11.ode = misc_en_US_goadedes.Ode9
meCO.Ode9C11.mode = misc_en_US_goadedes.Mode2
meCO.Ode9C11.melody = he.h.m2_en_US_goadedes.PsychaisKatharais.name
meCO.Ode9C11.incipit = "Ungrateful and jealous…"
meCO.Ode9C11.text = "Ungrateful and jealous let nobody prove to be * who today is offering the gifts befitting God, * and is singing to Christ who is born now. * Instead of frankincense and gold and myrrh bring the fragrance of virtue. * \"Spare our souls, O Christ our God, and save us, we pray.\""

meCO.Ode9C12.ode = misc_en_US_goadedes.Ode9
meCO.Ode9C12.mode = misc_en_US_goadedes.Mode2
meCO.Ode9C12.melody = he.h.m2_en_US_goadedes.PsychaisKatharais.name
meCO.Ode9C12.incipit = "Speaking to the Wise Men…"
meCO.Ode9C12.text = "Speaking to the Wise Men, Herod said with hardened heart, * \"Go and search diligently for the newborn King, * and when you have found him, return here and bring me word.\" * All along that awful man was plotting murder Him in cold blood. * \"Spare our souls, O Christ our God, and save us, we pray.\""

meCO.Ode9C13.ode = misc_en_US_goadedes.Ode9
meCO.Ode9C13.mode = misc_en_US_goadedes.Mode2
meCO.Ode9C13.melody = he.h.m2_en_US_goadedes.PsychaisKatharais.name
meCO.Ode9C13.incipit = "Oh the shameless act…"
meCO.Ode9C13.text = "Oh the shameless act of your blinding infanticide! * Therefore you forgot the fact that you could not take God * into your control and then destroy Him. * Seething with anger and rage, * you slaughtered the innocents, O Herod. * \"Spare our souls, O Christ our God, and save us, we pray.\""

