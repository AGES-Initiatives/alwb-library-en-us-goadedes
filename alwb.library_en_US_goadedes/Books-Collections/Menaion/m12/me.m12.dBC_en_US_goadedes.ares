meMA.Ode4C11.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C11.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C11.melody = he.h.m6_en_US_goadedes.TinEnStavroSou.notmetered
meMA.Ode4C11.incipit = "O Christ God, Your divine descent…"
meMA.Ode4C11.text = "O Christ God, Your divine descent into Hades proved to be life to the dead; for You, O good One, bound the enemies below, and You opened the path to heaven for humanity."

meMA.Ode4C12.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C12.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C12.melody = he.h.m6_en_US_goadedes.TinEnStavroSou.notmetered
meMA.Ode4C12.incipit = "We are honoring the cream…"
meMA.Ode4C12.text = "We are honoring the cream of the Fathers: Abraham, Isaac and Jacob; for it was from their seed that Christ Almighty shone forth, incarnate from a Virgin."

meMA.Ode4C13.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C13.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C13.melody = he.h.m6_en_US_goadedes.TinEnStavroSou.notmetered
meMA.Ode4C13.incipit = "Daniel made the lions…"
meMA.Ode4C13.text = "Daniel made the lions be like lambs for all to see, and thus he prefigured what would happen at Christ's descent. As a Prophet of God, he foreknew the future."

meMA.Ode4C14.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C14.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C14.melody = he.h.m6_en_US_goadedes.TinEnStavroSou.notmetered
meMA.Ode4C14.incipit = "Those Servants had not ingested…"
meMA.Ode4C14.text = "Those Servants had not ingested the poison of sin, and thus they were rescued from the flames. For they were truly more precious than gold, and they did not melt in the pagans' furnace."

meMA.Ode4C1T.ode = misc_en_US_goadedes.Ode4
meMA.Ode4C1T.mode = misc_en_US_goadedes.Mode6
meMA.Ode4C1T.melody = he.h.m6_en_US_goadedes.TinEnStavroSou.notmetered
meMA.Ode4C1T.incipit = "O immaculate and noble Lady…"
meMA.Ode4C1T.text = "O immaculate and noble Lady, your Son, who is the expectation of the Gentiles and salvation of the world, is loudly announced. The assemblage of the Fathers extols Him today."

