meMA.Ode6C11.incipit = "With a bright star…"
meMA.Ode6C11.text = "With a bright star, heaven shows Astrologers that the heavenly King is born on earth for us now in the city of David."

meMA.Ode6C12.ode = miscellanea_en_US_goadedes.misc.Ode6
