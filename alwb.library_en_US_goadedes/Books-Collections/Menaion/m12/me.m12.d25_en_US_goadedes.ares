meMA.C2.acrostic.title = "The Iambic Canon, by St. John of Damascus."
meMA.C2.acrostic.notice1 = "The Acrostic formed by the first letter of each line is as follows:"
meMA.C2.acrostic.text = "With poetry we praise the Son of God, born in the flesh for humanity’s sake, freeing the world from pain and death. From which save me, who translates for You, my King."
meMA.C2.acrostic.line1.text = "With poetry we praise the Son of God,"
meMA.C2.acrostic.line2.text = "Born in the flesh for humanity’s sake,"
meMA.C2.acrostic.line3.text = "Freeing the world from pain and death."
meMA.C2.acrostic.line4.text = "From which save me, who translates for You, my King."
meMA.C2.poet = poets_en_US_goadedes.po.IoannouTouDamaskinou
meMA.C2.translator = "The Canon with the acrostic was translated by Fr. Seraphim Dedes."
