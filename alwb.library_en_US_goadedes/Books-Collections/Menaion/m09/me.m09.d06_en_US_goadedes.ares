meMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_en_US_goadedes.heHE.OukEnSofia.greekname
meMA.Ode3C11.melody = he.h.m4_en_US_goadedes.heHE.OukEnSofia.notmetered
meMA.Ode3C11.incipit = "O all-lauded Captain…"
meMA.Ode3C11.text = "O all-lauded Captain of the Angels, strengthened by God, you go around the whole earth, saving us from calamities, as we call on your godlike name."

meMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_goadedes.heHE.OukEnSofia.notmetered
meMA.Ode3C12.incipit = "Most godlike Chief…"
meMA.Ode3C12.text = "Most godlike Chief Captain of God, you are a divine herald, a reliable defender of believers, a guide and instructor of the errant."

meMA.Ode3C13.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_goadedes.heHE.OukEnSofia.notmetered
meMA.Ode3C13.incipit = "O marvelous Archangel…"
meMA.Ode3C13.text = "O marvelous Archangel Michael, you are a clean mirror of the divine light, for you resplendently receive the reflections of the Holy Spirit."

meMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_goadedes.heHE.OukEnSofia.notmetered
meMA.Ode3C1T.incipit = "All-blameless Virgin…"
meMA.Ode3C1T.text = "All-blameless Virgin, the One who by His will created the immaterial Intellects again by His will dwelt in your material womb. The invisible One was seen in flesh."

