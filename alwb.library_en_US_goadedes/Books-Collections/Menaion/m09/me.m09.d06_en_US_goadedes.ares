meMA.Ode7C11.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_en_US_goadedes.heHE.ODiasosasEnPyri.greekname
meMA.Ode7C11.melody = he.h.m4_en_US_goadedes.heHE.ODiasosasEnPyri.notmetered
meMA.Ode7C11.incipit = "Blessed refulgent…"
meMA.Ode7C11.text = "Blessed refulgent Gabriel, as Daniel looked, he saw you standing before him at the Ulai River and instructing him in the vision he saw; and then you made Michael known to him."

meMA.Ode7C12.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_en_US_goadedes.heHE.ODiasosasEnPyri.notmetered
meMA.Ode7C12.incipit = "Let us faithfully…"
meMA.Ode7C12.text = "Let us faithfully lift our voices and extol Michael and Gabriel, the leaders of the Angels, the deiform bringers of light, our good helpers, protectors, and guardians."

meMA.Ode7C13.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C13.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_en_US_goadedes.heHE.ODiasosasEnPyri.notmetered
meMA.Ode7C13.incipit = "Great and luminous…"
meMA.Ode7C13.text = "Great and luminous Michael, you are handsome, majestic, and deiform; you were appointed leader of the spiritual bodiless ministers. We pray that all of you implore God on our behalf."

meMA.Ode7C1T.ode = miscellanea_en_US_goadedes.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_en_US_goadedes.heHE.ODiasosasEnPyri.notmetered
meMA.Ode7C1T.incipit = "O Virgin…"
meMA.Ode7C1T.text = "O Virgin, when the Archangel Gabriel saw the eternal Logos of the truly great Mind ineffably and awesomely residing in you, he saluted you and said, “Rejoice, blessed throne of God Most High!”"

