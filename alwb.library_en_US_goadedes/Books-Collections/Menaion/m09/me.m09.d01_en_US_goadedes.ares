meMA.Ode5C31.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C31.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C31.greekmelody = he.h.m8_en_US_goadedes.heHE.FotisonImas.greekname
meMA.Ode5C31.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C31.incipit = "O Symeon, when Christ…"
meMA.Ode5C31.text = "O Symeon, when Christ brought you out of a ferocious pit unharmed, He showed you to be like the Prophet Daniel. "

meMA.Ode5C32.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C32.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C32.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C32.incipit = "You dedicated yourself …"
meMA.Ode5C32.text = "You dedicated yourself entirely to the Lord, and you did your ascetic exercises in both extreme cold and extreme heat."

meMA.Ode5C33.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C33.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C33.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C33.incipit = "O devout Symeon…"
meMA.Ode5C33.text = "O devout Symeon, during Lent, you ate nothing for forty days. Thus you were like Moses and Elijah. "

meMA.Ode5C3T.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C3T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode5C3T.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.notmetered
meMA.Ode5C3T.incipit = "O pure Virgin Mary…"
meMA.Ode5C3T.text = "O pure Virgin Mary, ever implore your Son and our God to send down His mercy on us who believe in Him."

meMA.Ode6C1H.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode6C1H.greekmelody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.greekname
meMA.Ode6C1H.melody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.melody
meMA.Ode6C1H.incipit = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.incipit
meMA.Ode6C1H.text = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.text

meMA.Ode6C11.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode6C11.greekmelody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.greekname
meMA.Ode6C11.melody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.name
meMA.Ode6C11.incipit = "With the start of the year…"
meMA.Ode6C11.text = "With the start of the year, O Lord, grant us also to start anew * to live in a way that is pleasing to You, our benevolent God."

meMA.Ode6C12.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode6C12.melody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.name
meMA.Ode6C12.incipit = "O compassionate Savior…"
meMA.Ode6C12.text = "O compassionate Savior, grant Your servants who worship You * to live according to the spirit of Your law for many years."

meMA.Ode6C1T.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode6C1T.melody = he.h.m1_en_US_goadedes.heHE.TonProfitinDiesosas.name
meMA.Ode6C1T.incipit = "We implore you…"
meMA.Ode6C1T.text = "We implore you to rescue us from impending calamity. * We have learned to trust you, O Lady and Mother of Christ our God."

