meMA.Ode3C1H.incipit = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.incipit
meMA.Ode3C1H.text = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.text

meMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode3C11.greekmelody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.greekname
meMA.Ode3C11.melody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.name
meMA.Ode3C11.incipit = "Establish Your holy Church…"
meMA.Ode3C11.text = "Establish Your holy Church, * which is the fruitful vine that Your right hand * with love and wisdom and has planted upon the earth. * We pray You guard and protect her, O almighty Lord."

meMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode3C12.melody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.name
meMA.Ode3C12.incipit = "We pray You, O God…"
meMA.Ode3C12.text = "We pray You, O God of all, * to count us worthy to spend this whole year * performing spiritual works that will gladden You, * O Lord, as we faithfully sing You our hymns of praise."

meMA.Ode3C13.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode3C13.melody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.name
meMA.Ode3C13.incipit = "O merciful Savior Christ…"
meMA.Ode3C13.text = "O merciful Savior Christ, * I pray you grant to me that this whole year * may be serene. And fill me with Your godly words * like the ones You spoke to the Jews on the sabbath day."

meMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode3C1T.melody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.name
meMA.Ode3C1T.incipit = "O Lady, you are the one…"
meMA.Ode3C1T.text = "O Lady, you are the one * who wondrously received superhuman * and divine grace, for immutably Christ our God * abode in your holy womb. Therefore we glorify you."

