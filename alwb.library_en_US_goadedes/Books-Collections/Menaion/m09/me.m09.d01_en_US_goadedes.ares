meMA.Ode8C11.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode8C11.greekmelody = he.h.m1_en_US_goadedes.heHE.TonTousYmnologousEnKamino.greekname
meMA.Ode8C11.melody = he.h.m1_en_US_goadedes.heHE.TonTousYmnologousEnKamino.name
meMA.Ode8C11.incipit = "To You, the author of salvation…"
meMA.Ode8C11.text = "To You, the author of salvation, * the holy Church again offers * her annual firstfruits, O Christ, as she sings to You: * “Sing praises to Christ our God, and exalt Him beyond measure unto all the ages.”"

meMA.Ode8C12.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode8C12.melody = he.h.m1_en_US_goadedes.heHE.TonTousYmnologousEnKamino.name
meMA.Ode8C12.incipit = "He brought universe…"
meMA.Ode8C12.text = "He brought universe into being * from non-existence wisely, * and by His will He ordered the changing of the seasons * Sing praises to Christ our God, and exalt Him beyond measure unto all the ages."

meMA.Ode8C13.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C13.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode8C13.melody = he.h.m1_en_US_goadedes.heHE.TonTousYmnologousEnKamino.name
meMA.Ode8C13.incipit = "Let us sing to God…"
meMA.Ode8C13.text = "Let us sing to God, for He arranges * everything and also changes * the seasons for the complex administration of humanity. * Sing praises to Christ our God, and exalt Him beyond measure unto all the ages."

meMA.Ode8C1T.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode8C1T.melody = he.h.m1_en_US_goadedes.heHE.TonTousYmnologousEnKamino.name
meMA.Ode8C1T.incipit = "We your people…"
meMA.Ode8C1T.text = "We your people, as Orthodox Christians, * throughout the year sing your praises, * O immaculate Virgin and Mother of God. * We all extol you, O Theotokos, for you are our salvation."

