meMA.Ode6C31.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C31.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C31.greekmelody = he.h.m8_en_US_goadedes.heHE.ChitonaMoiParaschou.greekname
meMA.Ode6C31.melody = he.h.m8_en_US_goadedes.heHE.ChitonaMoiParaschou.notmetered
meMA.Ode6C31.incipit = "Christ made you a worker…"
meMA.Ode6C31.text = "Christ made you a worker of signs and wonders, O blessed Saint, and He showed you to be a dwelling place of divine energy."

meMA.Ode6C32.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C32.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C32.melody = he.h.m8_en_US_goadedes.heHE.ChitonaMoiParaschou.notmetered
meMA.Ode6C32.incipit = "O Symeon, your body…"
meMA.Ode6C32.text = "O Symeon, your body was lifted on the column as if on a cross. Thus you have been glorified with Christ, who for you was lifted on a tree."

meMA.Ode6C33.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C33.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C33.melody = he.h.m8_en_US_goadedes.heHE.ChitonaMoiParaschou.notmetered
meMA.Ode6C33.incipit = "O inspired St. Symeon…"
meMA.Ode6C33.text = "O inspired St. Symeon, you found a pathway in the air. We pray you carry up to the heights of heaven those who faithfully extol you."

meMA.Ode6C3T.ode = miscellanea_en_US_goadedes.misc.Ode6
meMA.Ode6C3T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode6C3T.melody = he.h.m8_en_US_goadedes.heHE.ChitonaMoiParaschou.notmetered
meMA.Ode6C3T.incipit = "O Theotokos, we the faithful…"
meMA.Ode6C3T.text = "O Theotokos, we the faithful declare you to be the temple and ark of God, the living bridal chamber and the heavenly gate."

meMA.Kontakion1.poet = poets_en_US_goadedes.po.PatriarchouKyrillou
meMA.Kontakion1.mode = miscellanea_en_US_goadedes.misc.Mode3
meMA.Kontakion1.greekmelody = he.a.m3_en_US_goadedes.heAU.IParthenosSimeron.greekname
meMA.Kontakion1.melody = he.a.m3_en_US_goadedes.heAU.IParthenosSimeron.name
meMA.Kontakion1.incipit = "You created the universe…"
meMA.Kontakion1.text = "You created the universe * in Your ineffable wisdom, * and by Your authority * You fixed the times and the seasons. * Grant success to all Your Christ-loving faithful people. * Bless this year, O Lord, and all our comings and goings. * We entreat You to direct us and all our actions * to conform to Your divine will."

