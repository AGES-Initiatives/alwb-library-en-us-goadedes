meMA.Ode4C31.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C31.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C31.greekmelody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.greekname
meMA.Ode4C31.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C31.incipit = "All blessed one, you laid…"
meMA.Ode4C31.text = "All blessed one, you laid the foundation of your ascetic life not on sand, but on your profound labors; and you built an unshakable tower of virtues."

meMA.Ode4C32.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C32.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C32.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C32.incipit = "With a rough rope binding…"
meMA.Ode4C32.text = "With a rough rope binding your body, you nailed it to the fear of the spirit, and you obtained a divine allotment, O devout father."

meMA.Ode4C33.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C33.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C33.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C33.incipit = "Venerable father, as you slept…"
meMA.Ode4C33.text = "Venerable father, as you slept in squalor, the hidden passions of the body fled from you, and the worms were afraid of you."

meMA.Ode4C34.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C34.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C34.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C34.incipit = "You voluntarily lived…"
meMA.Ode4C34.text = "You voluntarily lived in a dark well as if in a tomb, thus imitating Christ who after His voluntary Passion lay in the tomb as a life-giving corpse."

meMA.Ode4C3T.ode = miscellanea_en_US_goadedes.misc.Ode4
meMA.Ode4C3T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode4C3T.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.notmetered
meMA.Ode4C3T.incipit = "O immaculate Mary…"
meMA.Ode4C3T.text = "O immaculate Mary, we pray you always implore your Son and God, that He grant forgiveness of offences to us who entreat you."

meMA.Ode5C1H.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode5C1H.greekmelody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.greekname
meMA.Ode5C1H.melody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.melody
