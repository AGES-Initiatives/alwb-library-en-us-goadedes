meMA.Ode5C1H.text = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.text

meMA.Ode5C11.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode5C11.greekmelody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.greekname
meMA.Ode5C11.melody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.name
meMA.Ode5C11.incipit = "You are He who fills…"
meMA.Ode5C11.text = "You are He who fills all things with goodness, O my Christ. * Your servants pray You crown the year with Your blessings of abundant fruits; * and grant temperate weather in every season, O Lord."

meMA.Ode5C12.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode5C12.melody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.name
meMA.Ode5C12.incipit = "May we see improvement…"
meMA.Ode5C12.text = "May we see improvement in ourselves as we advance * from year to year, O gracious Lord, may we also have a peaceful life, * as we know You, the Father’s divine coeternal Word."

meMA.Ode5C13.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode5C13.melody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.name
meMA.Ode5C13.incipit = "You who with the Father…"
meMA.Ode5C13.text = "You who with the Father are eternal came to earth, * and You, O Lord, proclaimed release to the captives and recovering * of their sight to the blind from the Father, timeless God."

meMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode5C1T.melody = he.h.m1_en_US_goadedes.heHE.EkNyktosOrthrizontesYmnoumenSe.name
meMA.Ode5C1T.incipit = "We rely on you…"
meMA.Ode5C1T.text = "We rely on you, all-holy Mother of our God, * and we have put our hope in you; and we ask you to intercede * with your Son, blessed Virgin, to treat us with leniency."

