meMA.Ode9C1H.text = he.h.m1_en_US_goadedes.heHE.TyponTisAgnis.text

meMA.Ode9C11.ode = miscellanea_en_US_goadedes.misc.Ode9
meMA.Ode9C11.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode9C11.greekmelody = he.h.m1_en_US_goadedes.heHE.TyponTisAgnis.greekname
meMA.Ode9C11.melody = he.h.m1_en_US_goadedes.heHE.TyponTisAgnis.name
meMA.Ode9C11.incipit = "As the hypostatic Word…"
meMA.Ode9C11.text = "As the hypostatic Word of God, * indeed His Wisdom and Power, O Christ our Lord, * You sustain all things and hold together the universe. * We Your servants implore You to grant to us * a peaceful life, O Savior, * during the course of the coming year."

meMA.Ode9C12.ode = miscellanea_en_US_goadedes.misc.Ode9
meMA.Ode9C12.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode9C12.melody = he.h.m1_en_US_goadedes.heHE.TyponTisAgnis.name
meMA.Ode9C12.incipit = "Pre-eternal Holy Trinity…"
meMA.Ode9C12.text = "Pre-eternal Holy Trinity, * You are the only Godhead in three hypostases * indivisible Maker and King and of the ages all. * Hear the pure Theotokos who prays for us, * and grant to us, Your people, * a very peaceful and successful year."

meMA.Ode9C1T.ode = miscellanea_en_US_goadedes.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode9C1T.melody = he.h.m1_en_US_goadedes.heHE.TyponTisAgnis.name
meMA.Ode9C1T.incipit = "Ruler over all…"
meMA.Ode9C1T.text = "Ruler over all and Fashioner * of all creation, Savior, Lord of the universe, * grant peace to Your world, and never cease to preserve the Church * undisrupted by schism or heresy, * heeding the intercessions * of the all-holy virgin Mother of God. "

