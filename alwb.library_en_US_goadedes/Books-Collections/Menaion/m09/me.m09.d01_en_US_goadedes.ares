meMA.Ode1C31.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C31.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C31.greekmelody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrioPantesLaoi.greekname
meMA.Ode1C31.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrioPantesLaoi.notmetered
meMA.Ode1C31.incipit = "As I weave this song…"
meMA.Ode1C31.text = "As I weave this song for you, with the broken instrument of my tongue, I ask you to pray that I receive the light of knowledge taught by God, O God-bearing Symeon."

meMA.Ode1C32.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C32.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C32.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrioPantesLaoi.notmetered
meMA.Ode1C32.incipit = "O father, many Persians…"
meMA.Ode1C32.text = "O father, many Persians, Ethiopians, Indians, Scyths, and Arabs came to know your wisdom, and they glorified Christ, who was glorified through you."

meMA.Ode1C33.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C33.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C33.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrioPantesLaoi.notmetered
meMA.Ode1C33.incipit = "O blessed Saint…"
meMA.Ode1C33.text = "O blessed Saint, you were filled with the grace of the Spirit. Like Jacob, David and Moses, you were a shepherd, a leader of rational flocks."

meMA.Ode1C3T.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C3T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C3T.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrioPantesLaoi.notmetered
meMA.Ode1C3T.incipit = "We salute you…"
meMA.Ode1C3T.text = "We salute you, O immaculate Theotokos: Rejoice, blessed Virgin! In your womb you contained uncontainable God. Pray to Him for us who extol you, and entreat Him to deliver us from evils."

meMA.Ode3C1H.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_en_US_goadedes.misc.Mode1
meMA.Ode3C1H.greekmelody = he.h.m1_en_US_goadedes.heHE.StereosonMeChriste.greekname
