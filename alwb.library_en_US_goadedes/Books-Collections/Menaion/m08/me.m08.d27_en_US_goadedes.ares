alt1.meLI.MM.CommunionHymn.text = properties_en_US_goadedes.media.key

alt1.meLI.Megalynarion.text = "We salute you, holy athlete of Christ: * Rejoice! For you sympathize with all, answering their prayers. * Rejoice! For you greatly assist all pious Christians. * Now all the Church rejoices in you, St. Phanourios."
alt1.meLI.Megalynarion2.text = ""
