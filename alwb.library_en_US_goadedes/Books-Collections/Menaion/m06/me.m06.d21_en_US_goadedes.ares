meMA.Ode1C11.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C11.greekmelody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.greekname
meMA.Ode1C11.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C11.incipit = "Your acclaimed…"
meMA.Ode1C11.text = "Your acclaimed and highly honored memory is celebrated with sacred hymns and glorified with unapproachable glory, O athlete St. Julian."

meMA.Ode1C12.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C12.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C12.incipit = "As you competed…"
meMA.Ode1C12.text = "As you competed in martyrdom to the point of blood, you did not stumble due to any infirmity of the flesh, O soldier of Christ."

meMA.Ode1C13.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C13.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.notmetered
meMA.Ode1C13.incipit = "The Saint is decorated…"
meMA.Ode1C13.text = "The Saint is decorated by his martyric torments, shining brighter than gold and any precious stone."

meMA.Ode1C1T.ode = miscellanea_en_US_goadedes.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode1C1T.melody = he.h.m8_en_US_goadedes.heHE.AsomenToKyrio.name
meMA.Ode1C1T.incipit = "Mother of God…"
meMA.Ode1C1T.text = "Mother of God and Virgin, * in a way transcending nature you gave birth * to the incarnate Logos * who is God beyond divine. We sing your praise."

meMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode3C11.greekmelody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.greekname
meMA.Ode3C11.melody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.name
meMA.Ode3C11.incipit = "Julian responded…"
meMA.Ode3C11.text = "Julian responded to the deluded judge, “I have been commanded not to worship handcrafted stone objects as gods.”"

meMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode3C12.melody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.name
meMA.Ode3C12.incipit = "Julian, Martyr…"
meMA.Ode3C12.text = "Julian, Martyr of Christ, you appeared before the tyrant's court, while standing in the presence of the Judge of the living and the dead."

meMA.Ode3C13.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode3C13.melody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.name
meMA.Ode3C13.incipit = "Julian said…"
meMA.Ode3C13.text = "Julian said, “I am not crazy in confessing the one and only God, who is praised and worshiped as indivisible Trinity.”"

meMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode3C1T.melody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.name
meMA.Ode3C1T.incipit = "O all-pure one…"
meMA.Ode3C1T.text = "O all-pure one, give to us * your great assistance through your intense and ardent prayers, * and repel the grievous circumstances besetting us."

meMA.Ode3H.ode = miscellanea_en_US_goadedes.misc.Ode3
meMA.Ode3H.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode3H.greekmelody = he.h.m8_en_US_goadedes.heHE.SyEiToStereoma.greekname
