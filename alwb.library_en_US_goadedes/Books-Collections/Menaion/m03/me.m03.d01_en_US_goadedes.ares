meMA.Ode8C11.incipit = "O dignified Saint…"
meMA.Ode8C11.text = "O dignified Saint, the divine coal and immaterial fire within you burned the tyrants and preserved you uninjured."

meMA.Ode8C12.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode8C12.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.notmetered
meMA.Ode8C12.incipit = "…"
meMA.Ode8C12.text = "O St. Eudocia, as a reward for your godly labors, you received the grace to heal many, quell diseases, and revive the dead."

meMA.Ode8C1T.ode = miscellanea_en_US_goadedes.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_goadedes.misc.Mode8
meMA.Ode8C1T.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.notmetered
meMA.Ode8C1T.incipit = "…"
meMA.Ode8C1T.text = "O Theotokos, I pray you revive my soul, for it is deadened by sinful pleasures. O Virgin, you are the one who without marital union gave birth to Life."

