heAU.OuketiFloginiRomfaia.greekname = "Οὐκέτι φλογίνη ῥομφαία."
heAU.OuketiFloginiRomfaia.melody = "Automelon."
heAU.OuketiFloginiRomfaia.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.OuketiFloginiRomfaia.notmetered = "N/M (The fiery sword.)"
heAU.OuketiFloginiRomfaia.name = "The fiery sword."
heAU.OuketiFloginiRomfaia.incipit = "The fiery sword…"
heAU.OuketiFloginiRomfaia.text = "The fiery sword is not guarding the entrance to Eden anymore. For it was extinguished in paradoxical manner by the tree of the Cross. The sting of death and the victory of Hades have been stricken. And You, my Savior, came and cried out to those in Hades: Enter again into Paradise."

heAU.OukEtiKolyometha.greekname = "Οὐκ ἔτι κωλυόμεθα."
heAU.OukEtiKolyometha.melody = "Automelon."
heAU.OukEtiKolyometha.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.OukEtiKolyometha.notmetered = "N/M (No longer are we barred.)"
heAU.OukEtiKolyometha.name = "No longer are we barred."
heAU.OukEtiKolyometha.incipit = "No longer are we barred…"
heAU.OukEtiKolyometha.text = "No longer are we barred from the tree of life, * for we have the hope of Your Cross, O Master. * Glory to You, O Lord."

heAU.PyrosFoteinoteron.greekname = "Πυρὸς φωτεινότερον."
heAU.PyrosFoteinoteron.melody = "Automelon."
heAU.PyrosFoteinoteron.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.PyrosFoteinoteron.notmetered = "N/M (More dazzling than flame.)"
heAU.PyrosFoteinoteron.name = "More dazzling than flame."
heAU.PyrosFoteinoteron.incipit = "More dazzling than flame…"
heAU.PyrosFoteinoteron.text = "More dazzling than flame of fire, * more brilliant than shining light, * O Lady, is the mercy of your gift of grace; * for it burns away the sins of all your people * and refreshes the minds of those extolling * your splendid miracles, O all-blameless Mother of God."

heAU.SimeronGrigoreiOIoudas.greekname = "Σήμερον γρηγορεῖ ὁ Ἰούδας."
heAU.SimeronGrigoreiOIoudas.melody = "Automelon."
heAU.SimeronGrigoreiOIoudas.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.SimeronGrigoreiOIoudas.notmetered = "N/M (Judas today is vigilant.)"
heAU.SimeronGrigoreiOIoudas.name = "Judas today is vigilant."
heAU.SimeronGrigoreiOIoudas.incipit = "Judas today is vigilant…"
heAU.SimeronGrigoreiOIoudas.text = "Judas today is vigilant, waiting for his chance to betray the Lord, * Who is pre-eternal, the world’s only Savior, * and Who filled with five loaves the thousands of people. * On this day the Teacher is rejected by the lawless one, * who betrayed the Master whose disciple he once had been. * Him who filled the people with the manna he sold for thirty silver coins."

