heAU.title = "Automela of Grave Mode"

heAU.KatafronisantesPanton.greekname = "Καταφρονήσαντες πάντων."
heAU.KatafronisantesPanton.melody = "Automelon."
heAU.KatafronisantesPanton.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.KatafronisantesPanton.notmetered = "N/M (When you had wisely.)"
heAU.KatafronisantesPanton.name = "When you had wisely."
heAU.KatafronisantesPanton.incipit = "When you had wisely…"
heAU.KatafronisantesPanton.text = "When you had wisely despised everything on earth, * O holy martyr Saints, * and had courageously acknowledged Christ in the stadium, * in return for the tortures you received your rewards from Him. * Now that you have acquired access to God the omnipotent, * we beseech you most fervently * to entreat Him in earnest * to save the souls of us sinners who flee to you."

heAU.ODiEmeAnaschomenos.greekname = "Ὁ διʼ ἐμὲ ἀνασχόμενος."
heAU.ODiEmeAnaschomenos.melody = "Automelon."
heAU.ODiEmeAnaschomenos.mode = miscellanea_en_US_goadedes.misc.Mode7
heAU.ODiEmeAnaschomenos.notmetered = "N/M (O Christ God.)"
heAU.ODiEmeAnaschomenos.name = ""
heAU.ODiEmeAnaschomenos.incipit = "…"
heAU.ODiEmeAnaschomenos.text = "O Christ God, who for me endured being nailed to the Cross, accept my vigilant praise, in Your love for humanity."

heAU.OsEchousaToSympathes.greekname = "Ὡς ἔχουσα τὸ συμπαθές."
