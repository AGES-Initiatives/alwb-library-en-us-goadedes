heHE.TheonAnthropois.greekname = "Θεὸν ἀνθρώποις."
heHE.TheonAnthropois.name = "It is impossible."
heHE.TheonAnthropois.incipit = "It is impossible…"
heHE.TheonAnthropois.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TheonAnthropois.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.TheonAnthropois.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TheonAnthropois.text = "It is impossible for God to be seen * by people; and angelic orders dare not to look at Him. * But the Word incarnate has been seen by mankind, through you, O all-pure Virgin. * Now magnifying Him, * joining the celestial hosts, all generations call you blest."

heHE.TheonPateraDimiourgon.notmetered = "N/M (The three young men.)"
heHE.TheonPateraDimiourgon.greekname = "Θεὸν Πατέρα Δημιουργόν."
heHE.TheonPateraDimiourgon.name = "The three young men."
heHE.TheonPateraDimiourgon.incipit = "The three young men…"
