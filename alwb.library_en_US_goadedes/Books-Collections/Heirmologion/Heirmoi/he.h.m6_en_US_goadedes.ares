heHE.OnStratiaiOuranon.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.OnStratiaiOuranon.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.OnStratiaiOuranon.text = "Him whom the hosts of the heavens glorify, * at whom quake the Cherubim and the Seraphim, * every breath and creation, * extol Him now and bless Him, * and exalt supremely, unto all the ages."

heHE.OsEnIpeiroPezefsas.notmetered = "N/M (When they had walked.)"
heHE.OsEnIpeiroPezefsas.greekname = "Ὡς ἐν ἠπείρῳ πεζεύσας."
heHE.OsEnIpeiroPezefsas.name = "When they had walked."
heHE.OsEnIpeiroPezefsas.incipit = "When they had walked…"
heHE.OsEnIpeiroPezefsas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OsEnIpeiroPezefsas.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.OsEnIpeiroPezefsas.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.OsEnIpeiroPezefsas.text = "When they had walked through the Red Sea as over land, * the people of Israel watched as Pharaoh and his host * in pursuit were drowned. Then Moses said, * \"Let us sing a song of victory to God our Lord.\""

heHE.OukEstinAgios.notmetered = "N/M (No one is holy.)"
heHE.OukEstinAgios.greekname = "Οὔκ ἐστιν ἅγιος."
heHE.OukEstinAgios.name = "No one is holy."
heHE.OukEstinAgios.incipit = "No one is holy like the Lord…"
heHE.OukEstinAgios.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OukEstinAgios.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.OukEstinAgios.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.OukEstinAgios.text = "No one is holy like the Lord, * no one righteous like our God. * Being good, You exalted Your believers' horn of strength, * firmly establishing us * on the rock of confession of faith in You."

heHE.ProkatidonOProfitis.notmetered = "N/M (When Habakkuk the Prophet.)"
heHE.ProkatidonOProfitis.greekname = "Προκατιδὼν ὁ Προφήτης."
