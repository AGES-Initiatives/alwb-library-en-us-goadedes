heHE.MiEpodyrouMouMiter.notmetered = "N/M (Weep not for Me, O Mother.)"
heHE.MiEpodyrouMouMiter.greekname = "Μὴ ἐποδύρου μου Μῆτερ."
heHE.MiEpodyrouMouMiter.name = "Weep not for Me, O Mother."
heHE.MiEpodyrouMouMiter.incipit = "Weep not for Me, O Mother…"
heHE.MiEpodyrouMouMiter.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.MiEpodyrouMouMiter.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.MiEpodyrouMouMiter.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.MiEpodyrouMouMiter.text = "Weep not for Me, O Mother, as you see your Son, whom you conceived in your womb without seed, lying in a tomb. For I will rise from the dead and will be glorified; and as God, I will unceasingly exalt in glory those who magnify you with faith and love."

heHE.NomonPatroon.notmetered = "N/M (Braving all danger.)"
heHE.NomonPatroon.greekname = "Νόμων πατρῴων."
heHE.NomonPatroon.name = "Braving all danger."
heHE.NomonPatroon.incipit = "Braving all danger…"
