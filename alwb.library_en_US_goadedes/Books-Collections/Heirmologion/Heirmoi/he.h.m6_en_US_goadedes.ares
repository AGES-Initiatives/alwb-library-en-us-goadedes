StereosonKyrie.notmetered = "N/M (Establish upon the rock.)"
StereosonKyrie.name = "Establish upon the rock."
StereosonKyrie.incipit = "Establish upon the rock…"
StereosonKyrie.melody = misc_en_US_goadedes.Heirmos
StereosonKyrie.mode = misc_en_US_goadedes.Mode6
StereosonKyrie.ode = misc_en_US_goadedes.Ode3
StereosonKyrie.text = "Establish upon the rock of Your commandments my heart, O Master, * sorely shaken as it is, O Lord. * Only You are holy, and again only You are Lord."

StilinKakiasAntitheou.notmetered = "N/M ()"
