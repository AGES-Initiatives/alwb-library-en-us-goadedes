heHE.StereosonKyrie.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.StereosonKyrie.text = "Establish upon the rock of Your commandments my heart, O Master, * sorely shaken as it is, O Lord. * Only You are holy, and again only You are Lord."

heHE.StilinKakiasAntitheou.notmetered = "N/M ()"
heHE.StilinKakiasAntitheou.greekname = "Στήλην κακίας ἀντιθέου."
heHE.StilinKakiasAntitheou.name = ""
heHE.StilinKakiasAntitheou.incipit = "…"
