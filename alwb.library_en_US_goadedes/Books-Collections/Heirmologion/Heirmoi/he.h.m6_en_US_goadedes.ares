heHE.ChristosMouDynamis.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.ChristosMouDynamis.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.ChristosMouDynamis.text = "The holy Church of God exclaims befittingly, * \"Christ is God and my Lord, and He is my strength.\" * She is celebrating in the Lord and sings to Him * from a mind that has been purified."

heHE.DrosovolonMenTinKaminon.notmetered = "N/M (In the furnace the Lord's Angel.)"
heHE.DrosovolonMenTinKaminon.greekname = "Δροσοβόλον μὲν τὴν κάμινον."
heHE.DrosovolonMenTinKaminon.name = "In the furnace the Lord's Angel."
heHE.DrosovolonMenTinKaminon.incipit = "In the furnace the Lord's Angel…"
heHE.DrosovolonMenTinKaminon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.DrosovolonMenTinKaminon.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.DrosovolonMenTinKaminon.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.DrosovolonMenTinKaminon.text = "In the furnace the Lord's Angel saved the pious men, * blowing through it a dew-laden breeze, * while Chaldean men were completely burned, at God's command, * persuading the tyrant to confess * and cry aloud, \"O God of our fathers, blessed are You.\""

heHE.EkFlogosToisOsiois.notmetered = "N/M (From the flame.)"
heHE.EkFlogosToisOsiois.greekname = "Ἐκ φλογὸς τοῖς Ὁσίοις."
heHE.EkFlogosToisOsiois.name = "From the flame."
heHE.EkFlogosToisOsiois.incipit = "From the flame…"
heHE.EkFlogosToisOsiois.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EkFlogosToisOsiois.mode = miscellanea_en_US_goadedes.misc.Mode6
heHE.EkFlogosToisOsiois.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.EkFlogosToisOsiois.text = "From the flame You refreshed pious Servants as with dew * and with water consumed the righteous Prophet's sacrifice. * You accomplish all, O Christ, simply by willing it. * We exalt You beyond measure, unto all the ages."

heHE.EkNyktosOrthrizonta.notmetered = "N/M (Early in the morning.)"
heHE.EkNyktosOrthrizonta.greekname = "Ἐκ νυκτὸς ὀρθρίζοντα."
heHE.EkNyktosOrthrizonta.name = "Early in the morning."
heHE.EkNyktosOrthrizonta.incipit = "Early in the morning…"
