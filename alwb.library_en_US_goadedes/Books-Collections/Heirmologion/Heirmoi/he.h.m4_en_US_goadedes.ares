OtiEpoiiseMoiMegaleia.melody = misc_en_US_goadedes.HeirmosNotMetered
OtiEpoiiseMoiMegaleia.mode = misc_en_US_goadedes.Mode4
OtiEpoiiseMoiMegaleia.ode = misc_en_US_goadedes.Ode9
OtiEpoiiseMoiMegaleia.text = "For Mighty God has done great things for me, and holy is His name. And His mercy is from generation to generation on those who fear Him."

OtiSteiraEteken.notmetered = "N/M (Since the barren woman.)"
OtiSteiraEteken.name = "Since the barren woman."
OtiSteiraEteken.incipit = "Since the barren woman…"
OtiSteiraEteken.melody = misc_en_US_goadedes.Heirmos
OtiSteiraEteken.mode = misc_en_US_goadedes.Mode4
OtiSteiraEteken.ode = misc_en_US_goadedes.Ode3
OtiSteiraEteken.text = "Since the barren woman, yes, the Gentile Church, has borne many, * and she with many children, the Synagogue, is now forlorn, * we cry out to our God who is so marvelous, \"Holy are You, O Lord.\""

OukElatrefsan.notmetered = "N/M (Godly-minded three.)"
OukElatrefsan.name = "Godly-minded three."
OukElatrefsan.incipit = "Godly-minded three…"
