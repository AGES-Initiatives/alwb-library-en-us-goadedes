AnaxAnakton.notmetered = "N/M (Monarch of monarchs.)"
AnaxAnakton.name = "Monarch of monarchs."
AnaxAnakton.incipit = "Monarch of monarchs…"
AnaxAnakton.melody = misc_en_US_goadedes.Heirmos
AnaxAnakton.mode = misc_en_US_goadedes.Mode4
AnaxAnakton.ode = misc_en_US_goadedes.Ode4
AnaxAnakton.text = "Monarch of monarchs, the only from the only, * Logos who came forth from the uncaused Father, * as Benefactor You sent out Your unerring, * equipotent Spirit unto the Apostles, * as they sing, Glory be unto Your might, O Lord."
AnaxAnakton1.text = "Monarch of monarchs, the only from the only,"
AnaxAnakton2.text = "Logos who came forth from the uncaused Father,"
AnaxAnakton3.text = "As Benefactor You sent out Your unerring,"
AnaxAnakton4.text = "Equipotent Spirit unto the Apostles,"
AnaxAnakton5.text = "As they sing, Glory be unto Your might, O Lord."

