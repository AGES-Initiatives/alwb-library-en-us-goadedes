LytirionKatharsin.incipit = "Receive now the fire…"
LytirionKatharsin.melody = misc_en_US_goadedes.Heirmos
LytirionKatharsin.mode = misc_en_US_goadedes.Mode4
LytirionKatharsin.ode = misc_en_US_goadedes.Ode5
LytirionKatharsin.text = "Receive now the fire-breathing dew of the Spirit, * redemptive purgation of all your offences, * O children of the Church having been illumined; * for now a law has gone forth out of Zion, * namely the tongued and fiery grace of the Spirit."
LytirionKatharsin1.text = "Receive now the fire-breathing dew of the Spirit,"
LytirionKatharsin2.text = "Redemptive purgation of all your offences,"
LytirionKatharsin3.text = "O children of the Church having been illumined;"
LytirionKatharsin4.text = "For now a law has gone forth out of Zion,"
LytirionKatharsin5.text = "Namely the tongued and fiery grace of the Spirit."

