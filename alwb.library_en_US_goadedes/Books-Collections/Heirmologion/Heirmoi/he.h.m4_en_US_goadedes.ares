ApasGigenis.notmetered = "N/M (All you born on earth.)"
ApasGigenis.name = "All you born on earth."
ApasGigenis.incipit = "All you born on earth…"
ApasGigenis.melody = misc_en_US_goadedes.Heirmos
ApasGigenis.mode = misc_en_US_goadedes.Mode4
ApasGigenis.ode = misc_en_US_goadedes.Ode9
ApasGigenis.text = "All you born on earth, * with festival lamps in hand, in spirit leap for joy; * and all you the heavenly angelic orders, join in and celebrate; * and honoring the sacred wonders of the Mother of God, * sing the joyful salutation, \"O rejoice, * Theotokos, all-blest, ever-virgin pure!\""

