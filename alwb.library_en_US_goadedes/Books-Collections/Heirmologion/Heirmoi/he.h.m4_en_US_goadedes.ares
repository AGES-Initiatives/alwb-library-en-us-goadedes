AkoueKori.notmetered = "N/M (Hearken, O Damsel.)"
AkoueKori.name = "Hearken, O Damsel."
AkoueKori.incipit = "Hearken, O Damsel…"
AkoueKori.melody = misc_en_US_goadedes.Heirmos
AkoueKori.mode = misc_en_US_goadedes.Mode4
AkoueKori.ode = misc_en_US_goadedes.Ode8
AkoueKori.text = "Hearken, O Damsel, let Gabriel say. * Incline your ear, O Virgin pure, * and hear the counsel of the Most High, ancient and true; * and make ready for your own reception of God. * For through you the Uncontainable associates with mortal men. * And I am therefore rejoicing and shouting: * O all you works of the Lord, * praise and bless the Lord."

