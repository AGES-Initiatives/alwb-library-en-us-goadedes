DiaskedasasTinNykta.ode = misc_en_US_goadedes.Ode5
DiaskedasasTinNykta.text = "O Creator of all, You drove away the original darkness of the abyss, and shined the first-created light on the world. I pray You, dispel the night of the passions, and shine in me the noetic light."

EisakikoaTinAkoin.notmetered = "N/M ()"
EisakikoaTinAkoin.name = ""
EisakikoaTinAkoin.incipit = "…"
