EstereothiTiPistei.mode = misc_en_US_goadedes.Mode7
EstereothiTiPistei.ode = misc_en_US_goadedes.Ode3
EstereothiTiPistei.text = "The Church is established in the faith of Christ; and she unceasingly sings hymns and says: \"Holy are You, O Lord, and my spirit praises You.\""

KaminonKaiomenin.notmetered = "N/M ()"
KaminonKaiomenin.name = ""
KaminonKaiomenin.incipit = "…"
KaminonKaiomenin.melody = misc_en_US_goadedes.HeirmosNotMetered
KaminonKaiomenin.mode = misc_en_US_goadedes.Mode7
KaminonKaiomenin.ode = misc_en_US_goadedes.Ode7
KaminonKaiomenin.text = "O Savior, You bedewed the burning furnace, and You rescued those Servants, who were extolling You and saying: \"Blessed are You unto the ages, O Lord, the God of our fathers.\""

KaminonPaides.notmetered = "N/M (The fiery furnace.)"
KaminonPaides.name = "The fiery furnace."
KaminonPaides.incipit = "The fiery furnace…"
KaminonPaides.melody = misc_en_US_goadedes.Heirmos
KaminonPaides.mode = misc_en_US_goadedes.Mode7
KaminonPaides.ode = misc_en_US_goadedes.Ode7
KaminonPaides.text = "The fiery furnace turned moist and dew-laden, when the Servants were cast therein, * and they extolled the one God, and they sang to Him, * \"O highly exalted God of our fathers and exceedingly glorious.\""

KatanoonOProfitis.notmetered = "N/M (Prophet Habakkuk considered.)"
KatanoonOProfitis.name = "Prophet Habakkuk considered."
KatanoonOProfitis.incipit = "Prophet Habakkuk considered…"
KatanoonOProfitis.melody = misc_en_US_goadedes.Heirmos
KatanoonOProfitis.mode = misc_en_US_goadedes.Mode7
KatanoonOProfitis.ode = misc_en_US_goadedes.Ode4
KatanoonOProfitis.text = "Prophet Habakkuk considered Your coming in the latter times, * Christ, and to You he cried out, * \"O Lord, I have heard the report of Your mighty power, * that You came for the salvation of all Your anointed ones.\""

KyrieOTheosMou.notmetered = "N/M ()"
KyrieOTheosMou.name = ""
KyrieOTheosMou.incipit = "…"
KyrieOTheosMou.melody = misc_en_US_goadedes.HeirmosNotMetered
KyrieOTheosMou.mode = misc_en_US_goadedes.Mode7
KyrieOTheosMou.ode = misc_en_US_goadedes.Ode5
KyrieOTheosMou.text = "I have risen early in the morning. I entreat You, O Lord my God, grant me forgiveness of my offences, and I pray You direct my ways to the light of your commandments."

MiTisFthoras.notmetered = "N/M (When you conceived.)"
MiTisFthoras.name = "When you conceived."
MiTisFthoras.incipit = "When you conceived…"
MiTisFthoras.melody = misc_en_US_goadedes.Heirmos
MiTisFthoras.mode = misc_en_US_goadedes.Mode7
MiTisFthoras.ode = misc_en_US_goadedes.Ode9
MiTisFthoras.text = "When you conceived, you experienced no deflowerment. * You lent your flesh to the Logos, through whom all things were made. * Therefore we magnify you, Virgin Theotokos, * Mother who knew no man, space and holder of your Maker, who is uncontainable and infinite."

MitirTheouKaiParthenos.notmetered = "N/M (You as the Mother of God.)"
MitirTheouKaiParthenos.name = "You as the Mother of God."
MitirTheouKaiParthenos.incipit = "You as the Mother of God…"
MitirTheouKaiParthenos.melody = misc_en_US_goadedes.Heirmos
MitirTheouKaiParthenos.mode = misc_en_US_goadedes.Mode7
MitirTheouKaiParthenos.ode = misc_en_US_goadedes.Ode9
MitirTheouKaiParthenos.text = "You as the Mother of God and virgin Maid * gave birth remaining a virgin: this is no feat of nature, * but of the condescension of our God. * Thus you the only one who was accounted worthy of these miracles * of God do we magnify."

NaftionToSalo.notmetered = "N/M (I was feeling nauseous.)"
NaftionToSalo.name = "I was feeling nauseous."
NaftionToSalo.incipit = "I was feeling nauseous…"
NaftionToSalo.melody = misc_en_US_goadedes.Heirmos
NaftionToSalo.mode = misc_en_US_goadedes.Mode7
NaftionToSalo.ode = misc_en_US_goadedes.Ode6
NaftionToSalo.text = "I was feeling nauseous from mundane tempestuous worries. * My sins, as if shipmates, threw me overboard into the maw of the beast that devours souls. * Now, like Prophet Jonah, I am praying, * \"From this deathly Pit bring up my life, O Christ my God.\""

NefseiSouProsGeodi.notmetered = "N/M (Lord, at Your divine signal.)"
NefseiSouProsGeodi.name = "Lord, at Your divine signal."
NefseiSouProsGeodi.incipit = "Lord, at Your divine signal…"
NefseiSouProsGeodi.melody = misc_en_US_goadedes.Heirmos
NefseiSouProsGeodi.mode = misc_en_US_goadedes.Mode7
NefseiSouProsGeodi.ode = misc_en_US_goadedes.Ode1
NefseiSouProsGeodi.text = "Lord, at Your divine signal, the fluid streams of the Red Sea * became solid walls, as they were held back by a mighty wind. * Israel passed over dry-shod; * and they sang the victory song unto You, O God."

NyxAfengis.notmetered = "N/M (For unbelievers, delight.)"
NyxAfengis.name = "For unbelievers, delight"
NyxAfengis.incipit = "For unbelievers, delight…"
NyxAfengis.melody = misc_en_US_goadedes.Heirmos
NyxAfengis.mode = misc_en_US_goadedes.Mode7
NyxAfengis.ode = misc_en_US_goadedes.Ode5
NyxAfengis.text = "For unbelievers, delight is dark night; * but for believers, the light is in delighting in Your words, O Christ. * And so I rise early in the morning to sing in praise of Your divinity."

OIonasEkKoilias.notmetered = "N/M ()"
OIonasEkKoilias.name = ""
OIonasEkKoilias.incipit = "…"
OIonasEkKoilias.melody = misc_en_US_goadedes.HeirmosNotMetered
OIonasEkKoilias.mode = misc_en_US_goadedes.Mode7
OIonasEkKoilias.ode = misc_en_US_goadedes.Ode6
OIonasEkKoilias.text = "Jonah cried out from the belly of Hades: \"Raise up my life from corruption.\" And we now pray to You: \"O almighty Savior, have mercy on us.\""

OKatArchasTousOuranous.notmetered = "N/M (First You established.)"
OKatArchasTousOuranous.name = "First You established."
OKatArchasTousOuranous.incipit = "First You established…"
OKatArchasTousOuranous.melody = misc_en_US_goadedes.Heirmos
OKatArchasTousOuranous.mode = misc_en_US_goadedes.Mode7
OKatArchasTousOuranous.ode = misc_en_US_goadedes.Ode3
OKatArchasTousOuranous.text = "First You established by Your Word, by Your omnipotent Logos, * the heavens, O Savior, and You made * all their host by Your divine omnificent Spirit, as the breath of Your mouth. * I pray You, on the unshaken rock of Your confession, Lord, establish me."

OOuranousToLogo.notmetered = "N/M ()"
OOuranousToLogo.name = ""
OOuranousToLogo.incipit = "…"
OOuranousToLogo.melody = misc_en_US_goadedes.HeirmosNotMetered
OOuranousToLogo.mode = misc_en_US_goadedes.Mode7
OOuranousToLogo.ode = misc_en_US_goadedes.Ode1
OOuranousToLogo.text = "You are the one who by Your Word established the heavens, and who set the earth's foundation on many waters. Establish my mind in your will, O only Lord who loves mankind."

OPatrikousKolpous.notmetered = "N/M (O Christ our God, You did not depart.)"
OPatrikousKolpous.name = "O Christ our God, You did not depart."
OPatrikousKolpous.incipit = "O Christ our God, You did not depart…"
OPatrikousKolpous.melody = misc_en_US_goadedes.Heirmos
OPatrikousKolpous.mode = misc_en_US_goadedes.Mode7
OPatrikousKolpous.ode = misc_en_US_goadedes.Ode4
OPatrikousKolpous.text = "O Christ our God, You did not depart * Your Father's bosom when You descended to the earth. * I have heard about the mystery of Your plan to save us all, * and I glorify You, Lord who loves mankind."

OStereosasToLogo.notmetered = "N/M ()"
OStereosasToLogo.name = ""
OStereosasToLogo.incipit = "…"
OStereosasToLogo.melody = misc_en_US_goadedes.HeirmosNotMetered
OStereosasToLogo.mode = misc_en_US_goadedes.Mode7
OStereosasToLogo.ode = misc_en_US_goadedes.Ode3
OStereosasToLogo.text = "You are the One who by Your Word established the world which shall not be shaken. Establish our minds on the fear of You, Lord."

