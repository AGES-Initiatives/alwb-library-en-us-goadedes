heHE.EflexeReithro1a.text = "T"
heHE.EflexeReithro1b.text = "he Lord, who impeded the flames of the furnace"
heHE.EflexeReithro2.text = "Holding the godly Young Men, has completely"
heHE.EflexeReithro2a.text = "H"
heHE.EflexeReithro2b.text = "olding the godly Young Men, has completely"
heHE.EflexeReithro3.text = "Enkindled the dragons’ heads in the Jordan"
