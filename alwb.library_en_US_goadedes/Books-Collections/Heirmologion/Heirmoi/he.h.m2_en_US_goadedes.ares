heHE.SyToLogoSouKyrie.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.SyToLogoSouKyrie.text = "You established the heavens by Your divine word, almighty God. * Now I pray You also establish my heart in the faith, O Lord."

heHE.TaCherouvimMimoumenoi.notmetered = "N/M (The Servants in the furnace.)"
heHE.TaCherouvimMimoumenoi.greekname = "Τὰ Χερουβὶμ μιμούμενοι."
heHE.TaCherouvimMimoumenoi.name = "The Servants in the furnace."
heHE.TaCherouvimMimoumenoi.incipit = "The Servants in the furnace…"
heHE.TaCherouvimMimoumenoi.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TaCherouvimMimoumenoi.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TaCherouvimMimoumenoi.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.TaCherouvimMimoumenoi.text = "The Servants in the furnace fire * danced as they imitated * the Cherubim, and cried out: * Blessed are You, O Lord our God, * for You have caused all this to befall us and Your holy city, in truth and in judgment, on account of our sins. * You are supremely extolled and You are glorified, unto all the ages."

heHE.TinAchlynTisPsychisMou.notmetered = "N/M (I implore You to scatter.)"
heHE.TinAchlynTisPsychisMou.greekname = "Τὴν ἀχλὺν τῆς ψυχῆς μου."
heHE.TinAchlynTisPsychisMou.name = "I implore You to scatter."
heHE.TinAchlynTisPsychisMou.incipit = "I implore You to scatter…"
heHE.TinAchlynTisPsychisMou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinAchlynTisPsychisMou.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TinAchlynTisPsychisMou.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.TinAchlynTisPsychisMou.text = "I implore You to scatter the mist on my soul, O Savior, * and illuminate me with the light of Your divine commandments, * as the only King of peace."

heHE.TinAgninKaiAchranton.notmetered = "N/M ()"
heHE.TinAgninKaiAchranton.greekname = "Τὴν ἁγνὴν καὶ ἄχραντον."
