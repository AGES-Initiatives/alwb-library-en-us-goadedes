heHE.OTouFotosChorigos.notmetered = "N/M (Lord, the provider of light.)"
heHE.OTouFotosChorigos.greekname = "Ὁ τοῦ φωτὸς χορηγός."
heHE.OTouFotosChorigos.name = "Lord, the provider of light."
heHE.OTouFotosChorigos.incipit = "Lord, the provider of light…"
heHE.OTouFotosChorigos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OTouFotosChorigos.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.OTouFotosChorigos.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.OTouFotosChorigos.text = "Lord, the provider of light, * and the Creator of the ages, we pray * that You will guide us in the light of Your all-holy commands. * Except for You, we do not know any other God."

heHE.OlosYparcheisEfesis.notmetered = "N/M (You are desire entirely.)"
heHE.OlosYparcheisEfesis.greekname = "Ὅλος ὑπάρχεις ἔφεσις."
heHE.OlosYparcheisEfesis.name = "You are desire entirely."
heHE.OlosYparcheisEfesis.incipit = "You are desire entirely…"
heHE.OlosYparcheisEfesis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OlosYparcheisEfesis.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.OlosYparcheisEfesis.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.OlosYparcheisEfesis.text = "You are desire entirely, all sweetness are You, O Logos of God, the Virgin’s Son, O God of gods, Lord of all, more holy than the holy ones. * We therefore maginfy You and the Maiden who gave birth to You."

heHE.OsoiPalaion.notmetered = "N/M (You who have been delivered.)"
heHE.OsoiPalaion.greekname = "Ὅσοι παλαιῶν."
heHE.OsoiPalaion.name = "You who have been delivered."
heHE.OsoiPalaion.incipit = "You who have been delivered…"
