heHE.ProsKyrionEkKitous.notmetered = "N/M (The Prophet Jonah cried.)"
heHE.ProsKyrionEkKitous.greekname = "Πρὸς Κύριον ἐκ κήτους."
heHE.ProsKyrionEkKitous.name = "The Prophet Jonah cried."
heHE.ProsKyrionEkKitous.incipit = "The Prophet Jonah cried…"
