heHE.TonEnKaminoTouPyros.notmetered = "N/M (When God went down.)"
heHE.TonEnKaminoTouPyros.greekname = "Τὸν ἐν καμίνῳ τοῦ πυρός."
heHE.TonEnKaminoTouPyros.name = "When God went down."
heHE.TonEnKaminoTouPyros.incipit = "When God went down…"
heHE.TonEnKaminoTouPyros.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEnKaminoTouPyros.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TonEnKaminoTouPyros.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonEnKaminoTouPyros.text = "When God went down into the fire * of the furnace with the holy Hebrew Servants, * He converted the flames there into dew refreshing them. * Extol Him as Lord, O you works of His, * and exalt Him beyond measure, * unto all the ages."

heHE.TonEnTiVatoMosi.notmetered = "N/M (Within the bush.)"
heHE.TonEnTiVatoMosi.greekname = "Τὸν ἐν τῇ βάτῳ Μωσῇ."
heHE.TonEnTiVatoMosi.name = "Within the bush."
heHE.TonEnTiVatoMosi.incipit = "Within the bush…"
heHE.TonEnTiVatoMosi.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEnTiVatoMosi.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TonEnTiVatoMosi.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonEnTiVatoMosi.text = "Within the bush once, the Lord * on Mount Sinai prefigured * unto Moses the virgin Maiden’s miracle of old.* Extol Him as you bless Him, * and exalt supremely, * unto all the ages."

heHE.TonGigenonTisIkouse.notmetered = "N/M (Among the earthborn.)"
heHE.TonGigenonTisIkouse.greekname = "Τῶν γηγενῶν τίς ἤκουσε."
heHE.TonGigenonTisIkouse.name = "Among the earthborn."
heHE.TonGigenonTisIkouse.incipit = "Among the earthborn…"
heHE.TonGigenonTisIkouse.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonGigenonTisIkouse.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TonGigenonTisIkouse.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TonGigenonTisIkouse.text = "Among the earthborn who has ever heard or beheld a marvel such as this? * Namely, a Virgin was found to have conceived in the womb, * and she delivered the infant without a birth-pang. * Your miracle was so great! * We magnify you, Virgin Mary, for you indeed gave birth to God."

heHE.TonPalaiDrosisanta.notmetered = "N/M ()"
