heHE.TinAgninKaiAchranton.text = ""

heHE.TinEkParthenou.notmetered = "N/M (When he foresaw.)"
heHE.TinEkParthenou.greekname = "Τὴν ἐκ παρθένου."
heHE.TinEkParthenou.name = "When he foresaw."
heHE.TinEkParthenou.incipit = "When he foresaw…"
heHE.TinEkParthenou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinEkParthenou.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TinEkParthenou.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.TinEkParthenou.text = "When he foresaw Your nativity * from the Virgin, the Prophet * crying out of old proclaimed: * I have heard Your report and I have become frightened; * for You have come to us * out of Teman and from the overshadowed * holy mountain, O my Christ."

heHE.TinMoseosOdin.notmetered = "N/M (O my soul, taking up.)"
heHE.TinMoseosOdin.greekname = "Τὴν Μωσέως ᾠδήν."
heHE.TinMoseosOdin.name = "O my soul, taking up."
heHE.TinMoseosOdin.incipit = "O my soul, taking up…"
heHE.TinMoseosOdin.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinMoseosOdin.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TinMoseosOdin.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.TinMoseosOdin.text = "O my soul, taking up the ode of Moses, cry aloud and say: * He became for me a helper and shelter unto salvation. * He is my God and I will glorify Him."

heHE.TinParthenianSou.notmetered = "N/M (O undefiled Theotokos.)"
heHE.TinParthenianSou.greekname = "Τὴν παρθενίαν σου."
heHE.TinParthenianSou.name = "O undefiled Theotokos."
heHE.TinParthenianSou.incipit = "O undefiled Theotokos…"
heHE.TinParthenianSou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinParthenianSou.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TinParthenianSou.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TinParthenianSou.text = "O undefiled Theotokos, your virginity * was not consumed by the fire of Divinity. * So we magnify you with unceasing hymns."

heHE.TinYperfyosSarki.notmetered = "N/M (You conceived within your womb.)"
heHE.TinYperfyosSarki.greekname = "Τὴν ὑπερφυῶς σαρκί."
heHE.TinYperfyosSarki.name = "You conceived within your womb."
heHE.TinYperfyosSarki.incipit = "You conceived within your womb…"
heHE.TinYperfyosSarki.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinYperfyosSarki.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TinYperfyosSarki.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TinYperfyosSarki.text = "You conceived within your womb extraordinarily * the Logos who outside time came forth from God the Father. * We faithful magnify you with our never silent hymns."

heHE.TisNyktosDielthousis.notmetered = "N/M (With the night being over.)"
heHE.TisNyktosDielthousis.greekname = "Τῆς νυκτὸς διελθούσης."
heHE.TisNyktosDielthousis.name = "With the night being over."
heHE.TisNyktosDielthousis.incipit = "With the night being over…"
heHE.TisNyktosDielthousis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TisNyktosDielthousis.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TisNyktosDielthousis.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.TisNyktosDielthousis.text = "With the night being over, * day has arrived, O Master, and the light has shone forth upon the world. * For this reason the orders * of Angels extol You, * and they glorify You, O Christ our God."

heHE.TisPisteosEnPetra.notmetered = "N/M (You strengthened me.)"
heHE.TisPisteosEnPetra.greekname = "Τῆς πίστεως ἐν πέτρᾳ."
heHE.TisPisteosEnPetra.name = "You strengthened me."
heHE.TisPisteosEnPetra.incipit = "You strengthened me…"
heHE.TisPisteosEnPetra.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TisPisteosEnPetra.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TisPisteosEnPetra.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.TisPisteosEnPetra.text = "You strengthened me on the rock of faith, O Savior. * You made me smile at my enemies and deride them. * My spirit is rejoicing, as I am singing, * “No one is holy like our God, * and no one is righteous but You, O Lord.”"

heHE.ToDogmatiToTyranniko.notmetered = "N/M ()"
heHE.ToDogmatiToTyranniko.greekname = "Τῷ δόγματι τῷ τυραννικῷ."
heHE.ToDogmatiToTyranniko.name = ""
heHE.ToDogmatiToTyranniko.incipit = "…"
heHE.ToDogmatiToTyranniko.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToDogmatiToTyranniko.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.ToDogmatiToTyranniko.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.ToDogmatiToTyranniko.text = "The three righteous Servants did not obey * the impious decree of the tyrant. * And they were cast into the furnace’s fire, * where they stood confessing God, * and they sang, “Bless the Lord, all you works of the Lord.”"

heHE.ToTinAvaton.notmetered = "N/M (Let us sing to the Lord.)"
heHE.ToTinAvaton.greekname = "Τῷ τὴν ἄβατον."
heHE.ToTinAvaton.name = "Let us sing to the Lord."
heHE.ToTinAvaton.incipit = "Let us sing to the Lord…"
heHE.ToTinAvaton.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToTinAvaton.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.ToTinAvaton.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.ToTinAvaton.text = "Let us sing to the Lord, who dried up the impassable * surging waters of the Sea by His divine command, * and who also led the way * for the chosen people of Israel to cross on foot. * Come let us sing to Him, for He is greatly glorified."

heHE.TonArritoSofia.notmetered = "N/M (With ineffable wisdom.)"
heHE.TonArritoSofia.greekname = "Τὸν ἀῤῥήτῳ σοφίᾳ."
heHE.TonArritoSofia.name = "With ineffable wisdom."
heHE.TonArritoSofia.incipit = "With ineffable wisdom…"
heHE.TonArritoSofia.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonArritoSofia.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TonArritoSofia.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonArritoSofia.text = "With ineffable wisdom * God the Logos constructed all creation * from nonexistence He brought the universe into being. * All you works of the Lord, bless Him as your Lord, * and exalt Him beyond measure unto all the ages."

heHE.TonEkTheouTheonLogon.notmetered = "N/M (Being from God.)"
heHE.TonEkTheouTheonLogon.greekname = "Τὸν ἐκ Θεοῦ Θεὸν Λόγον."
heHE.TonEkTheouTheonLogon.name = "Being from God."
heHE.TonEkTheouTheonLogon.incipit = "Being from God…"
heHE.TonEkTheouTheonLogon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEkTheouTheonLogon.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.TonEkTheouTheonLogon.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TonEkTheouTheonLogon.text = "Being from God, God the Logos * with ineffable wisdom * came to make Adam new who fell through food * into corruption unhappily. * From an all-holy Virgin * ineffably He took on flesh for us. * We the faithful extol Him concordantly and magnify."

heHE.TonEnFonaisAngelikais.notmetered = "N/M ()"
heHE.TonEnFonaisAngelikais.greekname = "Τὸν ἐν φωναῖς Ἀγγελικαῖς."
