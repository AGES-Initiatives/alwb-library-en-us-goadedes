heHE.VatosEnOrei.notmetered = "N/M (The burning bush.)"
heHE.VatosEnOrei.greekname = "Βάτος ἐν ὄρει."
heHE.VatosEnOrei.name = "The burning bush."
heHE.VatosEnOrei.incipit = "The burning bush…"
heHE.VatosEnOrei.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.VatosEnOrei.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.VatosEnOrei.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.VatosEnOrei.text = "The burning bush that was not consumed * and the Chaldean furnace that emitted due * indeed prefigured you, O Bride of God. * You accepted in your womb, which was material, divine * immaterial fire * without being burnt. So to your Son we cry * and say, “O God of our Fathers, You are blessed.”"

heHE.VythoAmartimaton.notmetered = "N/M (The gulf of my transgressions.)"
heHE.VythoAmartimaton.greekname = "Βυθῷ ἁμαρτημάτων."
heHE.VythoAmartimaton.name = "The gulf of my transgressions."
heHE.VythoAmartimaton.incipit = "The gulf of my transgressions…"
heHE.VythoAmartimaton.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.VythoAmartimaton.mode = miscellanea_en_US_goadedes.misc.Mode2
heHE.VythoAmartimaton.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.VythoAmartimaton.text = "The gulf of my transgressions, O Savior, presses me, * and in the ocean of life I am foundering. * As once You extracted Jonah from the whale, * so now from the passions haul me up, * and bring me to safety."

heHE.VythouAnekalypse.notmetered = "N/M (Behold, how the Lord.)"
heHE.VythouAnekalypse.greekname = "Βυθοῦ ἀνεκάλυψε."
heHE.VythouAnekalypse.name = "Behold, how the Lord."
heHE.VythouAnekalypse.incipit = "Behold, how the Lord…"
heHE.VythouAnekalypse.melody = miscellanea_en_US_goadedes.misc.Heirmos
