heHE.NefsonProsYmnous.notmetered = "N/M (Incline Your ear.)"
heHE.NefsonProsYmnous.greekname = "Νεῦσον πρὸς ὕμνους."
heHE.NefsonProsYmnous.name = "Incline Your ear."
heHE.NefsonProsYmnous.incipit = "Incline Your ear…"
