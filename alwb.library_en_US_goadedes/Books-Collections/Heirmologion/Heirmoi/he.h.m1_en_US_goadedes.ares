heHE.ProsecheOuraneKaiLaliso.text = ""

heHE.RavdosEkTisRizis.notmetered = "N/M (A rod has come forth.)"
heHE.RavdosEkTisRizis.greekname = "Ῥάβδος ἐκ τῆς ῥίζης."
heHE.RavdosEkTisRizis.name = "A rod has come forth."
heHE.RavdosEkTisRizis.incipit = "A rod has come forth…"
heHE.RavdosEkTisRizis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.RavdosEkTisRizis.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.RavdosEkTisRizis.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.RavdosEkTisRizis.text = "A rod has come forth from Jesse’s root, * and You its flower grew from it, praiseworthy Christ, now born of the Virgin, * whom Habakkuk called Paran, shady, thickly wooded mountain. * From her who knew no man You came incarnate, O immaterial God. * Glory to Your strength, O Lord!"

heHE.RiseisProfitonKaiAinigmata.notmetered = "N/M (Sayings of the Prophets.)"
heHE.RiseisProfitonKaiAinigmata.greekname = "Ῥήσεις Προφητῶν καὶ αἰνίγματα."
heHE.RiseisProfitonKaiAinigmata.name = "Sayings of the Prophets."
heHE.RiseisProfitonKaiAinigmata.incipit = "Sayings of the Prophets…"
heHE.RiseisProfitonKaiAinigmata.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.RiseisProfitonKaiAinigmata.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.RiseisProfitonKaiAinigmata.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.RiseisProfitonKaiAinigmata.text = "Sayings of the Prophets and mysteries * revealed Your incarnation from a Virgin mystically, O Christ: * that Your lightning’s brightness as light for Gentiles would emanate. * The abyss calls out to You with a voice of gladness: * To Your strength be glory, benevolent Lord!"

heHE.SeInPerEideMoysis.notmetered = "N/M ()"
heHE.SeInPerEideMoysis.greekname = "Σὲ ἥν περ εἶδε Μωυσῆς."
heHE.SeInPerEideMoysis.name = ""
heHE.SeInPerEideMoysis.incipit = "…"
heHE.SeInPerEideMoysis.melody = miscellanea_en_US_goadedes.misc.Heirmos
