heHE.EkNyktosErgon.notmetered = "N/M (Now rising.)"
heHE.EkNyktosErgon.greekname = "Ἐκ νυκτὸς ἔργων."
heHE.EkNyktosErgon.name = "Now rising."
heHE.EkNyktosErgon.incipit = "Now rising…"
heHE.EkNyktosErgon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EkNyktosErgon.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EkNyktosErgon.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.EkNyktosErgon.text = "Now rising to You, O Christ, from the night of sinning — * Iniquitous misdeeds, bedarkened by error, — * To extol You, our Benefactor, we pray that You grant us * Your mercy, and come to us and make the path easy, * So that ascending by it we may attain to glory."
