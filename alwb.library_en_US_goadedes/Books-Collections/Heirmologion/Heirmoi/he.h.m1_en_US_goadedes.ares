heHE.NenikintaiTisFyseos.greekname = "Νενίκηνται τῆς φύσεως."
heHE.NenikintaiTisFyseos.name = "In you the laws."
heHE.NenikintaiTisFyseos.incipit = "In you the laws of nature…"
heHE.NenikintaiTisFyseos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.NenikintaiTisFyseos.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.NenikintaiTisFyseos.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.NenikintaiTisFyseos.text = "In you the laws of nature are defeated,* O undefiled Virgin pure. * In virginity childbirth was accomplished, and death introduces life. * You, after bearing, are virgin, and after dying are living. * Mother of God, * ever save your inheritance."

heHE.NyxPeriescheMeSotir.notmetered = "N/M ()"
heHE.NyxPeriescheMeSotir.greekname = "Νὺξ περιέσχε με Σωτήρ."
heHE.NyxPeriescheMeSotir.name = "N/M ()"
heHE.NyxPeriescheMeSotir.incipit = "N/M…"
heHE.NyxPeriescheMeSotir.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.NyxPeriescheMeSotir.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.NyxPeriescheMeSotir.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.NyxPeriescheMeSotir.text = "O Savior, night and the fog of sin have surrounded me. Therefore I pray You, let the morning of repentance rise on me, O compassionate Lord, before I reach the indefinite end of my life, O Christ, the Life and the Light."

