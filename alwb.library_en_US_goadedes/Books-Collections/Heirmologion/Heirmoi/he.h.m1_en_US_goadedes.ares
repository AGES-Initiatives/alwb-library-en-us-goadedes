heHE.AftiIKliti.notmetered = "N/M (This is it.)"
heHE.AftiIKliti.greekname = "Αὔτη ἡ κλητή."
heHE.AftiIKliti.name = "This is it."
heHE.AftiIKliti.incipit = "This is it…"
heHE.AftiIKliti.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AftiIKliti.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.AftiIKliti.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.AftiIKliti.text = "This is it, the Day which is chosen and holy, * Day One of the Sabbaths. * It is the queen-day, the Lord's Day, * and the Feast of all feasts, * and the Festival above every festival, * on which we extol Christ and bless Him to the ages."

heHE.AlionPontogenes.notmetered = "N/M (Briny and born.)"
heHE.AlionPontogenes.greekname = "Ἅλιον ποντογενές."
heHE.AlionPontogenes.name = "Briny and born."
heHE.AlionPontogenes.incipit = "Briny and born…"
heHE.AlionPontogenes.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AlionPontogenes.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.AlionPontogenes.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.AlionPontogenes.text = "Briny and born of the deep, * the fire in the maw of the whale * was a prefiguring of Your three-daylong burial; * and of this figure was Jonah the interpreter. * For, preserved unscathed as before he was swallowed up, he cried out: * I shall sacrifice to You with a voice of praise, O Lord."

heHE.AnastaseosImera.notmetered = "N/M (Resurrection Day.)"
heHE.AnastaseosImera.greekname = "Ἀναστάσεως ἡμέρα."
heHE.AnastaseosImera.name = "Resurrection Day."
heHE.AnastaseosImera.incipit = "Resurrection Day…"
heHE.AnastaseosImera.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AnastaseosImera.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.AnastaseosImera.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.AnastaseosImera.text = "Resurrection Day! O peoples, let us brilliantly shine! * Pascha, the Lord's Pascha! * For Christ our God has out of death * passed us over into life, * and likewise from earth * to heaven, as we now sing * unto Him a triumphal hymn."

