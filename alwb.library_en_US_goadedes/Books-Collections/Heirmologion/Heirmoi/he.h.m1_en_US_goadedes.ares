heHE.ToProTonAionon.notmetered = "N/M (To the Son.)"
heHE.ToProTonAionon.greekname = "Τῷ πρὸ τῶν αἰώνων."
heHE.ToProTonAionon.name = "To the Son."
heHE.ToProTonAionon.incipit = "To the Son…"
heHE.ToProTonAionon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToProTonAionon.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.ToProTonAionon.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.ToProTonAionon.text = "To the Son, begotten * without flux, of the Father, before the ages, * and who was lately made incarnate * of the Virgin without seed; * to Christ God now let us cry aloud: * You have exalted the horn of our strength. Only You are holy, O Lord."

heHE.ToTheionKaiArriton.notmetered = "N/M (O Christ, I shall tell.)"
heHE.ToTheionKaiArriton.greekname = "Τὸ θεῖον καὶ ἄῤῥητον."
heHE.ToTheionKaiArriton.name = "O Christ, I shall tell."
heHE.ToTheionKaiArriton.incipit = "O Christ, I shall tell…"
heHE.ToTheionKaiArriton.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToTheionKaiArriton.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.ToTheionKaiArriton.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.ToTheionKaiArriton.text = "O Christ, I shall tell of Your virtues * and of their beauty, divine and ineffable: * As coeternal and hypostatic radiance * you shone from glory eternal; and coming forth * incarnate from the Virgin's womb, * You have risen as a Sun * on those in darkness and in the shadow of death."

heHE.ToVoithisantiTheo.notmetered = "N/M ()"
heHE.ToVoithisantiTheo.greekname = "Τῷ βοηθήσαντι Θεῷ."
heHE.ToVoithisantiTheo.name = "N/M ()"
heHE.ToVoithisantiTheo.incipit = "…"
heHE.ToVoithisantiTheo.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.ToVoithisantiTheo.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.ToVoithisantiTheo.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.ToVoithisantiTheo.text = "To God who helped Moses in Egypt, and through him drowned Pharaoh with his entire army, let us sing a song of victory, for He is glorified."

heHE.TonEnFlogiPyros.notmetered = "N/M (Let us extol the Lord.)"
heHE.TonEnFlogiPyros.greekname = "Τὸν ἐν φλογὶ πυρός."
heHE.TonEnFlogiPyros.name = "Let us extol the Lord."
heHE.TonEnFlogiPyros.incipit = "Let us extol the Lord…"
heHE.TonEnFlogiPyros.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEnFlogiPyros.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TonEnFlogiPyros.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonEnFlogiPyros.text = "Let us extol the Lord, who went down to the Servants * inside the fiery furnace, * in the form of an Angel, and guarded them from the flames; and let us sing to Him * and exalt Him beyond measure, unto all the ages."

