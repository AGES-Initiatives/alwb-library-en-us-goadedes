EkNyktosErgon.notmetered = "N/M (Would that You come.)"
EkNyktosErgon.name = "Would that You come."
EkNyktosErgon.incipit = "Would that You come…"
EkNyktosErgon.melody = misc_en_US_goadedes.Heirmos
EkNyktosErgon.mode = misc_en_US_goadedes.Mode1
EkNyktosErgon.ode = misc_en_US_goadedes.Ode5
EkNyktosErgon.text = "Would that You come, O Christ, to us who once were darkened * by the night of works of error, but who offer * hymns to You wakefully now, O Benefactor. * Grant us forgiveness, and make easy the pathway. * Ascending this path may we also find glory."
EkNyktosErgon1.text = "Would that You come, O Christ, to us who once were darkened <br>by the night of works of error, but who offer <br>hymns to You wakefully now, O Benefactor. <br>Grant us forgiveness, and make easy the pathway. <br>Ascending this path may we also find glory."
