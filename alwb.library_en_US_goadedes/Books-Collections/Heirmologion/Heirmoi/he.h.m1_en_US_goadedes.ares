NenikintaiTisFyseos.notmetered = "N/M (In you the laws.)"
NenikintaiTisFyseos.name = "In you the laws."
NenikintaiTisFyseos.incipit = "In you the laws of nature…"
NenikintaiTisFyseos.melody = misc_en_US_goadedes.Heirmos
NenikintaiTisFyseos.mode = misc_en_US_goadedes.Mode1
NenikintaiTisFyseos.ode = misc_en_US_goadedes.Ode9
NenikintaiTisFyseos.text = "In you the laws of nature are defeated,* O undefiled Virgin pure. * In virginity childbirth was accomplished, and death introduces life. * You, after bearing, are virgin, and after dying are living. * Mother of God, * ever save your inheritance."

NyxPeriescheMeSotir.notmetered = "N/M ()"
NyxPeriescheMeSotir.name = "N/M ()"
NyxPeriescheMeSotir.incipit = "N/M…"
NyxPeriescheMeSotir.melody = misc_en_US_goadedes.HeirmosNotMetered
NyxPeriescheMeSotir.mode = misc_en_US_goadedes.Mode1
NyxPeriescheMeSotir.ode = misc_en_US_goadedes.Ode5
NyxPeriescheMeSotir.text = "O Savior, night and the fog of sin have surrounded me. Therefore I pray You, let the morning of repentance rise on me, O compassionate Lord, before I reach the indefinite end of my life, O Christ, the Life and the Light."

