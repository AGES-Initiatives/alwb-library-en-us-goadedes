EkNyktosOrthrizontes.notmetered = "N/M ()"
EkNyktosOrthrizontes.name = "N/M ()"
EkNyktosOrthrizontes.incipit = "N/M…"
EkNyktosOrthrizontes.melody = misc_en_US_goadedes.HeirmosNotMetered
EkNyktosOrthrizontes.mode = misc_en_US_goadedes.Mode1
EkNyktosOrthrizontes.ode = misc_en_US_goadedes.Ode5
EkNyktosOrthrizontes.text = "Rising early in the morning, we extol You, O Christ our God, who for us became poor, and endured the Cross and death in the flesh."

EkyklosenImas.notmetered = "N/M (The lowest depth encircles us.)"
EkyklosenImas.name = "The lowest depth encircles us."
EkyklosenImas.incipit = "The lowest depth encircles us…"
EkyklosenImas.melody = misc_en_US_goadedes.Heirmos
EkyklosenImas.mode = misc_en_US_goadedes.Mode1
EkyklosenImas.ode = misc_en_US_goadedes.Ode6
EkyklosenImas.text = "The lowest depth encircles us, like Jonah said, * and there is no deliverer. * We are counted as sheep for slaughter, O Lord. * Save Your people, for You are our only God. * You are our strength, and You restore us, the weak and broken down."

EnKaminoPaides.notmetered = "N/M (Three young men of Israel.)"
EnKaminoPaides.name = "Three young men of Israel."
EnKaminoPaides.incipit = "Three young men of Israel…"
EnKaminoPaides.melody = misc_en_US_goadedes.Heirmos
EnKaminoPaides.mode = misc_en_US_goadedes.Mode1
EnKaminoPaides.ode = misc_en_US_goadedes.Ode8
EnKaminoPaides.text = "Three young men of Israel were thrown * into the blazing furnace, * as if they were being smelted. * Shining brighter than pure gold with beauty of piety, * they sang, \"Bless the Lord, all you works; to the Lord sing a hymn * and exalt Him beyond measure unto all the ages.\""

EnPnevmatiProvlepon.notmetered = "N/M (When you, Prophet Habbakuk.)"
EnPnevmatiProvlepon.name = "When you, Prophet Habbakuk."
EnPnevmatiProvlepon.incipit = "When you, Prophet Habbakuk…"
EnPnevmatiProvlepon.melody = misc_en_US_goadedes.Heirmos
EnPnevmatiProvlepon.mode = misc_en_US_goadedes.Mode1
EnPnevmatiProvlepon.ode = misc_en_US_goadedes.Ode4
EnPnevmatiProvlepon.text = "When you, Prophet Habbakuk, inspired had foreseen * the incarnation of the Word, you shouted and declared, * \"In the approaching years You shall be acknowledged; * You will be revealed when the time comes. * Glory to Your power, O Lord.\""

