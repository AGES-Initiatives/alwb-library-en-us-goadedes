heHE.TonProfitinDiesosas.notmetered = "N/M (As You rescued the Prophet.)"
heHE.TonProfitinDiesosas.greekname = "Τὸν Προφήτην διέσωσας."
heHE.TonProfitinDiesosas.name = "As You rescued the Prophet."
heHE.TonProfitinDiesosas.incipit = "As You rescued the Prophet…"
heHE.TonProfitinDiesosas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonProfitinDiesosas.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TonProfitinDiesosas.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.TonProfitinDiesosas.text = "As You rescued the Prophet from the belly of the whale of old, * I pray bring me up from the sea of my sins, O benevolent Lord."

heHE.TonProfitinIonan.notmetered = "N/M (Imitating the inspired Prophet.)"
heHE.TonProfitinIonan.greekname = "Τὸν Προφήτην Ἰωνᾶν."
heHE.TonProfitinIonan.name = "Imitating the inspired Prophet."
heHE.TonProfitinIonan.incipit = "Imitating the inspired Prophet…"
heHE.TonProfitinIonan.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonProfitinIonan.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TonProfitinIonan.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.TonProfitinIonan.text = "Imitating the inspired Prophet Jonah in the whale, * I cry out to You, O Lord, * \"From destruction free my life * and save me, I pray * to You, O Savior of the world. Glory to You!\""

heHE.TonTousYmnologousEnKamino.notmetered = "N/M (In the furnace He preserved.)"
heHE.TonTousYmnologousEnKamino.greekname = "Τὸν τοὺς ὑμνολόγους ἐν καμίνῳ."
heHE.TonTousYmnologousEnKamino.name = "In the furnace He preserved."
heHE.TonTousYmnologousEnKamino.incipit = "In the furnace He preserved…"
heHE.TonTousYmnologousEnKamino.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonTousYmnologousEnKamino.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TonTousYmnologousEnKamino.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonTousYmnologousEnKamino.text = "In the furnace He preserved those Servants * who sang a hymn to extol Him. * He made the roaring furnace like moist whistling wind blowing through it. * Sing praises to Christ our God, and extol Him beyond measure unto all the ages."

heHE.TonTyponTouTheiouStavrou.notmetered = "N/M (When Jonah was in.)"
heHE.TonTyponTouTheiouStavrou.greekname = "Τὸν τύπον τοῦ θείου Σταυροῦ."
heHE.TonTyponTouTheiouStavrou.name = "When Jonah was in."
heHE.TonTyponTouTheiouStavrou.incipit = "When Jonah was in…"
heHE.TonTyponTouTheiouStavrou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonTyponTouTheiouStavrou.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TonTyponTouTheiouStavrou.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.TonTyponTouTheiouStavrou.text = "When Jonah was in the belly of the whale, * he extended his arms and thus foreshadowed the figure of Your holy Cross, O Lord. * Then he bounded forth * from the monster, being saved by Your power, O Logos."

heHE.TousEnKaminoPaidas.notmetered = "N/M (O Lord and Savior.)"
heHE.TousEnKaminoPaidas.greekname = "Τοὺς ἐν καμίνῳ Παῖδας."
heHE.TousEnKaminoPaidas.name = "O Lord and Savior."
heHE.TousEnKaminoPaidas.incipit = "O Lord and Savior…"
heHE.TousEnKaminoPaidas.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.TousEnKaminoPaidas.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TousEnKaminoPaidas.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.TousEnKaminoPaidas.text = "O Lord and Savior, fire did not harm, * nor touch at all, * Your Servants in the furnace of old. * Those three young men then shouted in unison, * blessing and extolling God, and sang, * \"God of our fathers, unto the ages, blessed are You!\""

heHE.TyponTisAgnis.notmetered = "N/M (Burning, yet.)"
heHE.TyponTisAgnis.greekname = "Τύπον τῆς ἁγνῆς."
heHE.TyponTisAgnis.name = "Burning, yet remaining."
heHE.TyponTisAgnis.incipit = "Burning, yet remaining…"
heHE.TyponTisAgnis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TyponTisAgnis.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.TyponTisAgnis.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TyponTisAgnis.text = "Burning, yet remaining unconsumed, * the bush revealed a type of your virgin childbirth. * And now we entreat that you extinguish the wild fire * of temptations, for it is engulfing us, * all-holy Theotokos, * and we will magnify you ceaselessly."

heHE.YmnonSoiProsferomen.notmetered = "N/M ()"
heHE.YmnonSoiProsferomen.greekname = "Ὕμνον σοι προσφέρομεν."
heHE.YmnonSoiProsferomen.name = "N/M ()"
heHE.YmnonSoiProsferomen.incipit = "N/M…"
heHE.YmnonSoiProsferomen.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.YmnonSoiProsferomen.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.YmnonSoiProsferomen.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.YmnonSoiProsferomen.text = "We offer You a hymn of the Bodiless hosts, as did the Servants in the furnace. And we extol You and say: Bless the Lord, all you works of the Lord."

