TonProfitinDiesosas.notmetered = "N/M (As You saved Prophet Jonah.)"
TonProfitinDiesosas.name = "As You saved Prophet Jonah."
TonProfitinDiesosas.incipit = "As You saved Prophet Jonah…"
TonProfitinDiesosas.melody = misc_en_US_goadedes.Heirmos
TonProfitinDiesosas.mode = misc_en_US_goadedes.Mode1
TonProfitinDiesosas.ode = misc_en_US_goadedes.Ode6
TonProfitinDiesosas.text = "As You saved Prophet Jonah from the belly of the whale of old, * I pray raise me up from the sea of my sins, in Your love for man."

TonProfitinIonan.notmetered = "N/M (Imitating the inspired Prophet.)"
TonProfitinIonan.name = "Imitating the inspired Prophet."
TonProfitinIonan.incipit = "Imitating the inspired Prophet…"
TonProfitinIonan.melody = misc_en_US_goadedes.Heirmos
TonProfitinIonan.mode = misc_en_US_goadedes.Mode1
TonProfitinIonan.ode = misc_en_US_goadedes.Ode6
TonProfitinIonan.text = "Imitating the inspired Prophet Jonah in the whale, * I cry out to You, O Lord, * \"From destruction free my life * and save me, I pray * to You, O Savior of the world. Glory to You!\""

