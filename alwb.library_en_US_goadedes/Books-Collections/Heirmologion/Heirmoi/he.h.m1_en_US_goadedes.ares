heHE.EkNyktosErgon.notmetered = "N/M (Would that You come.)"
heHE.EkNyktosErgon.greekname = "Ἐκ νυκτὸς ἔργων."
heHE.EkNyktosErgon.name = "Would that You come."
heHE.EkNyktosErgon.incipit = "Would that You come…"
heHE.EkNyktosErgon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EkNyktosErgon.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EkNyktosErgon.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.EkNyktosErgon.text = "Would that You come, O Christ, to us who once were darkened * by the night of works of error, but who offer * hymns to You wakefully now, O Benefactor. * Grant us forgiveness, and make easy the pathway. * Ascending this path may we also find glory."
heHE.EkNyktosErgon1.text = "Would that You come, O Christ, to us who once were darkened <br>by the night of works of error, but who offer <br>hymns to You wakefully now, O Benefactor. <br>Grant us forgiveness, and make easy the pathway. <br>Ascending this path may we also find glory."
