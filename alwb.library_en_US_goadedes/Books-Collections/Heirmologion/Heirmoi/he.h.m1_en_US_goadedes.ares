heHE.EkNyktosOrthrizontes.notmetered = "N/M ()"
heHE.EkNyktosOrthrizontes.greekname = "Ἐκ νυκτὸς ὀρθρίζοντες."
heHE.EkNyktosOrthrizontes.name = "N/M ()"
heHE.EkNyktosOrthrizontes.incipit = "N/M…"
heHE.EkNyktosOrthrizontes.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.EkNyktosOrthrizontes.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EkNyktosOrthrizontes.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.EkNyktosOrthrizontes.text = "Rising early in the morning, we extol You, O Christ our God, who for us became poor, and endured the Cross and death in the flesh."

heHE.EkNyktosOrthrizontesYmnoumenSe.notmetered = "N/M (Rising from the night, we sing.)"
heHE.EkNyktosOrthrizontesYmnoumenSe.greekname = "Ἐκ νυκτὸς ὀρθρίζοντες ὑμνοῦμέν σε."
heHE.EkNyktosOrthrizontesYmnoumenSe.name = "Rising from the night, we sing."
heHE.EkNyktosOrthrizontesYmnoumenSe.incipit = "Rising from the night, we sing…"
heHE.EkNyktosOrthrizontesYmnoumenSe.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EkNyktosOrthrizontesYmnoumenSe.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EkNyktosOrthrizontesYmnoumenSe.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.EkNyktosOrthrizontesYmnoumenSe.text = "Rising from the night, we sing extolling You, O Christ, * the Father's coeternal Son * and the Savior of our souls, O Lord. * Grant Your peace to the world, in Your divine benevolence."

heHE.EkyklosenImas.notmetered = "N/M (The lowest depth encircles us.)"
heHE.EkyklosenImas.greekname = "Ἐκύκλωσεν ἡμᾶς."
heHE.EkyklosenImas.name = "The lowest depth encircles us."
heHE.EkyklosenImas.incipit = "The lowest depth encircles us…"
heHE.EkyklosenImas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EkyklosenImas.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EkyklosenImas.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.EkyklosenImas.text = "The lowest depth encircles us, like Jonah said, * and there is no deliverer. * We are counted as sheep for slaughter, O Lord. * Save Your people, for You are our only God. * You are our strength, and You restore us, the weak and broken down."

heHE.EnKaminoPaides.notmetered = "N/M (Three young men of Israel.)"
heHE.EnKaminoPaides.greekname = "Ἐν καμίνῳ Παῖδες."
heHE.EnKaminoPaides.name = "Three young men of Israel."
heHE.EnKaminoPaides.incipit = "Three young men of Israel…"
heHE.EnKaminoPaides.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EnKaminoPaides.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EnKaminoPaides.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.EnKaminoPaides.text = "Three young men of Israel were thrown * into the blazing furnace, * as if they were being smelted. * Shining brighter than pure gold with beauty of piety, * they sang, \"Bless the Lord, all you works; to the Lord sing a hymn * and exalt Him beyond measure unto all the ages.\""

heHE.EnPnevmatiProvlepon.notmetered = "N/M (When you, Prophet Habbakuk.)"
heHE.EnPnevmatiProvlepon.greekname = "Ἐν Πνεύματι προβλέπων."
heHE.EnPnevmatiProvlepon.name = "When you, Prophet Habbakuk."
heHE.EnPnevmatiProvlepon.incipit = "When you, Prophet Habbakuk…"
heHE.EnPnevmatiProvlepon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EnPnevmatiProvlepon.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.EnPnevmatiProvlepon.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.EnPnevmatiProvlepon.text = "When you, Prophet Habbakuk, inspired had foreseen * the incarnation of the Word, you shouted and declared, * \"In the approaching years You shall be acknowledged; * You will be revealed when the time comes. * Glory to Your strength, O Lord.\""

