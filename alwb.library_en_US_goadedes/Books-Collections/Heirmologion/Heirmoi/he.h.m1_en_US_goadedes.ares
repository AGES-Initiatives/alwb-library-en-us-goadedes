TonTyponTouTheiouStavrou.notmetered = "N/M (When Jonah was in.)"
TonTyponTouTheiouStavrou.name = "When Jonah was in."
TonTyponTouTheiouStavrou.incipit = "When Jonah was in…"
TonTyponTouTheiouStavrou.melody = misc_en_US_goadedes.Heirmos
TonTyponTouTheiouStavrou.mode = misc_en_US_goadedes.Mode1
TonTyponTouTheiouStavrou.ode = misc_en_US_goadedes.Ode6
TonTyponTouTheiouStavrou.text = "When Jonah was in the belly of the whale, * he extended his arms and thus foreshadowed the figure of Your holy Cross, O Lord. * Then he bounded forth * from the monster, being saved by Your power, O Logos."

TousEnKaminoPaidas.notmetered = "N/M ()"
TousEnKaminoPaidas.name = "N/M ()"
TousEnKaminoPaidas.incipit = "N/M…"
TousEnKaminoPaidas.melody = misc_en_US_goadedes.HeirmosNotMetered
TousEnKaminoPaidas.mode = misc_en_US_goadedes.Mode1
TousEnKaminoPaidas.ode = misc_en_US_goadedes.Ode7
TousEnKaminoPaidas.text = "O Savior, the fire did not touch nor affect Your Servants who were in the furnace. Then the three, as if from one mouth, extolled and blessed You, saying: \"O God of our Fathers, blessed are You!\""

TyponTisAgnis.notmetered = "N/M (Burning, yet.)"
TyponTisAgnis.name = "Burning, yet remaining."
TyponTisAgnis.incipit = "Burning, yet remaining…"
TyponTisAgnis.melody = misc_en_US_goadedes.Heirmos
TyponTisAgnis.mode = misc_en_US_goadedes.Mode1
TyponTisAgnis.ode = misc_en_US_goadedes.Ode9
TyponTisAgnis.text = "Burning, yet remaining unconsumed, * the bush revealed a type of your virgin childbirth. * And now we entreat that you extinguish the wild fire * of temptations, for it is engulfing us, * all-holy Theotokos, * and we will magnify you ceaselessly."

YmnonSoiProsferomen.notmetered = "N/M ()"
YmnonSoiProsferomen.name = "N/M ()"
YmnonSoiProsferomen.incipit = "N/M…"
YmnonSoiProsferomen.melody = misc_en_US_goadedes.HeirmosNotMetered
YmnonSoiProsferomen.mode = misc_en_US_goadedes.Mode1
YmnonSoiProsferomen.ode = misc_en_US_goadedes.Ode8
YmnonSoiProsferomen.text = "We offer You a hymn of the Bodiless hosts, as did the Servants in the furnace. And we extol You and say: Bless the Lord, all you works of the Lord."

