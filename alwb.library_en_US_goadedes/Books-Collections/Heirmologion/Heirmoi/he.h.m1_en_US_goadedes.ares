heHE.OEnTiVatoOftheis.name = "When You appeared."
heHE.OEnTiVatoOftheis.incipit = "When You appeared…"
heHE.OEnTiVatoOftheis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OEnTiVatoOftheis.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OEnTiVatoOftheis.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.OEnTiVatoOftheis.text = "When You appeared in the burning bush to the law-giver Moses, * You prefigured there the childbirth of the Ever-Virgin. * Blessed are You, O Lord, the God of our fathers."

heHE.OFotisasTiEllampsei.notmetered = "N/M (By the brightness of Your coming.)"
heHE.OFotisasTiEllampsei.greekname = "Ὁ φωτίσας τῇ ἐλλάμψει."
heHE.OFotisasTiEllampsei.name = "By the brightness of Your coming."
heHE.OFotisasTiEllampsei.incipit = "By the brightness of Your coming…"
heHE.OFotisasTiEllampsei.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OFotisasTiEllampsei.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OFotisasTiEllampsei.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.OFotisasTiEllampsei.text = "By the brightness of Your coming You enlightened the world, O Christ, * by the power of Your Cross you illumined the ends thereof. * With the light of knowing You as God enlighten now the hearts of * those who extol You with Orthodox faith."

heHE.OMonosEidos.notmetered = "N/M (As You are the only One.)"
heHE.OMonosEidos.greekname = "Ὁ μόνος εἰδώς."
heHE.OMonosEidos.name = "As You are the only One."
heHE.OMonosEidos.incipit = "As You are the only One…"
heHE.OMonosEidos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OMonosEidos.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OMonosEidos.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.OMonosEidos.text = "As You are the only One who knows * our mortal nature's frailty and with compassion assumed the form thereof, * O Savior, gird me with the power from on high, * to cry out that Holy is the animate temple * of Your ineffable glory, benevolent Lord."

heHE.OPaidasEkKaminou.notmetered = "N/M (The same One.)"
heHE.OPaidasEkKaminou.greekname = "Ὁ Παῖδας ἐκ καμίνου."
heHE.OPaidasEkKaminou.name = "The same One."
heHE.OPaidasEkKaminou.incipit = "The same One…"
heHE.OPaidasEkKaminou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OPaidasEkKaminou.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OPaidasEkKaminou.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.OPaidasEkKaminou.text = "The same One who delivered the Lads from fire, becoming a mortal man, * suffers in the flesh; and by His Passion He arrays * mortal nature with majesty, free of all * corruption, the only God of the fathers, * blessed and supremely glorious."

heHE.OTonAnoVython.notmetered = "N/M (O all-blessed Father.)"
heHE.OTonAnoVython.greekname = "Ὁ τὸν ἄνω βυθόν."
heHE.OTonAnoVython.name = "N/M (O all-blessed Father.)"
heHE.OTonAnoVython.incipit = "O all-blessed Father…"
heHE.OTonAnoVython.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.OTonAnoVython.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OTonAnoVython.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.OTonAnoVython.text = "O all-blessed Father, you reverently swam up into the ocean of divine incomprehensibility on high, and you supported your mind on the divine Trinity, as on an incomprehensible rock. Therefore we magnify you."

heHE.OdinEpinikion.notmetered = "N/M (An anthem of victory.)"
heHE.OdinEpinikion.greekname = "ᾨδὴν ἐπινίκιον."
heHE.OdinEpinikion.name = "An anthem of victory."
heHE.OdinEpinikion.incipit = "An anthem of victory…"
heHE.OdinEpinikion.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OdinEpinikion.mode = miscellanea_en_US_goadedes.misc.Mode1
heHE.OdinEpinikion.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.OdinEpinikion.text = "An anthem of victory let us all sing now * to God as the One who wrought * wondrous and portentous signs * by His exalted arm, * and who saved Israel of old. For He is glorified."

