heHE.AsomenOdinToTheo.greekname = "ᾌσωμεν ᾠδὴν τῷ Θεῷ."
heHE.AsomenOdinToTheo.name = ""
heHE.AsomenOdinToTheo.incipit = "…"
heHE.AsomenOdinToTheo.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.AsomenOdinToTheo.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.AsomenOdinToTheo.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.AsomenOdinToTheo.text = "Let us sing a song to God, who delivered Israel from bitter slavery to Pharaoh, and who led them by a pillar of fire and a cloud of light; for He is glorified."

heHE.AsomenToKyrioPantesLaoi.notmetered = "N/M ()"
heHE.AsomenToKyrioPantesLaoi.greekname = "ᾌσωμεν τῷ Κυρίῳ πάντες λαοί."
heHE.AsomenToKyrioPantesLaoi.name = ""
