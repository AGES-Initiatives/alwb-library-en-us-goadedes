heHE.AsomenToKyrioPantesLaoi.text = ""

heHE.AsomenToKyrio.notmetered = "N/M (Using the words of Moses.)"
heHE.AsomenToKyrio.greekname = "ᾌσωμεν τῷ Κυρίῳ."
heHE.AsomenToKyrio.name = "Using the words of Moses."
heHE.AsomenToKyrio.incipit = "Using the words of Moses…"
heHE.AsomenToKyrio.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AsomenToKyrio.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.AsomenToKyrio.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.AsomenToKyrio.text = "Using the words of Moses * and the people, let us sing unto the Lord, * who led them through the Red Sea. * Only He is triumphantly glorified."

heHE.AvyssosAmartion.notmetered = "N/M (My transgressions)"
heHE.AvyssosAmartion.greekname = "Ἄβυσσος ἁμαρτιῶν."
heHE.AvyssosAmartion.name = "My transgressions."
heHE.AvyssosAmartion.incipit = "My transgressions…"
heHE.AvyssosAmartion.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AvyssosAmartion.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.AvyssosAmartion.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.AvyssosAmartion.text = "My transgressions and my sins, * like a tempest on the deep sea, afflict me * and forcibly compel me to the depths of despondency. * So I pray You to extend * Your mighty arm to me, as to Peter, * O God, my Pilot, and to rescue me." //and to raise me * up from corruption, O merciful Lord my God.

heHE.ChitonaMoiParaschou.notmetered = "N/M (I pray… grant.)"
heHE.ChitonaMoiParaschou.greekname = "Χιτῶνά μοι παράσχου."
heHE.ChitonaMoiParaschou.name = "I pray that You will grant."
heHE.ChitonaMoiParaschou.incipit = "I pray that You will grant…"
heHE.ChitonaMoiParaschou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ChitonaMoiParaschou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ChitonaMoiParaschou.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.ChitonaMoiParaschou.text = "I pray that You will grant to me a bright * garment, O most merciful Lord, who cover yourself * with the light as with a garment, O Christ our God."

heHE.EfrixePasaAkoi.notmetered = "N/M (All hearers.)"
heHE.EfrixePasaAkoi.greekname = "Ἔφριξε πᾶσα ἀκοή."
heHE.EfrixePasaAkoi.name = "All hearers shuddered."
heHE.EfrixePasaAkoi.incipit = "All hearers shuddered…"
heHE.EfrixePasaAkoi.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EfrixePasaAkoi.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EfrixePasaAkoi.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.EfrixePasaAkoi.text = "All hearers shuddered when they heard * the unheard-of condescension of God to us: * that voluntarily * the Most High came down, and even bodily, * and from a Virgin’s womb was born, * becoming a man from her. * Hence, the immaculate * Theotokos we believers now magnify."

heHE.EisakikoaKyrie.notmetered = "N/M (I have heard.)"
heHE.EisakikoaKyrie.greekname = "Εἰσακήκοα Κύριε."
heHE.EisakikoaKyrie.name = "I have heard the report."
heHE.EisakikoaKyrie.incipit = "I have heard the report…"
heHE.EisakikoaKyrie.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EisakikoaKyrie.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EisakikoaKyrie.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.EisakikoaKyrie.text = "I have heard the report, O Lord, * of Your incarnation, which is a mystery. * I considered Your astounding works, * and I glorified Your divinity." //I have heard the report, O Lord, * of Your dispensation’s amazing mystery. * I considered well Your mighty works; * thereforeYour divinity I glorified.

heHE.EisakikoaKyrieTinAkoinSou.notmetered = "N/M"
heHE.EisakikoaKyrieTinAkoinSou.greekname = "Εἰσακήκος Κύριε, τὴν ἀκοήν."
heHE.EisakikoaKyrieTinAkoinSou.name = ""
heHE.EisakikoaKyrieTinAkoinSou.incipit = "…"
heHE.EisakikoaKyrieTinAkoinSou.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.EisakikoaKyrieTinAkoinSou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EisakikoaKyrieTinAkoinSou.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.EisakikoaKyrieTinAkoinSou.text = "Lord, I heard Your report and I was afraid. I considered Your works, and I glorified Your dominion, O Master."

