heHE.EnPedioDeeira.text = ""

heHE.EpevisEfIppous.notmetered = "N/M (You mounted Your horses.)"
heHE.EpevisEfIppous.greekname = "Ἐπέβης ἐφʼ ἵππους."
heHE.EpevisEfIppous.name = "You mounted Your horses."
heHE.EpevisEfIppous.incipit = "You mounted Your horses…"
heHE.EpevisEfIppous.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EpevisEfIppous.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EpevisEfIppous.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.EpevisEfIppous.text = "You mounted Your horses, namely Your holy Apostles, O Lord, * and You guided them, holding their reins in Your hands. * And thus Your chariot became salvation for us all, * who are faithfully singing * unto You: Glory to Your strength, O Lord!"

heHE.EptaplasiosKaminon.notmetered = "N/M (The tyrant king.)"
heHE.EptaplasiosKaminon.greekname = "Ἑπταπλασίως κάμινον."
heHE.EptaplasiosKaminon.name = "The tyrant king."
heHE.EptaplasiosKaminon.incipit = "The tyrant king…"
heHE.EptaplasiosKaminon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EptaplasiosKaminon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EptaplasiosKaminon.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.EptaplasiosKaminon.text = "The tyrant king of Babylon * had the furnace made seven times * hotter for the godly men, for he was furious. * But after he witnessed how * a higher power rescued them, * he cried out and said, “You Servants, bless the Creator; * sing hymns to the Redeemer, O you priests; and you people * exalt Him beyond measure, unto all the ages.”"

heHE.EstereothiIKardiaMouEnKyrio.notmetered = "N/M (My heart exults.)"
heHE.EstereothiIKardiaMouEnKyrio.greekname = "Ἐστερεώθη ἡ καρδία μου."
heHE.EstereothiIKardiaMouEnKyrio.name = "My heart exults."
heHE.EstereothiIKardiaMouEnKyrio.incipit = "My heart exults…"
heHE.EstereothiIKardiaMouEnKyrio.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EstereothiIKardiaMouEnKyrio.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EstereothiIKardiaMouEnKyrio.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.EstereothiIKardiaMouEnKyrio.text = "My heart exults and in the Lord firmly is established; * and in my God is my horn exalted; * and my mouth is open wide at my enemies, and in Your salvation I rejoice."

heHE.EvlogeitePaides.notmetered = "N/M (You three pious Servants.)"
heHE.EvlogeitePaides.greekname = "Εὐλογεῖτε Παῖδες."
heHE.EvlogeitePaides.name = "You three pious Servants."
heHE.EvlogeitePaides.incipit = "You three pious Servants…"
heHE.EvlogeitePaides.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EvlogeitePaides.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EvlogeitePaides.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.EvlogeitePaides.text = "You three pious Servants, * same in number as the Trinity, * bless God the Father, Maker of all; * sing a hymn to the Word who condescended * and who changed the fire in the furnace into dew; * as for the all-holy Spirit, who gives life to all, exalt Him * beyond measure unto the ages." //You in equal number * to the Trinity, pious Youths, bless God the Father Maker of all, * and also praise the Word who condescended * and who changed the fire in the furnace into dew; * the all-holy Spirit who gives life to all creation, * also exalt supremely to the ages.

heHE.EvlogitosOKyrios.notmetered = "N/M (Blessed is He.)"
heHE.EvlogitosOKyrios.greekname = "Εὐλογητὸς ὁ Κύριος."
heHE.EvlogitosOKyrios.name = "Blessed is He."
heHE.EvlogitosOKyrios.incipit = "Blessed is He…"
heHE.EvlogitosOKyrios.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.EvlogitosOKyrios.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.EvlogitosOKyrios.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.EvlogitosOKyrios.text = "Blessed is He who raised a horn of salvation in the house * of His servant David, as He spoke by the mouth * of Prophets, the Lord God of Israel. * The Day dawned upon us from on high, and He gave light and guided * our feet into the way of peace through His mercy."

heHE.ExOrousKataskiou.notmetered = "N/M (The Prophet understood.)"
heHE.ExOrousKataskiou.greekname = "Ἐξ ὄρους κατασκίου."
heHE.ExOrousKataskiou.name = "The Prophet understood."
heHE.ExOrousKataskiou.incipit = "The Prophet understood…"
heHE.ExOrousKataskiou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ExOrousKataskiou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ExOrousKataskiou.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.ExOrousKataskiou.text = "The Prophet understood that You would be incarnate * from the Theotokos when he had considered * with divine vision, O Logos, the shaded mountain. * Afraid, he glorified Your power, O Lord."

heHE.ExestiEpiTouto.notmetered = "N/M (The heavens.)"
heHE.ExestiEpiTouto.greekname = "Ἐξέστη ἐπὶ τούτῳ."
heHE.ExestiEpiTouto.name = "The heavens were astounded."
heHE.ExestiEpiTouto.incipit = "The heavens were astounded…"
heHE.ExestiEpiTouto.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ExestiEpiTouto.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ExestiEpiTouto.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.ExestiEpiTouto.text = "The heavens were astounded because of this, * and the ends of the earth were astonished, because God himself * bodily appeared to the human race, * O Theotokos, coming forth * from your virgin womb, which indeed became * more spacious than the heavens. * And therefore by the orders * of men and angels you are magnified."

heHE.FotisonImas.notmetered = "N/M (Sovereign Lord.)"
heHE.FotisonImas.greekname = "Φώτισον ἡμᾶς."
heHE.FotisonImas.name = "Sovereign Lord our God."
heHE.FotisonImas.incipit = "Sovereign Lord our God…"
heHE.FotisonImas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.FotisonImas.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.FotisonImas.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.FotisonImas.text = "Sovereign Lord our God, * as the One who is benevolent, * with your commandments enlighten us, we pray; * and also grant us * Your peace with Your own uplifted arm."

heHE.IKekommeni.notmetered = "N/M (The rod of Moses divided.)"
heHE.IKekommeni.greekname = "Ἡ κεκομμένη."
heHE.IKekommeni.name = "The rod of Moses divided."
heHE.IKekommeni.incipit = "The rod of Moses divided…"
heHE.IKekommeni.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.IKekommeni.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.IKekommeni.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.IKekommeni.text = "The rod of Moses divided the sea of old, * and for the first time, the sun saw the ground beneath the sea. * As Israel passed over what had been impassable, * the enemy was drowned beneath the sea. * And then the people sang the song, * saying: “Let us sing to the Lord, * for He is greatly glorified.”"

