heHE.OrousParilthes.greekname = "Ὅρους παρῆλθες."
heHE.OrousParilthes.name = "When you conceived."
heHE.OrousParilthes.incipit = "When you conceived…"
heHE.OrousParilthes.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OrousParilthes.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OrousParilthes.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.OrousParilthes.text = "When you conceived your Maker and Lord, * you eluded nature’s conventions, O Virgin pure; * and you became a gate to salvation for the world. * Therefore, O Theotokos, * we faithful never cease to magnify you."

heHE.OrthrizeiToPnevmaMou.notmetered = "N/M ()"
heHE.OrthrizeiToPnevmaMou.greekname = "Ὀρθρίζει τὸ πνεῦμά μου."
heHE.OrthrizeiToPnevmaMou.name = "N/M ()"
heHE.OrthrizeiToPnevmaMou.incipit = "N/M ()…"
heHE.OrthrizeiToPnevmaMou.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.OrthrizeiToPnevmaMou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OrthrizeiToPnevmaMou.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.OrthrizeiToPnevmaMou.text = "My spirit rises early to You, O God, for Your commandments are the light of Your presence. With them shine upon our minds, O Master, and guide us on the path of life."

heHE.OrthrizontesVoomenSoi.notmetered = "N/M (We rise at early morn.)"
heHE.OrthrizontesVoomenSoi.greekname = "Ὀρθρίζοντες βοῶμέν σοι."
heHE.OrthrizontesVoomenSoi.name = "We rise at early morn."
heHE.OrthrizontesVoomenSoi.incipit = "We rise at early morn…"
heHE.OrthrizontesVoomenSoi.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OrthrizontesVoomenSoi.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OrthrizontesVoomenSoi.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.OrthrizontesVoomenSoi.text = "We rise at early morn and cry out to You, * “Save us, O Lord!” * For You only are our God; * we have no other except for You."

heHE.OsTonProfitinErryso.notmetered = "N/M (O Christ my God.)"
heHE.OsTonProfitinErryso.greekname = "Ὡς τὸν Προφήτην ἐῤῥύσω."
heHE.OsTonProfitinErryso.name = "O Christ my God."
heHE.OsTonProfitinErryso.incipit = "O Christ my God…"
heHE.OsTonProfitinErryso.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OsTonProfitinErryso.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OsTonProfitinErryso.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.OsTonProfitinErryso.text = "O Christ my God, as You rescued * Prophet Jonah of old from the depths of the sea, * I pray You, now rescue me * from my sins, and steer the ship * of my life, as You are my benevolent Lord."

heHE.OsYdataThalassis.notmetered = "N/M (The troubles of my life.)"
heHE.OsYdataThalassis.greekname = "Ὡς ὕδατα θαλάσσης."
heHE.OsYdataThalassis.name = "The troubles of my life."
heHE.OsYdataThalassis.incipit = "The troubles of my life…"
heHE.OsYdataThalassis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OsYdataThalassis.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OsYdataThalassis.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.OsYdataThalassis.text = "The troubles of my life are like storms at sea, * O Logos, and the waves are afflicting me. * I cry to You, as once did the Prophet Jonah, * to raise my life from corruption, O only Lord and compassionate God."

heHE.OukEstinAgios.notmetered = "N/M (No one is holy.)"
heHE.OukEstinAgios.greekname = "Οὔκ ἐστιν ἅγιος."
heHE.OukEstinAgios.name = "No one is holy."
heHE.OukEstinAgios.incipit = "No one is holy…"
heHE.OukEstinAgios.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.OukEstinAgios.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OukEstinAgios.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.OukEstinAgios.text = "No one is holy like You are, O Lord, and no one is righteous like You our God. All creation extols You. No one is holy except for You, O Lord."

heHE.OukEstinAgiosOsOTheos.notmetered = "N/M"
heHE.OukEstinAgiosOsOTheos.greekname = "Οὔκ ἐστιν ἅγιος ὡς ὁ Θεός."
heHE.OukEstinAgiosOsOTheos.name = "No one is holy like God."
heHE.OukEstinAgiosOsOTheos.incipit = "…"
heHE.OukEstinAgiosOsOTheos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OukEstinAgiosOsOTheos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OukEstinAgiosOsOTheos.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.OukEstinAgiosOsOTheos.text = "[Missing]"

heHE.OuraniasApsidos.notmetered = "N/M (O divine Master Builder.)"
heHE.OuraniasApsidos.greekname = "Οὐρανίας ἁψῖδος."
heHE.OuraniasApsidos.name = "O divine Master Builder."
heHE.OuraniasApsidos.incipit = "O divine Master Builder…"
heHE.OuraniasApsidos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.OuraniasApsidos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.OuraniasApsidos.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.OuraniasApsidos.text = "O divine Master Builder of the celestial vault, * Founder of the Church and the ultimate object of desire, * only benevolent support of every believer, * in the love for You, O Lord, firmly establish me."

heHE.PaidesEvraion.notmetered = "N/M (Servants, descendants.)"
heHE.PaidesEvraion.greekname = "Παῖδες Ἑβραίων."
heHE.PaidesEvraion.name = "Servants, descendants."
heHE.PaidesEvraion.incipit = "Servants, descendants…"
heHE.PaidesEvraion.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.PaidesEvraion.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.PaidesEvraion.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.PaidesEvraion.text = "Servants, descendants of the Hebrews, * bravely trampled down the flame within the furnace; * and they changed into dew * the fire, and they chanted: * Blessed are You, O Lord, the God * of our fathers to the ages."

heHE.PaidesTheoseveis.notmetered = "N/M (God-fearing holy Servants.)"
heHE.PaidesTheoseveis.greekname = "Παῖδες θεοσεβεῖς."
heHE.PaidesTheoseveis.name = "God-fearing holy Servants."
heHE.PaidesTheoseveis.incipit = "God-fearing holy Servants…"
heHE.PaidesTheoseveis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.PaidesTheoseveis.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.PaidesTheoseveis.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.PaidesTheoseveis.text = "God-fearing holy Servants did not worship * the image of gold of old in Babylon. * They were thrown in the furnace, * where no fiery flame, but moist * whistling wind refreshed them, as they extolled * the Lord and sang: “O God of our fathers, blessed are You.”"

heHE.ProsTonDynamenonSozein.notmetered = "N/M (From the pit of sins.)"
heHE.ProsTonDynamenonSozein.greekname = "Πρὸς τὸν δυνάμενον σῴζειν."
heHE.ProsTonDynamenonSozein.name = "From the pit of sins."
heHE.ProsTonDynamenonSozein.incipit = "From the pit of sins…"
heHE.ProsTonDynamenonSozein.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.ProsTonDynamenonSozein.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ProsTonDynamenonSozein.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.ProsTonDynamenonSozein.text = "From the pit of sins I cried to the Lord, who can save me; and He lifted my life from destruction, for He is benevolent."

heHE.ProsecheOurane.notmetered = "N/M ()"
heHE.ProsecheOurane.greekname = "Πρόσεχε οὐρανέ."
heHE.ProsecheOurane.name = ""
heHE.ProsecheOurane.incipit = "…"
heHE.ProsecheOurane.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ProsecheOurane.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ProsecheOurane.ode = miscellanea_en_US_goadedes.misc.Ode2
heHE.ProsecheOurane.text = ""

heHE.RavdosEisTypon.notmetered = "N/M (The rod of Aaron.)"
heHE.RavdosEisTypon.greekname = "Ῥάβδος εἰς τύπον."
heHE.RavdosEisTypon.name = "The rod of Aaron."
heHE.RavdosEisTypon.incipit = "The rod of Aaron…"
heHE.RavdosEisTypon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.RavdosEisTypon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.RavdosEisTypon.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.RavdosEisTypon.text = "The rod of Aaron is seen as typifying the mystery; * for it budded, and thus was the priest selected. * Now in the Church, that was barren once, the Cross of Christ, like a tree, * has blossomed forth, * as power and a firm support." //The rod is taken to be an archetype of the mystery; * for it blossomed and thus chose the priest from others. * And now the tree of the Cross, unto the formerly barren Church, * has blossomed forth, * as power and a firm support.

heHE.SeTinApeirogamon.notmetered = "N/M (With unceasing hymns.)"
heHE.SeTinApeirogamon.greekname = "Σὲ τὴν ἀπειρόγαμον."
heHE.SeTinApeirogamon.name = "With unceasing hymns."
heHE.SeTinApeirogamon.incipit = "With unceasing hymns…"
heHE.SeTinApeirogamon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.SeTinApeirogamon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.SeTinApeirogamon.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.SeTinApeirogamon.text = "With unceasing hymns of praise we glorify and magnify you, * the unwedded Mother of the Most High God; * by a word you gave birth to the true God inexplicably, * and you are higher than the immaculate angelic hosts."

heHE.SeTinOuranou.notmetered = "N/M"
heHE.SeTinOuranou.greekname = "Σὲ τὴν οὐρανοῦ."
heHE.SeTinOuranou.name = ""
heHE.SeTinOuranou.incipit = "…"
heHE.SeTinOuranou.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.SeTinOuranou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.SeTinOuranou.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.SeTinOuranou.text = "We, all generations, magnify you, the mediator between heaven and earth. Indeed, O Virgin, the fullness of the Godhead dwelt in you physically."

heHE.StavronCharaxasMosis.notmetered = "N/M (Moses prefigured.)"
heHE.StavronCharaxasMosis.greekname = "Σταυρὸν χαράξας Μωσῆς."
heHE.StavronCharaxasMosis.name = "Moses prefigured."
heHE.StavronCharaxasMosis.incipit = "Moses prefigured…"
heHE.StavronCharaxasMosis.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.StavronCharaxasMosis.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.StavronCharaxasMosis.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.StavronCharaxasMosis.text = "Moses prefigured the Cross. * Lifting up the rod, he stretched out his hand and split the Sea for Israel to cross on land. * Then he stretched it out again, * and the sea returned and covered Pharaoh’s chariots. * The Cross was thus portrayed as our invincible armor. * So let us sing to the Lord, * Christ our God, for He is greatly glorified." //When Moses inscribed a cross * with his rod extended, * he split the Red Sea for the host * of Israel that went on foot. * Then crosswise he once again * struck it and united it on Pharaoh’s chariots, * portraying on its surface the invincible weapon. * Therefore to Christ our God * let us all sing, for He has been glorified.

heHE.StereomaMouSotir.notmetered = "N/M ()"
heHE.StereomaMouSotir.greekname = "Στερέωμά μου Σωτήρ."
heHE.StereomaMouSotir.name = ""
heHE.StereomaMouSotir.incipit = "…"
heHE.StereomaMouSotir.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.StereomaMouSotir.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.StereomaMouSotir.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.StereomaMouSotir.text = "O Savior, You are my firm support, my refuge, and my strength. When my heart is shaken, confirm it in the fear of You; for there is no one holy, like You are God."

heHE.StereosonImonTonNoun.notmetered = "N/M (By Your word.)"
heHE.StereosonImonTonNoun.greekname = "Στερέωσον ἡμῶν τὸν νοῦν."
heHE.StereosonImonTonNoun.name = "By Your word."
heHE.StereosonImonTonNoun.incipit = "By Your word…"
heHE.StereosonImonTonNoun.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.StereosonImonTonNoun.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.StereosonImonTonNoun.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.StereosonImonTonNoun.text = "By Your word, O Lord, the heavens were established. * Now our hearts and minds, we pray You to establish, * that we may praise You, O Savior, and glorify You, * the all compassionate Redeemer and God."

heHE.SyEiToStereoma.notmetered = "N/M (Lord, You are the firm.)"
heHE.SyEiToStereoma.greekname = "Σὺ εἶ τὸ στερέωμα."
heHE.SyEiToStereoma.name = "Lord, You are the firm."
heHE.SyEiToStereoma.incipit = "Lord, You are the firm…"
heHE.SyEiToStereoma.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.SyEiToStereoma.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.SyEiToStereoma.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.SyEiToStereoma.text = "Lord, You are the firm support * of those run to You faithfully. * You are the light * of those who are darkened. * And my spirit is praising You."

heHE.SyMouIschys.notmetered = "N/M (You are my strength.)"
heHE.SyMouIschys.greekname = "Σύ μου ἰσχύς."
heHE.SyMouIschys.name = "You are my strength."
heHE.SyMouIschys.incipit = "You are my strength…"
heHE.SyMouIschys.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.SyMouIschys.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.SyMouIschys.ode = miscellanea_en_US_goadedes.misc.Ode4
heHE.SyMouIschys.text = "You are my strength. * You are my power, O Christ my Lord. * God my Savior, You are my exceeding joy. You came to us, never absent from the bosom of the Father, * and You assumed human poverty. * And so I join the Prophet Habakkuk, singing: Glory * to Your strength, O benevolent Lord my God!"

heHE.SynechomenonDexaiMe.notmetered = "N/M ()"
heHE.SynechomenonDexaiMe.greekname = "Συνεχόμενον δέξαι με."
heHE.SynechomenonDexaiMe.name = "N/M ()"
heHE.SynechomenonDexaiMe.incipit = "…"
heHE.SynechomenonDexaiMe.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.SynechomenonDexaiMe.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.SynechomenonDexaiMe.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.SynechomenonDexaiMe.text = "I am constrained by my many transgressions. O benevolent Lord, receive me as I entreat Your tender mercies, as did the Prophet, and save me."

heHE.ThalassanEpixas.notmetered = "N/M (Lord, You congealed.)"
heHE.ThalassanEpixas.greekname = "Θάλασσαν ἔπηξας."
heHE.ThalassanEpixas.name = "Lord, You congealed."
heHE.ThalassanEpixas.incipit = "Lord, You congealed…"
heHE.ThalassanEpixas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ThalassanEpixas.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ThalassanEpixas.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.ThalassanEpixas.text = "Lord, You congealed the sea and saved Your people, keeping them dry; * and then You threw into the deep prideful Pharaoh’s chariots and army. * Then to the mountain of Your holiness You brought Your people, who shouted, * “Let us sing a song of victory * to You, our God; for in battles You are powerful and strong.”"

heHE.TheouSynkatavasin.notmetered = "N/M (The fire in Babylon.)"
heHE.TheouSynkatavasin.greekname = "Θεοῦ συγκατάβασιν."
heHE.TheouSynkatavasin.name = "The fire in Babylon."
heHE.TheouSynkatavasin.incipit = "The fire in Babylon…"
heHE.TheouSynkatavasin.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TheouSynkatavasin.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TheouSynkatavasin.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.TheouSynkatavasin.text = "The fire in Babylon * withdrew in fear from the condescension of God. * For this reason the Servants * within the furnace as in a moist meadow danced * with feet exultant, and sang that immortal hymn, * saying, “O God of our fathers, blessed are You!”"

heHE.TinAgninEndoxos.notmetered = "N/M (The pure Theotokos.)"
heHE.TinAgninEndoxos.greekname = "Τὴν ἁγνὴν ἐνδόξως."
heHE.TinAgninEndoxos.name = "The pure Theotokos."
heHE.TinAgninEndoxos.incipit = "The pure Theotokos…"
heHE.TinAgninEndoxos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinAgninEndoxos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TinAgninEndoxos.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TinAgninEndoxos.text = "The pure Theotokos who in her womb * conceived the divine fire * without being burned, let us honor gloriously, O peoples, * magnifying her with never-silent hymns."

heHE.TinDeisinEkcheo.notmetered = "N/M (I pour out my supplication.)"
heHE.TinDeisinEkcheo.greekname = "Τὴν δέησιν ἐκχεῶ."
heHE.TinDeisinEkcheo.name = "I pour out my supplication."
heHE.TinDeisinEkcheo.incipit = "I pour out my supplication…"
heHE.TinDeisinEkcheo.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TinDeisinEkcheo.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TinDeisinEkcheo.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.TinDeisinEkcheo.text = "I pour out my supplication to the Lord, * and I tell Him all about my afflictions, * and that my soul has been flooded with evils, * and that my life has come close to being in hell. * Like Prophet Jonah, I entreat, * “Raise me up from corruption, O Lord my God.”"

heHE.TisTonPathonMeAchlyos.notmetered = "N/M ()"
heHE.TisTonPathonMeAchlyos.greekname = "Τῆς τῶν παθῶν με ἀχλύος."
heHE.TisTonPathonMeAchlyos.name = ""
heHE.TisTonPathonMeAchlyos.incipit = "…"
heHE.TisTonPathonMeAchlyos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TisTonPathonMeAchlyos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TisTonPathonMeAchlyos.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.TisTonPathonMeAchlyos.text = ""

heHE.ToEktinaxanti.notmetered = "N/M (He overthrew.)"
heHE.ToEktinaxanti.greekname = "Τῷ ἐκτινάξαντι."
heHE.ToEktinaxanti.name = "He overthrew."
heHE.ToEktinaxanti.incipit = "He overthrew…"
heHE.ToEktinaxanti.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToEktinaxanti.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ToEktinaxanti.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.ToEktinaxanti.text = "Christ overthrew all of Pharaoh’s army, * and He submerged it in the Sea; * and over dry ground He led the children of Israel. * To Him, yes Christ, let us sing, * for He is glorified unto the ages."

heHE.ToSyntripsantiPolemous.notmetered = "N/M ()"
heHE.ToSyntripsantiPolemous.greekname = "Τῷ συντρίψαντι πολέμους."
heHE.ToSyntripsantiPolemous.name = "To the Lord."
heHE.ToSyntripsantiPolemous.incipit = "To the Lord who crushed combatants…"
heHE.ToSyntripsantiPolemous.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.ToSyntripsantiPolemous.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.ToSyntripsantiPolemous.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.ToSyntripsantiPolemous.text = "To the Lord who crushed combatants * by His strong and upraised arm * for His people Israel, * and led them as they passed over the Red Sea, * let us sing, for He is our deliverer and God and greatly glorified."

heHE.TonAnarchonVasilea.notmetered = "N/M (The regiments.)"
heHE.TonAnarchonVasilea.greekname = "Τὸν ἄναρχον Βασιλέα."
heHE.TonAnarchonVasilea.name = "The regiments."
heHE.TonAnarchonVasilea.incipit = "The regiments…"
heHE.TonAnarchonVasilea.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonAnarchonVasilea.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonAnarchonVasilea.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonAnarchonVasilea.text = "The regiments of the Angels all shudder * at the unoriginate King of glory. * O priest and people praise and exalt Him beyond measure, * unto all the ages."

heHE.TonAoratonMorfi.notmetered = "N/M ()"
heHE.TonAoratonMorfi.greekname = "Τὸν ἀόρατον μορφῇ."
heHE.TonAoratonMorfi.name = ""
heHE.TonAoratonMorfi.incipit = "…"
heHE.TonAoratonMorfi.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonAoratonMorfi.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonAoratonMorfi.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TonAoratonMorfi.text = ""

heHE.TonChaldaionIKaminos.notmetered = "N/M (All ablaze.)"
heHE.TonChaldaionIKaminos.greekname = "Τῶν Χαλδαίων ἡ κάμινος."
heHE.TonChaldaionIKaminos.name = "All ablaze."
heHE.TonChaldaionIKaminos.incipit = "All ablaze…"
heHE.TonChaldaionIKaminos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonChaldaionIKaminos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonChaldaionIKaminos.ode = miscellanea_en_US_goadedes.misc.Ode7
heHE.TonChaldaionIKaminos.text = "All ablaze with a raging fire was the Chaldean furnace, * through which blew a dew-laden breeze at God’s almighty order. * The three holy Servants sang, * “You are the God of our fathers, and blessed are You.”"

heHE.TonEkNyktosAgnosias.notmetered = "N/M (From the night of ignorance.)"
heHE.TonEkNyktosAgnosias.greekname = "Τὸν ἐκ νυκτὸς ἀγνωσίας."
heHE.TonEkNyktosAgnosias.name = "From the night of ignorance."
heHE.TonEkNyktosAgnosias.incipit = "From the night of ignorance…"
heHE.TonEkNyktosAgnosias.melody = miscellanea_en_US_goadedes.misc.HeirmosNotMetered
heHE.TonEkNyktosAgnosias.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonEkNyktosAgnosias.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.TonEkNyktosAgnosias.text = "From the night of ignorance, I am ever wandering on the highway, where my soul might be captured. I pray You guide me to the light of the knowledge of You, O Lord, and lead me on the path of Your commandments."

heHE.TonEkPatrosProAionon.notmetered = "N/M (Before the ages begotten.)"
heHE.TonEkPatrosProAionon.greekname = "Τὸν ἐκ Πατρὸς πρὸ αἰώνων."
heHE.TonEkPatrosProAionon.name = "Before the ages begotten."
heHE.TonEkPatrosProAionon.incipit = "Before the ages begotten…"
heHE.TonEkPatrosProAionon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEkPatrosProAionon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonEkPatrosProAionon.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonEkPatrosProAionon.text = "Before the ages begotten of the Father, * the Son was later incarnate of His Mother. * Bless the Lord, priests and people; exalt Him beyond measure, unto all the ages."

heHE.TonEnOreiAgio.notmetered = "N/M (O extol Him.)"
heHE.TonEnOreiAgio.greekname = "Τὸν ἐν ὄρει ἁγίῳ."
heHE.TonEnOreiAgio.name = "O extol Him."
heHE.TonEnOreiAgio.incipit = "O extol Him…"
heHE.TonEnOreiAgio.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonEnOreiAgio.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonEnOreiAgio.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonEnOreiAgio.text = "O extol Him, who on the holy mountain * showed His glory, as Lord, * and in the bush with fire * revealed to Moses knowledge of the mystery * of the Ever-Virgin, supremely exalting Him unto all the ages."

heHE.TonFovonSouKyrie.notmetered = "N/M (O Lord, we Your servants.)"
heHE.TonFovonSouKyrie.greekname = "Τὸν φόβον σου Κύριε."
heHE.TonFovonSouKyrie.name = "O Lord, we Your servants."
heHE.TonFovonSouKyrie.incipit = "O Lord, we Your servants…"
heHE.TonFovonSouKyrie.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonFovonSouKyrie.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonFovonSouKyrie.ode = miscellanea_en_US_goadedes.misc.Ode3
heHE.TonFovonSouKyrie.text = "O Lord, we Your servants pray You plant Your fear * in our hearts, and become for us * who wholeheartedly in truth invoke * Your holy name, O Savior, strength and confirmation."

heHE.TonIonanEnToKitei.notmetered = "N/M (O Lord, You made Prophet Jonah.)"
heHE.TonIonanEnToKitei.greekname = "Τὸν Ἰωνᾶν ἐν τῷ κήτει."
heHE.TonIonanEnToKitei.name = "O Lord, You made Prophet Jonah."
heHE.TonIonanEnToKitei.incipit = "O Lord, You made Prophet Jonah…"
heHE.TonIonanEnToKitei.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonIonanEnToKitei.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonIonanEnToKitei.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.TonIonanEnToKitei.text = "O Lord, you made Prophet Jonah settle as a solitary in the whale. * You led him out then from corruption; * do likewise now rescue me, * for I am held in bondage in nets of the foe."

