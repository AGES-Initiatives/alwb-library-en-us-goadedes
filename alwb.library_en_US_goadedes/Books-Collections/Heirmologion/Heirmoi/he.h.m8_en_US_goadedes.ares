heHE.IlasthitiMoiSotir.notmetered = "N/M (Be gracious to me.)"
heHE.IlasthitiMoiSotir.greekname = "Ἱλάσθητί μοι Σωτήρ."
heHE.IlasthitiMoiSotir.name = "Be gracious to me."
heHE.IlasthitiMoiSotir.incipit = "Be gracious to me…"
heHE.IlasthitiMoiSotir.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.IlasthitiMoiSotir.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.IlasthitiMoiSotir.ode = miscellanea_en_US_goadedes.misc.Ode6
heHE.IlasthitiMoiSotir.text = "Be gracious to me, O Lord, * for many are my iniquities; * and bring me up from the depth of evils, I supplicate. * Like Jonah, I cry to You. * O my Savior, hear me, * as the God of my Deliverance."

heHE.InaTiMeAposo.notmetered = "N/M (Why reject me.)"
heHE.InaTiMeAposo.greekname = "Ἵνα τί με ἀπώσω."
heHE.InaTiMeAposo.name = "Why reject me, Your servant."
heHE.InaTiMeAposo.incipit = "Why reject me, Your servant…"
heHE.InaTiMeAposo.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.InaTiMeAposo.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.InaTiMeAposo.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.InaTiMeAposo.text = "Why reject me, Your servant, * from before Your face, O Lord the light that never sets? * Miserable sinner that I am, now this strange darkness covers me. * Savior, I implore You to bring me back to Your commandments, * which are light, and I pray You to guide my ways."

heHE.KyrieOTheosImon.notmetered = "N/M (O Lord our God.)"
heHE.KyrieOTheosImon.greekname = "Κύριε ὁ Θεὸς ἡμῶν."
heHE.KyrieOTheosImon.name = "O Lord our God, give peace."
heHE.KyrieOTheosImon.incipit = "O Lord our God, give peace…"
heHE.KyrieOTheosImon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.KyrieOTheosImon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.KyrieOTheosImon.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.KyrieOTheosImon.text = "O Lord our God, give peace to us, for You have rendered all. * O Lord our God, we pray, take possession of us. * Lord, O Lord, we do not know any other God except for You, and we name Your name."

heHE.KyriosTheotokon.notmetered = "N/M (You are the Theotokos.)"
heHE.KyriosTheotokon.greekname = "Κυρίως Θεοτόκον."
heHE.KyriosTheotokon.name = "You are the Theotokos."
heHE.KyriosTheotokon.incipit = "You are the Theotokos…"
heHE.KyriosTheotokon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.KyriosTheotokon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.KyriosTheotokon.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.KyriosTheotokon.text = "You are the Theotokos * lit’rally, O Virgin; * and we confess it, for we have been saved through you * whom we, O pure one, with choirs of angels magnify."

heHE.MegalynomenSeTinMiteraTouFotos.notmetered = "N/M (We all magnify you as the Mother of the Light.)"
heHE.MegalynomenSeTinMiteraTouFotos.greekname = "Μεγαλύνομέν σε τὴν Μητέρα τοῦ φωτός."
heHE.MegalynomenSeTinMiteraTouFotos.name = "We all magnify you as the Mother of the Light."
heHE.MegalynomenSeTinMiteraTouFotos.incipit = "We all magnify you as the Mother of the Light…"
heHE.MegalynomenSeTinMiteraTouFotos.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.MegalynomenSeTinMiteraTouFotos.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.MegalynomenSeTinMiteraTouFotos.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.MegalynomenSeTinMiteraTouFotos.text = "We all magnify you as the Mother of the Light, * and we glorify you, O Virgin Theotokos, * as the one who gave birth * to the Savior of our souls, Jesus Christ."

heHE.MegalynomenSeTinMiteraTouTheou.notmetered = "N/M (We all magnify you as the Mother of our God.)"
heHE.MegalynomenSeTinMiteraTouTheou.greekname = "Μεγαλύνομέν σε τὴν Μητέρα τοῦ Θεοῦ."
heHE.MegalynomenSeTinMiteraTouTheou.name = "We all magnify you as the Mother of our God."
heHE.MegalynomenSeTinMiteraTouTheou.incipit = "We all magnify you as the Mother of our God…"
heHE.MegalynomenSeTinMiteraTouTheou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.MegalynomenSeTinMiteraTouTheou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.MegalynomenSeTinMiteraTouTheou.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.MegalynomenSeTinMiteraTouTheou.text = "We all magnify you as the Mother of our God, * and we glorify you, O Virgin Theotokos, * as the one who gave birth to the Savior of our souls."

