heHE.AsmaAnapempsomen.name = "All you people."
heHE.AsmaAnapempsomen.incipit = "All you people…"
heHE.AsmaAnapempsomen.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.AsmaAnapempsomen.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.AsmaAnapempsomen.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.AsmaAnapempsomen.text = "All you people, come and let us sing unto our God most wonderful, * who rescued Israel and freed it from its bondage, * an anthem of victory, singing to Him and saying, * “We shall sing to You, our only Master.”"

