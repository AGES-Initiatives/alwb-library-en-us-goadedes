heHE.TonProdilothenta.notmetered = "N/M (Let us sing to Him.)"
heHE.TonProdilothenta.greekname = "Τὸν προδηλωθέντα."
heHE.TonProdilothenta.name = "Let us sing to Him."
heHE.TonProdilothenta.incipit = "Let us sing to Him…"
heHE.TonProdilothenta.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonProdilothenta.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonProdilothenta.ode = miscellanea_en_US_goadedes.misc.Ode9
heHE.TonProdilothenta.text = "Let us sing to Him who to Moses of old prefigured * in the burning bush the childbirth of ever-virgin Mary, * for the salvation of us the believers. * Let us magnify Him with unceasing hymns."

heHE.TonVasileaTonOuranon.notmetered = "N/M (O praise and bless Him.)"
heHE.TonVasileaTonOuranon.greekname = "Τὸν Βασιλέα τῶν οὐρανῶν."
heHE.TonVasileaTonOuranon.name = "O praise and bless Him."
heHE.TonVasileaTonOuranon.incipit = "O praise and bless Him…"
heHE.TonVasileaTonOuranon.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonVasileaTonOuranon.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonVasileaTonOuranon.ode = miscellanea_en_US_goadedes.misc.Ode8
heHE.TonVasileaTonOuranon.text = "O praise and bless Him * who by the armies of Angels * is extolled as King of the heavens, * supremely exalting Him unto all the ages."

heHE.TonZofonTisPsychisMou.notmetered = "N/M (You drove out.)"
heHE.TonZofonTisPsychisMou.greekname = "Τὸν ζόφον τῆς ψυχῆς μου."
heHE.TonZofonTisPsychisMou.name = "You drove out."
heHE.TonZofonTisPsychisMou.incipit = "You drove out…"
heHE.TonZofonTisPsychisMou.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.TonZofonTisPsychisMou.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.TonZofonTisPsychisMou.ode = miscellanea_en_US_goadedes.misc.Ode5
heHE.TonZofonTisPsychisMou.text = "You drove out the abyss’s * primal darkness, O Christ God, of old, as Giver of light. * Now dispel gloomy darkness * from my soul, I entreat You, * O Word, and grant me the light * of Your commands, so that rising early, I may glorify You.."

heHE.YgranDiodefsas.notmetered = "N/M (Of old when the Israelite.)"
heHE.YgranDiodefsas.greekname = "Ὑγρὰν διοδεύσας."
heHE.YgranDiodefsas.name = "Of old when the Israelite."
heHE.YgranDiodefsas.incipit = "Of old when the Israelite…"
heHE.YgranDiodefsas.melody = miscellanea_en_US_goadedes.misc.Heirmos
heHE.YgranDiodefsas.mode = miscellanea_en_US_goadedes.misc.Mode8
heHE.YgranDiodefsas.ode = miscellanea_en_US_goadedes.misc.Ode1
heHE.YgranDiodefsas.text = "Of old when the Israelite had crossed * the watery passage as if over a tract of land, * fleeing from the misery of Egypt, * he cried: “To God our Redeemer, O let us sing.”"

