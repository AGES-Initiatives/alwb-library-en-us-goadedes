A_Resource_Whose_Name = other.stgerasimosofamerica1_en_US_goadedes

hoCA.title1 = "SUPPLICATION SERVICE"
hoCA.title2 = "TO OUR DEVOUT AND GOD-BEARING FATHER"
hoCA.title3 = "GERASIMOS"
hoCA.title4 = "BISHOP OF ABYDUS"
hoCA.title5 = "AMERICA'S TEACHER OF PEACE AND LOVE"
hoCA.title6 = "Reposed in peace on June 12, 1995"
hoCA.poet = "English text by Fr. Seraphim Dedes"
hoCA.translator = ""
hoCA.Insert1 = "our devout and God-bearing father Gerasimos of America,"

hoCA.media.text = ""

hoCA.Apolytikion1.mode = miscellanea_en_US_goadedes.misc.Mode4
hoCA.Apolytikion1.greekmelody = he.a.m4_en_US_goadedes.heAU.OYpsotheisEnToStavro.greekname
hoCA.Apolytikion1.melody = he.a.m4_en_US_goadedes.heAU.OYpsotheisEnToStavro.name
hoCA.Apolytikion1.incipit = "Let us extol our all-devout…"
hoCA.Apolytikion1.text = "Let us extol and praise devout St. Gerasimos, * the son of Corinth, and the bishop of Abydus, * In modern-day America he shone with light divine * as a fine example of godly love, peace, and of wisdom, * scholarship and holiness. Let us pray to him, saying: * “Give us who need enlightenment your words * of God-inspired instruction, O Saint of God.”"

hoCA.Apolytikion2.mode = ho.ho23_en_US_goadedes.hoCA.Apolytikion2.mode
hoCA.Apolytikion2.melody = ho.ho23_en_US_goadedes.hoCA.Apolytikion2.melody
hoCA.Apolytikion2.incipit = ho.ho23_en_US_goadedes.hoCA.Apolytikion2.incipit
hoCA.Apolytikion2.text = ho.ho23_en_US_goadedes.hoCA.Apolytikion2.text

hoCA.acrostic.title = "The Canon,"
hoCA.acrostic.notice1 = "which has an acrostic in the Theotokia:"
hoCA.acrostic.text = "“Seraphim”"
hoCA.verse = prayers_en_US_goadedes.pr.verse_SaintM.text

hoCA.Ode11.ode = miscellanea_en_US_goadedes.misc.Ode1
hoCA.Ode11.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode11.greekmelody = he.h.m8_en_US_goadedes.heHE.YgranDiodefsas.greekname
hoCA.Ode11.melody = he.h.m8_en_US_goadedes.heHE.YgranDiodefsas.name
hoCA.Ode11.incipit = "O bishop Gerasimos…"
hoCA.Ode11.text = "O bishop Gerasimos, guiding light * and teacher of virtue in America, never cease * dispelling the darkness of our passions, * by your most luminous words and your prayers to God."

hoCA.Ode12.ode = miscellanea_en_US_goadedes.misc.Ode1
hoCA.Ode12.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode12.melody = he.h.m8_en_US_goadedes.heHE.YgranDiodefsas.name
hoCA.Ode12.incipit = "O blessed Gerasimos…"
hoCA.Ode12.text = "O blessed Gerasimos, God indeed * was pleased by your manner and your disciplined way of life. * With sympathy hear us who entreat you, * and who rely on your prayers to the Lord for us."

hoCA.Ode13.ode = miscellanea_en_US_goadedes.misc.Ode1
hoCA.Ode13.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode13.melody = he.h.m8_en_US_goadedes.heHE.YgranDiodefsas.name
hoCA.Ode13.incipit = "From youth you ignored…"
hoCA.Ode13.text = "From youth you ignored everything mundane * and fleeting, O father, and devoted yourself to God. * We pray you to help us be aspirants * to things eternal, O righteous Gerasimos."

hoCA.Ode1T.ode = miscellanea_en_US_goadedes.misc.Ode1
hoCA.Ode1T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode1T.melody = he.h.m8_en_US_goadedes.heHE.YgranDiodefsas.name
hoCA.Ode1T.incipit = "Seeking your assistance…"
hoCA.Ode1T.text = "Seeking your assistance, we pray with faith * O pure Theotokos, and implore you to wash away * the filth of our passions, with the help of * your Son's disciple and friend, St. Gerasimos."

hoCA.Ode31.ode = miscellanea_en_US_goadedes.misc.Ode3
hoCA.Ode31.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode31.greekmelody = he.h.m8_en_US_goadedes.heHE.OuraniasApsidos.greekname
hoCA.Ode31.melody = he.h.m8_en_US_goadedes.heHE.OuraniasApsidos.name
hoCA.Ode31.incipit = "Erudition and virtue…"
hoCA.Ode31.text = "Erudition and virtue made you a model for both * bishops and professors, O blessed father Gerasimos. * we pray you grant to all who magnify you with longing * to know what is good and beneficial for our souls."

hoCA.Ode32.ode = miscellanea_en_US_goadedes.misc.Ode3
hoCA.Ode32.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode32.melody = he.h.m8_en_US_goadedes.heHE.OuraniasApsidos.name
hoCA.Ode32.incipit = "Sympathetically hear us…"
hoCA.Ode32.text = "Sympathetically hear us who supplicate you, O Saint. * Save us from demonic deception and every harm to us, * who joyfully present our sacred hymns to extol you, * holy and illustrious bishop Gerasimos."

hoCA.Ode33.ode = miscellanea_en_US_goadedes.misc.Ode3
hoCA.Ode33.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode33.melody = he.h.m8_en_US_goadedes.heHE.OuraniasApsidos.name
hoCA.Ode33.incipit = "As you wished to converse…"
hoCA.Ode33.text = "As you wished to converse with the Lord alone through your prayers, * you ordered the time of your earthly life to accomplish this, * avoiding foolish disputes and useless banter and gossip. * Help us to avoid all this, blessed Gerasimos."

hoCA.Ode3T.ode = miscellanea_en_US_goadedes.misc.Ode3
hoCA.Ode3T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode3T.melody = he.h.m8_en_US_goadedes.heHE.OuraniasApsidos.name
hoCA.Ode3T.incipit = "Ever-virgin, all-blessed…"
hoCA.Ode3T.text = "Ever-virgin, all-blessed, most holy Mother of God, * intercede for me with your only-begotten Son and Lord, * to cleanse me from the stain of all my sins and offences, * and to wash me thoroughly from all my lawlessness."

hoCA.Troparion31.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Troparion31.melody = ""
hoCA.Troparion31.incipit = "O Spirit-filled…"
hoCA.Troparion31.text = "O Spirit-filled, * all-holy bishop Gerasimos, we entreat you * to direct us who with devotion honor your memory, * and to guide us to knowledge and salvation."

hoCA.Troparion32.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Troparion32.melody = ""
hoCA.Troparion32.incipit = ho.ho23_en_US_goadedes.hoCA.Troparion32.incipit
hoCA.Troparion32.text = ho.ho23_en_US_goadedes.hoCA.Troparion32.text

hoCA.Kathisma11.mode = miscellanea_en_US_goadedes.misc.Mode2
hoCA.Kathisma11.greekmelody = he.a.m2_en_US_goadedes.heAU.TaAnoZiton.greekname
hoCA.Kathisma11.melody = he.a.m2_en_US_goadedes.heAU.TaAnoZiton.name
hoCA.Kathisma11.incipit = "We pray, never cease…"
hoCA.Kathisma11.text = "We pray, never cease imploring Christ our King and God * to send down to us, the faithful, His abundant grace. * Blessed St. Gerasimos, * our beloved and thrice-blessed hierarch, * we extol your efforts to promote * our sanctification in America."

hoCA.Ode41.ode = miscellanea_en_US_goadedes.misc.Ode4
hoCA.Ode41.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode41.greekmelody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.greekname
hoCA.Ode41.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.name
hoCA.Ode41.incipit = "As a son of Corinthia…"
hoCA.Ode41.text = "As a son of Corinthia, * you became a resident of St. Anna's Skete * on Mt. Athos, O Gerasimos. * Free us who extol you from temptations all."

hoCA.Ode42.ode = miscellanea_en_US_goadedes.misc.Ode4
hoCA.Ode42.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode42.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.name
hoCA.Ode42.incipit = "You wholeheartedly sought…"
hoCA.Ode42.text = "You wholeheartedly sought to please * God in every way, blessed St. Gerasimos. * Intercede with Him on our behalf * to alleviate our pain and suffering."

hoCA.Ode43.ode = miscellanea_en_US_goadedes.misc.Ode4
hoCA.Ode43.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode43.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.name
hoCA.Ode43.incipit = "When you taught…"
hoCA.Ode43.text = "When you taught on the Holy Hill, * you enlightened students and seminarians. * Now we pray you, St. Gerasimos, * to dispel the darkness of our ignorance."

hoCA.Ode4T.ode = miscellanea_en_US_goadedes.misc.Ode4
hoCA.Ode4T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode4T.melody = he.h.m8_en_US_goadedes.heHE.EisakikoaKyrie.name
hoCA.Ode4T.incipit = "Redirect me towards holiness…"
hoCA.Ode4T.text = "Redirect me towards holiness, * blessed Theotokos and Virgin full of grace, * at the prayers of St. Gerasimos, * the believers' modern-day inerrant guide."

hoCA.Ode51.ode = miscellanea_en_US_goadedes.misc.Ode5
hoCA.Ode51.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode51.greekmelody = he.h.m8_en_US_goadedes.heHE.FotisonImas.greekname
hoCA.Ode51.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.name
hoCA.Ode51.incipit = "Drive away the night…"
hoCA.Ode51.text = "Drive away the night * of the passions with the brilliant light * of your earnest supplications to the Lord * for the faithful who implore you, St. Gerasimos."

hoCA.Ode52.ode = miscellanea_en_US_goadedes.misc.Ode5
hoCA.Ode52.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode52.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.name
hoCA.Ode52.incipit = "Shaken by the cares…"
hoCA.Ode52.text = "Shaken by the cares * of this life, we now invoke your prayers * to support us, O unshakable support * of the parishes in Pittsburgh, O Gerasimos."

hoCA.Ode53.ode = miscellanea_en_US_goadedes.misc.Ode5
hoCA.Ode53.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode53.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.name
hoCA.Ode53.incipit = "Since you have attained…"
hoCA.Ode53.text = "Since you have attained * heights of virtue, we rely on you * to assist us on the way that leads us up * to the mansions in the heavens, O Gerasimos."

hoCA.Ode5T.ode = miscellanea_en_US_goadedes.misc.Ode5
hoCA.Ode5T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode5T.melody = he.h.m8_en_US_goadedes.heHE.FotisonImas.name
hoCA.Ode5T.incipit = "Always pray for us…"
hoCA.Ode5T.text = "Always pray for us * and dispel the clouds of misery * with your light, all-blessed Mother of our God, * when your servants are confronted with calamity."

hoCA.Ode61.ode = miscellanea_en_US_goadedes.misc.Ode6
hoCA.Ode61.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode61.greekmelody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.greekname
hoCA.Ode61.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.name
hoCA.Ode61.incipit = "Gerasimos, by your holy way…"
hoCA.Ode61.text = "Gerasimos, by your holy way of life * and your wise and inspirational teachings, * you blessed the cities of Pittsburgh and Brookline. * You were a blameless professor of future priests. * And now you earnestly implore * the Redeemer for us now entreating you. "

hoCA.Ode62.ode = miscellanea_en_US_goadedes.misc.Ode6
hoCA.Ode62.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode62.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.name
hoCA.Ode62.incipit = "We think of you…"
hoCA.Ode62.text = "We think of you, O devout and humble Saint, * as a precious gem and peer of the fathers. * We pray you, help us, your spiritual children, * to be victorious over the enemy * and to defeat him by the might * of the life-giving Cross, O Gerasimos."

hoCA.Ode63.ode = miscellanea_en_US_goadedes.misc.Ode6
hoCA.Ode63.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode63.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.name
hoCA.Ode63.incipit = "We pray to you…"
hoCA.Ode63.text = "We pray to you, and indeed rely on you * to assist us in pursuing a peaceful, * God-pleasing life, so that we who extol you * may not be sentenced to hell and eternal fire, * O godly-minded and divine * theologian and bishop Gerasimos."

hoCA.Ode6T.ode = miscellanea_en_US_goadedes.misc.Ode6
hoCA.Ode6T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode6T.melody = he.h.m8_en_US_goadedes.heHE.TinDeisinEkcheo.name
hoCA.Ode6T.incipit = "Persistently pray…"
hoCA.Ode6T.text = "Persistently pray to Christ the King of peace, * that He send to us the peace that surpasses * all understanding, O all-holy Lady, * and that He hear us, your servants, when we invoke * your holy prayers as well as those * of our all-holy bishop Gerasimos."

hoCA.Troparion61.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Troparion61.melody = ""
hoCA.Troparion61.incipit = other.stgerasimosofamerica1_en_US_goadedes.hoCA.Troparion31.incipit
hoCA.Troparion61.text = other.stgerasimosofamerica1_en_US_goadedes.hoCA.Troparion31.text

hoCA.Troparion62.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Troparion62.melody = ""
hoCA.Troparion62.incipit = ho.ho23_en_US_goadedes.hoCA.Troparion62.incipit
hoCA.Troparion62.text = ho.ho23_en_US_goadedes.hoCA.Troparion62.text

hoCA.Kontakion.mode = miscellanea_en_US_goadedes.misc.Mode2
hoCA.Kontakion.melody = he.a.m2_en_US_goadedes.heAU.ToisTonAimatonSou.name
hoCA.Kontakion.incipit = "Let us extol that blameless hierarch…"
hoCA.Kontakion.text = "Let us extol that blameless hierarch Gerasimos, * the humble scholar and professor of New Testament. * The Holy Spirit filled his soul with many gifts of grace. * He was a spiritual father in America. * Inspired by God he blessed the faithful and supported them."

hoCA.PostGospelGlory.mode = miscellanea_en_US_goadedes.misc.Mode2
hoCA.PostGospelGlory.melody = ""
hoCA.PostGospelGlory.incipit = ho.ho03_en_US_goadedes.hoMA.PostGospelHierarch.incipit
hoCA.PostGospelGlory.text = ho.ho03_en_US_goadedes.hoMA.PostGospelHierarch.text

hoCA.PostGospelBoth.mode = miscellanea_en_US_goadedes.misc.Mode2
hoCA.PostGospelBoth.melody = ""
hoCA.PostGospelBoth.incipit = ho.ho03_en_US_goadedes.hoMA.PostGospelTheotokos.incipit
hoCA.PostGospelBoth.text = ho.ho03_en_US_goadedes.hoMA.PostGospelTheotokos.text

hoCA.PostGospelHymn1.mode = miscellanea_en_US_goadedes.misc.Mode6
hoCA.PostGospelHymn1.greekmelody = he.a.m6_en_US_goadedes.heAU.OlinApothemenoi.greekname
hoCA.PostGospelHymn1.melody = he.a.m6_en_US_goadedes.heAU.OlinApothemenoi.name
hoCA.PostGospelHymn1.incipit = "Man of God, Gerasimos…"
hoCA.PostGospelHymn1.text = "Man of God, Gerasimos, you are a solid foundation * of the holy Church of Christ, * born in Greece and flourishing in America. * You were a virtuous and effective bishop. * You instructed seminarians. * Hear us who pray to you. * Rescue us from evils and tragedies, * all-blessed Saint, and save us from * ever being harmed by the evil one; * and we shall extol you, the torch that shines the light of God on all, * the inexhaustible reservoir of the holy love of Christ."

hoCA.Ode71.ode = miscellanea_en_US_goadedes.misc.Ode7
hoCA.Ode71.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode71.greekmelody = he.h.m8_en_US_goadedes.heHE.OiEkTisIoudaias.greekname
hoCA.Ode71.melody = he.h.m8_en_US_goadedes.heHE.OiEkTisIoudaias.name
hoCA.Ode71.incipit = "You directed your students…"
hoCA.Ode71.text = "You directed your students * in the ways of the Lord, O devout Gerasimos. * Now we with faith implore you * to guide us to divine joy * and to life that will never end, * by your entreaties for us, who gratefully extol you."

hoCA.Ode72.ode = miscellanea_en_US_goadedes.misc.Ode7
hoCA.Ode72.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode72.melody = he.h.m8_en_US_goadedes.heHE.OiEkTisIoudaias.name
hoCA.Ode72.incipit = "By your presence…"
hoCA.Ode72.text = "By your presence you blessed us * at the school of theology on the Holy Hill. * We pray that you continue * to bless us who extol you * as a saint, O Gerasimos, * now in the company of the Church's sainted bishops."

hoCA.Ode73.ode = miscellanea_en_US_goadedes.misc.Ode7
hoCA.Ode73.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode73.melody = he.h.m8_en_US_goadedes.heHE.OiEkTisIoudaias.name
hoCA.Ode73.incipit = "Quickly come to assist us…"
hoCA.Ode73.text = "Quickly come to assist us, * when our life becomes difficult, St. Gerasimos, * we fervently entreat you, * whom Christ our God has given * to the Church in America, * to intercede on behalf of all who sing your praises."

hoCA.Ode7T.ode = miscellanea_en_US_goadedes.misc.Ode7
hoCA.Ode7T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode7T.melody = he.h.m8_en_US_goadedes.heHE.OiEkTisIoudaias.name
hoCA.Ode7T.incipit = "Holy Mother of Christ God…"
hoCA.Ode7T.text = "Holy Mother of Christ God, * I implore you to be my Directress, pointing me * towards celestial mansions, * all pure and blessed Virgin, * as the sure and inerrant guide * for humankind to sincere repentance and salvation."

hoCA.Ode81.ode = miscellanea_en_US_goadedes.misc.Ode8
hoCA.Ode81.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode81.greekmelody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.greekname
hoCA.Ode81.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode81.incipit = "O holy father…"
hoCA.Ode81.text = "O holy father, dispel the darkness of sorrow * and offences from us who extol you, * by your supplications to God that shine like sunlight."

hoCA.Ode82.ode = miscellanea_en_US_goadedes.misc.Ode8
hoCA.Ode82.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode82.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode82.incipit = "Be not dismissive…"
hoCA.Ode82.text = "Be not dismissive of our entreaties, O blessed * St. Gerasimos, rather present them * to the Lord our God, as a hierarch and high priest. "

hoCA.Ode83.ode = miscellanea_en_US_goadedes.misc.Ode8
hoCA.Ode83.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode83.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode83.incipit = "From heaven guard us…"
hoCA.Ode83.text = "From heaven guard us, who seek the grace you were given, * O Gerasimos, as a successor * of the Lord's Apostles in doctrine and demeanor."

hoCA.Ode8T.ode = miscellanea_en_US_goadedes.misc.Ode8
hoCA.Ode8T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode8T.melody = he.h.m8_en_US_goadedes.heHE.TonVasileaTonOuranon.name
hoCA.Ode8T.incipit = "I pray you, Lady…"
hoCA.Ode8T.text = "I pray you, Lady, show me the way to repentance. * Give me strength to resist the deceiver, * O all-blessed Virgin, most holy Theotokos."

hoCA.Ode91.ode = miscellanea_en_US_goadedes.misc.Ode9
hoCA.Ode91.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode91.greekmelody = he.h.m8_en_US_goadedes.heHE.KyriosTheotokon.greekname
hoCA.Ode91.melody = he.h.m8_en_US_goadedes.heHE.KyriosTheotokon.name
hoCA.Ode91.incipit = "We prayerfully extol you…"
hoCA.Ode91.text = "We prayerfully extol you * as our intercessor * with our Creator and God, O Gerasimos, * and as a vigilant guard of the holy Church of Christ."

hoCA.Ode92.ode = miscellanea_en_US_goadedes.misc.Ode9
hoCA.Ode92.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode92.melody = he.h.m8_en_US_goadedes.heHE.KyriosTheotokon.name
hoCA.Ode92.incipit = "Remember us, O father…"
hoCA.Ode92.text = "Remember us, O father, * as we now extol you, * the sturdy pillar of faith and enlightener * of theological students, O St. Gerasimos."

hoCA.Ode93.ode = miscellanea_en_US_goadedes.misc.Ode9
hoCA.Ode93.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode93.melody = he.h.m8_en_US_goadedes.heHE.KyriosTheotokon.name
hoCA.Ode93.incipit = "O godly father…"
hoCA.Ode93.text = "O godly father, bless us, * who with faith implore you * to ask the Lord to release us who honor you * from our inveterate passions, O St. Gerasimos."

hoCA.Ode9T.ode = miscellanea_en_US_goadedes.misc.Ode9
hoCA.Ode9T.mode = miscellanea_en_US_goadedes.misc.Mode8
hoCA.Ode9T.melody = he.h.m8_en_US_goadedes.heHE.KyriosTheotokon.name
hoCA.Ode9T.incipit = "Most holy Theotokos…"
hoCA.Ode9T.text = "Most holy Theotokos, * the immortal fountain * of holy joy for all people, refresh the souls * of those in pain and affliction, who sing your hymns of praise."

hoCA.Meg00.incipit = ho.ho03_en_US_goadedes.hoMA.Ode9.Meg00.incipit
