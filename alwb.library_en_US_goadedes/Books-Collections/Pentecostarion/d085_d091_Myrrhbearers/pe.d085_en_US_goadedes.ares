peMA.Ode5C1F1.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1F1.mode = miscellanea_en_US_goadedes.misc.Mode2
peMA.Ode5C1F1.greekmelody = he.h.m2_en_US_goadedes.heHE.TinAchlynTisPsychisMou.greekname
peMA.Ode5C1F1.melody = he.h.m2_en_US_goadedes.heHE.TinAchlynTisPsychisMou.name
peMA.Ode5C1F1.incipit = "We keep the memory…"
peMA.Ode5C1F1.text = "We keep the memory of the pious Myrrh-bearing Women * and all Your Disciples, as we celebrate Your Resurrection, * and we sing Your praise, O Christ."

peMA.Ode5C1F2.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1F2.mode = miscellanea_en_US_goadedes.misc.Mode2
peMA.Ode5C1F2.melody = he.h.m2_en_US_goadedes.heHE.TinAchlynTisPsychisMou.name
peMA.Ode5C1F2.incipit = "Let us worthily honor…"
peMA.Ode5C1F2.text = "Let us worthily honor Joseph of Arimathea, * for he faithfully took down from the cross the Lord's holy body, * and he laid it in a tomb."

peMA.Ode5C1Tr.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1Tr.mode = miscellanea_en_US_goadedes.misc.Mode2
peMA.Ode5C1Tr.melody = he.h.m2_en_US_goadedes.heHE.TinAchlynTisPsychisMou.name
peMA.Ode5C1Tr.incipit = "I worship You, O God…"
peMA.Ode5C1Tr.text = "I worship You, O God, the Father and the Son and the Spirit * three hypostases, and I believe the three to be united * in Your essence, being one."

peMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode2
peMA.Ode5C1T.melody = he.h.m2_en_US_goadedes.heHE.TinAchlynTisPsychisMou.name
peMA.Ode5C1T.incipit = "We believers extol you…"
peMA.Ode5C1T.text = "We believers extol you who gave birth, transcending nature, * to the One who renewed all of nature, Christ the only Master, * as the Mother of our God."

