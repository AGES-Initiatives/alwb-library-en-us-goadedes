peMA.Ode3C21.mode = miscellanea_en_US_goadedes.misc.Mode1
peMA.Ode3C21.greekmelody = he.h.m1_en_US_goadedes.heHE.DeftePomaPiomen.greekname
peMA.Ode3C21.melody = he.h.m1_en_US_goadedes.heHE.DeftePomaPiomen.name
peMA.Ode3C21.incipit = "O Virgin, today…"
peMA.Ode3C21.text = "O Virgin, today I am restored * to immortal life by the great benevolence * of Christ our God born of you. * He flashes the resurrection’s light * that shines on the universe."

peMA.Ode3C22.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C22.mode = miscellanea_en_US_goadedes.misc.Mode1
peMA.Ode3C22.melody = he.h.m1_en_US_goadedes.heHE.DeftePomaPiomen.name
peMA.Ode3C22.incipit = "Immaculate Lady…"
peMA.Ode3C22.text = "Immaculate Lady, as you gaze * on your Son, whom you bore in the flesh, and who as God * rose from the dead as He said, * celebrate. And as He is our God, * magnify Him, Virgin pure."

