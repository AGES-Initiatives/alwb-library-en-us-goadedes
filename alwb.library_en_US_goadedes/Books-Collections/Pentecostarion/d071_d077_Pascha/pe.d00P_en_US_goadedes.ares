pePA.Ode3T1.melody = he.h.m1_en_US_goadedes.heHE.DeftePomaPiomen.name
pePA.Ode3T1.incipit = "O Virgin, today…"
pePA.Ode3T1.text = "O Virgin, today I am restored * to immortal life by the great benevolence * of Christ our God born of you. * He flashes the resurrection's light * that shines on the universe."

pePA.Ode3T2.ode = miscellanea_en_US_goadedes.misc.Ode3
pePA.Ode3T2.mode = miscellanea_en_US_goadedes.misc.Mode1
pePA.Ode3T2.melody = he.h.m1_en_US_goadedes.heHE.DeftePomaPiomen.name
pePA.Ode3T2.incipit = "Immaculate Lady…"
pePA.Ode3T2.text = "Immaculate Lady, as you gaze * on your Son, whom you bore in the flesh, and who as God * rose from the dead as He said, * celebrate. And as He is our God, * magnify Him, Virgin pure."

