peMA.Ode7C1F1.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode7C1F1.melody = he.h.m3_en_US_goadedes.heHE.OTinFloga.name
peMA.Ode7C1F1.incipit = "With a word, Lord, You healed…"
peMA.Ode7C1F1.text = "With a word, Lord, You healed the Paralytic, * who had lain for many years lame and crippled. * You told him, \"Rise, and take up your pallet, and go forth, * and extol the wonderful works of God.\""

peMA.Ode7C1F2.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C1F2.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode7C1F2.melody = he.h.m3_en_US_goadedes.heHE.OTinFloga.name
peMA.Ode7C1F2.incipit = "O Commander of the Lord's Angels…"
peMA.Ode7C1F2.text = "O Commander of the Lord's Angels, Michael, * we the faithful assemble in your temple * to worship God; and we pray you guide and protect us * from every harm, by interceding for us."

peMA.Ode7C1Tr.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C1Tr.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode7C1Tr.melody = he.h.m3_en_US_goadedes.heHE.OTinFloga.name
peMA.Ode7C1Tr.incipit = "O Trinity, we the faithful…"
peMA.Ode7C1Tr.text = "O Trinity, we the faithful praise You * as the God of the universe and Master. * We pray You save us from every danger. * And may we enjoy Your blessings, O Lord."

peMA.Ode7C1T.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C1T.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode7C1T.melody = he.h.m3_en_US_goadedes.heHE.OTinFloga.name
peMA.Ode7C1T.incipit = "You gave birth inffably…"
peMA.Ode7C1T.text = "You gave birth inffably, O Virgin, * to the Son begotten before all ages, * without change, from the unoriginate Father. * That is why, O pure one, we call you blessed."

