peMA.Ode1C1F1.ode = miscellanea_en_US_goadedes.misc.Ode1
peMA.Ode1C1F1.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode1C1F1.melody = he.h.m3_en_US_goadedes.heHE.ThavmastosEndoxos.name
peMA.Ode1C1F1.incipit = "At the pool…"
peMA.Ode1C1F1.text = "At the pool by the Sheep Gate, whenever the Angel would come down * and trouble the water, it would heal only one person. * But now through holy baptism, many multitudes are purified by Christ."

peMA.Ode1C1F2.ode = miscellanea_en_US_goadedes.misc.Ode1
peMA.Ode1C1F2.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode1C1F2.melody = he.h.m3_en_US_goadedes.heHE.ThavmastosEndoxos.name
peMA.Ode1C1F2.incipit = "Leader of the Angels…"
peMA.Ode1C1F2.text = "Leader of the Angels, Commander of the ministers on high, * as we have congregated in your all-holy temple * to extol God, we pray you to guard us keep us safe from every assault."

peMA.Ode1C1Tr.ode = miscellanea_en_US_goadedes.misc.Ode1
peMA.Ode1C1Tr.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode1C1Tr.melody = he.h.m3_en_US_goadedes.heHE.ThavmastosEndoxos.name
peMA.Ode1C1Tr.incipit = "Let us join the Bodiless…"
peMA.Ode1C1Tr.text = "Let us join the Bodiless hosts and ceaselessly extol * unoriginate God, in three persons, one in nature, * Father and Logos and Spirit, having undivided the kingdom and rule."

peMA.Ode1C1T.ode = miscellanea_en_US_goadedes.misc.Ode1
peMA.Ode1C1T.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode1C1T.melody = he.h.m3_en_US_goadedes.heHE.ThavmastosEndoxos.name
peMA.Ode1C1T.incipit = "O all-holy Virgin…"
peMA.Ode1C1T.text = "O all-holy Virgin, this city always honors you with faith. * We pray that you protect us from foreign occupation, * and from civil unrest and from deadly violence and every other threat."

peMA.Ode3C1H.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1H.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C1H.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.melody
peMA.Ode3C1H.incipit = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.incipit
peMA.Ode3C1H.text = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.text

peMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C11.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C11.incipit = "The sun beheld You…"
peMA.Ode3C11.text = "The sun beheld You crucified, O Lord and Logos of God, * and it withheld its light that day, and earth entirely quaked in fear. * The dead were resurrected, when You yourself had died, O almighty One."

peMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C12.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C12.incipit = "…"
peMA.Ode3C12.text = ""

peMA.Ode3C1F1.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1F1.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C1F1.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C1F1.incipit = "For many years…"
peMA.Ode3C1F1.text = "For many years, my soul has been sickly and weak and infirm. * O Savior, most benevolent, make me whole, like the Paralytic of old, * so that I may walk in the ways that You showed us all who long for You."

peMA.Ode3C1F2.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1F2.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C1F2.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C1F2.incipit = "Together with the hosts…"
peMA.Ode3C1F2.text = "Together with the hosts on high, O Archangel of the Lord, * we pray you to entreat Him on behalf of us who extol you with faith, * as you guard us and preserve us when we stumble in the passions of life."

peMA.Ode3C1Tr.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1Tr.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C1Tr.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C1Tr.incipit = "Glory to the Father…"
peMA.Ode3C1Tr.text = "\"Glory to the Father, Son, and Spirit!\" let us cry aloud. * One in nature is God, whom all the powers of the heavens glorify, * With fear they cry aloud, singing, \"Holy, Holy, Holy!\" the thrice-holy hymn."

peMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode3C1T.melody = he.h.m3_en_US_goadedes.heHE.SteiraPsychi.name
peMA.Ode3C1T.incipit = "Virgin Mother immaculate…"
peMA.Ode3C1T.text = "Virgin Mother immaculate, having conceived without seed, * incomprehensible was your childbirth, a great and awsome miracle, * revered by the Angels all, O Lady, and glorified by humanity."

