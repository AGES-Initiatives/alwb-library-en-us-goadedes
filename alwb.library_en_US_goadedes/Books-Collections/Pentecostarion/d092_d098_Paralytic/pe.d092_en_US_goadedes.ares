peMA.Ode5C1F1.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1F1.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode5C1F1.melody = he.h.m3_en_US_goadedes.heHE.FotiToAnespero.name
peMA.Ode5C1F1.incipit = "You once raised…"
peMA.Ode5C1F1.text = "You once raised the Paralytic, O Christ. * Now I implore You to restore my crippled soul, * for by transgressions it is paralyzed; * and assist me to walk in Your straight paths."

peMA.Ode5C1F2.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1F2.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode5C1F2.melody = he.h.m3_en_US_goadedes.heHE.FotiToAnespero.name
peMA.Ode5C1F2.incipit = "O Michael, Chief of Angels…"
peMA.Ode5C1F2.text = "O Michael, Chief of Angels, keep us safe; * for our congregation has assembled today * in your sacred temple to glorify * and proclaim the great things that God has done."

peMA.Ode5C1Tr.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1Tr.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode5C1Tr.melody = he.h.m3_en_US_goadedes.heHE.FotiToAnespero.name
peMA.Ode5C1Tr.incipit = "With faith we say…"
peMA.Ode5C1Tr.text = "With faith we say the Trinity is one * in honor and divided in hypostases, * united in nature eternally, * the Father, Son, and all-holy Spirit."

peMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Ode5C1T.melody = he.h.m3_en_US_goadedes.heHE.FotiToAnespero.name
peMA.Ode5C1T.incipit = "O Mother of our God…"
peMA.Ode5C1T.text = "O Mother of our God, we call you blessed. * We extol you, O Virgin, for you seedlessly * conceived, and you gave birth ineffably * to the Master of all and Creator."

