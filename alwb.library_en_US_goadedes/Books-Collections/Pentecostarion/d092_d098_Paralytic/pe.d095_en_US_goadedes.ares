peMA.Ode9C27.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C27.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C27.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C27.incipit = "…"
peMA.Ode9C27.text = ""

peMA.Ode9C2Tr.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C2Tr.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C2Tr.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C2Tr.incipit = "For lawless men…"
peMA.Ode9C2Tr.text = "For lawless men it is foreign to praise and worship * the Trinity, the Father and Son and Holy Spirit, * unoriginate God, omnipotence uncreated. * The Trinity, by all-powerful might, * created and sustains the universe."

peMA.Ode9C2T.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C2T.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C2T.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C2T.incipit = "Within your womb…"
peMA.Ode9C2T.text = "Within your womb, you contained Christ, O Virgin Mother. * He is one of the Trinity, Giver of life, and Savior. * All creation extols Him. In His presence hosts in heaven tremble. * O Lady blessed by all, implore Him for us, * entreating Him to save our souls."

peMA.Exaposteilarion1.mode = miscellanea_en_US_goadedes.misc.Mode3
peMA.Exaposteilarion1.greekmelody = he.a.m3_en_US_goadedes.heAU.OOuranonToisAstrois.greekname
peMA.Exaposteilarion1.melody = he.a.m3_en_US_goadedes.heAU.OOuranonToisAstrois.name
peMA.Exaposteilarion1.incipit = "O Lord, You have the chalice…"
peMA.Exaposteilarion1.text = "O Lord, You have the chalice * that never empties of Your gifts. * Please let me draw from Your water, * unto forgiveness of my sins, * for I am very thirsty, * only merciful compassionate Savior."

peMA.Lauds1.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Lauds1.greekmelody = he.a.m4_en_US_goadedes.heAU.OsGennaionEnMartysi.greekname
peMA.Lauds1.melody = he.a.m4_en_US_goadedes.heAU.OsGennaionEnMartysi.name
peMA.Lauds1.incipit = "When the Father’s personified…"
peMA.Lauds1.text = "When the Father’s personified * Wisdom, Power, and Radiance, * the eternal Logos, the very Son of God, * went to the Temple and there appeared * in flesh and began to teach * the ungrateful and severe * Jewish populace at the feast, * they were marveling * at the wealth of His knowledge, and they cried out, “How does this man have such learning, * since he was not taught by anyone?”"

peMA.Lauds2.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Lauds2.greekmelody = he.a.m4_en_US_goadedes.heAU.OsGennaionEnMartysi.greekname
peMA.Lauds2.melody = he.a.m4_en_US_goadedes.heAU.OsGennaionEnMartysi.name
peMA.Lauds2.incipit = "The Messiah and Lord…"
peMA.Lauds2.text = "The Messiah and Lord of all * stopped the mouths of the Jewish scribes, * and He reprimanded the Jews who did not keep * the Law, and said, “Do not judge by mere appearances, but with just * judgment. For example, I * healed a man who was paralyzed, on the Sabbath day. * Therefore I am the Lord of both the Sabbath and the Law. O unjust people, * why seek to kill Me who raised the dead?”"

peMA.Lauds3.mode = miscellanea_en_US_goadedes.misc.Mode4
