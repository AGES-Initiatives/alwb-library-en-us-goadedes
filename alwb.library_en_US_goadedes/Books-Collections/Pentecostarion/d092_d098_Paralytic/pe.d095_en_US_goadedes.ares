peMA.Ode7C1H.incipit = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.incipit
peMA.Ode7C1H.text = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.text

peMA.Ode7C11.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C11.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode7C11.greekmelody = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.greekname
peMA.Ode7C11.melody = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.name
peMA.Ode7C11.incipit = "Savior, destroying…"
peMA.Ode7C11.text = "Savior, destroying * the might of death by Your omnipotence, * You made known the path of life unto humankind; * and with gratitude we sing to You, * “In the temple of Your holy glory, You are blessed, O Lord.”"

peMA.Ode7C12.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C12.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode7C12.melody = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.name
peMA.Ode7C12.incipit = "The Jewish people…"
peMA.Ode7C12.text = "The Jewish people * saw You in flesh and did not recognize * that You were indeed the Logos and Son of God. * As for us, we sing extolling You, * “In the temple of Your holy glory, You are blessed, O Lord.”"

peMA.Ode7C1T.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode7C1T.melody = he.h.m4_en_US_goadedes.heHE.EnTiKaminoAvramiaioi.name
peMA.Ode7C1T.incipit = "Rejoice, O hallowed…"
peMA.Ode7C1T.text = "Rejoice, O hallowed * and divine tabernacle of God Most High! * Theotokos, joy was given through you to us, * who with gratitude cry out to you, * “O all-blameless Lady, among women you are blessed indeed.”"

peMA.Ode7C2H.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C2H.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C2H.greekmelody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.greekname
peMA.Ode7C2H.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.melody
peMA.Ode7C2H.incipit = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.incipit
peMA.Ode7C2H.text = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.text

peMA.Ode7C21.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C21.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C21.greekmelody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.greekname
peMA.Ode7C21.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C21.incipit = "As the Lord who gives…"
peMA.Ode7C21.text = "As the Lord who gives rest to all and the wellspring of wonders, * You were physically tired and You willingly were thirsty. * O Jesus, who promise all the living water, You sat and asked for water to drink."

peMA.Ode7C22.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C22.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C22.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C22.incipit = "You conversed with a woman…"
peMA.Ode7C22.text = "You conversed with a woman of Samaria, O Master, * pointing out the folly of the unlawful Jewish people. * The woman believed in You as truly the Son of God, but they rejected You."

peMA.Ode7C23.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C23.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C23.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C23.incipit = "You, O Savior…"
peMA.Ode7C23.text = "You, O Savior, the ever-flowing, living wellspring, promised * to give living water flowing immortality’s water * to all who with faith receive Your Holy Spirit, who from the Father alone proceeds."

peMA.Ode7C24.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C24.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C24.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C24.incipit = "With but five loaves…"
peMA.Ode7C24.text = "With but five loaves of bread You fed five thousand hungry people; * You collected the pieces that were left over, O Savior, * and fed many thousands more. * You showed Your glory thus to Your Disciples, O Lord."

peMA.Ode7C25.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C25.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C25.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C25.incipit = "As You said, he who eats…"
peMA.Ode7C25.text = "As You said, he who eats Your bread, O Lord, will live forever, * and likewise he who drinks Your blood abides in You, my Savior, * and You will abide in him, * and You, as God, will resurrect him on the final day."

peMA.Ode7C26.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C26.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C26.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C26.incipit = "Lord, You showed…"
peMA.Ode7C26.text = "Lord, You showed that Your plan for our salvation was amazing * and confirmed Your divine authority by many wonders. * You drove away illnesses, * as God, and raised the dead and gave sight to the blind."

peMA.Ode7C27.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode7C27.mode = miscellanea_en_US_goadedes.misc.Mode7
peMA.Ode7C27.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C27.incipit = "You restored…"
peMA.Ode7C27.text = "You restored those with skin disease, You healed those who were crippled, * and You cured people paralyzed and a hemorrhaging woman. * You also walked on water. * You showed Your glory thus to Your Disciples, O Lord."

peMA.Ode7C2Tr.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C2Tr.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C2Tr.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C2Tr.incipit = "Lord, we worship…"
peMA.Ode7C2Tr.text = "Lord, we worship Your holy Father who has no beginning * and the grace of the Spirit that You gave to Your Apostles * when as God You sent them out * to all the ends of the earth to preach Gospel of truth."

peMA.Ode7C2T.ode = miscellanea_en_US_goadedes.misc.Ode7
peMA.Ode7C2T.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode7C2T.melody = he.h.m8_en_US_goadedes.heHE.TonChaldaionIKaminos.name
peMA.Ode7C2T.incipit = "In your womb…"
peMA.Ode7C2T.text = "In your womb you contained the boundless and infinite Logos. * With your breasts you nursed the Maker and Feeder of the whole world. * In you arms you held the Lord who provides for us, O immaculate Mother of God."

peMA.Ode8C1H.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C1H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode8C1H.greekmelody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.greekname
peMA.Ode8C1H.melody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.melody
peMA.Ode8C1H.incipit = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.incipit
peMA.Ode8C1H.text = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.text

peMA.Ode8C11.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C11.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode8C11.greekmelody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.greekname
peMA.Ode8C11.melody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.name
peMA.Ode8C11.incipit = "You rose from the sepulcher…"
peMA.Ode8C11.text = "You rose from the sepulcher, O Lord, with the Divinity’s glory adorning You, * and You, O Master, were beautiful. * You appeared to Your Apostles and You promised them that You would send * the Spirit’s power to those who cry out, * “Bless the Lord and praise Him, all you works of the Lord.”"

peMA.Ode8C12.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C12.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode8C12.melody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.name
peMA.Ode8C12.incipit = "As God You put death…"
peMA.Ode8C12.text = "As God You put death and hell to death, * and as the Source of life, You poured out unto all * eternal life, which the grace and charm * of the present and most splendid days we celebrate from year to year * depict distinctly to those who cry out, * “Bless the Lord and praise Him, all you works of the Lord.”"

peMA.Ode8C13.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C13.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode8C13.melody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.name
peMA.Ode8C13.incipit = "O Christ, when You came…"
peMA.Ode8C13.text = "O Christ, when You came into the world as Sun of Righteousness, You sent Your Apostles out, * as if Your beams; and they carried You, * Light that darkness cannot overcome. * They drove away the mist of dark, * spiritual ignorance, and they cried out, * “Bless the Lord and praise Him, all you works of the Lord.”"

peMA.Ode8C1T.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode8C1T.melody = he.h.m4_en_US_goadedes.heHE.CheirasEkpetasasDaniil.name
peMA.Ode8C1T.incipit = "A scepter and ruler…"
peMA.Ode8C1T.text = "A scepter and ruler from the tribe of Judah clearly have departed. And behold, * all-blameless Virgin, the Son you bore * is the One who was in store for them, * the expectation of the Gentiles, Christ, to whom we sing extolling, * “Bless the Lord and praise Him, all you works of the Lord.”"

peMA.Ode8C2H.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C2H.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C2H.greekmelody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.greekname
peMA.Ode8C2H.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.melody
peMA.Ode8C2H.incipit = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.incipit
peMA.Ode8C2H.text = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.text

peMA.Ode8C21.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C21.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C21.greekmelody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.greekname
peMA.Ode8C21.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C21.incipit = "All you peoples…"
peMA.Ode8C21.text = "All you peoples, come and see Him, who is praised as King upon His glorious throne, * being blasphemed by people who transgress the Law. * And seeing this, extol Him, whom the Prophets had foretold was the Messiah."

peMA.Ode8C22.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C22.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C22.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C22.incipit = "You are Christ indeed…"
peMA.Ode8C22.text = "You are Christ indeed, the One who from on high descended and came into this world. * Salvation and forgiveness of ancestral sins * come from You, our Redeemer. * You are life indeed for all those who have believed You."

peMA.Ode8C23.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C23.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C23.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C23.incipit = "About the middle of the feast…"
peMA.Ode8C23.text = "About the middle of the feast, as it is written, the very Wisdom of God * went up into the Temple; and He stood and taught * that He himself is truly * the Messiah, the Christ, through whom there is salvation."

peMA.Ode8C24.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C24.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C24.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C24.incipit = "Christ would demonstrate…"
peMA.Ode8C24.text = "Christ would demonstrate His signs * each and every day, including the Sabbath days. * He would miraculously cure those suffering * from various diseases. * But the wayward people were angry and indignant."

peMA.Ode8C25.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C25.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C25.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C25.incipit = "On the Sabbath day…"
peMA.Ode8C25.text = "“On the Sabbath day he healed * someone who lay paralyzed for many years, * but in so doing he transgressed the Law of God.” * So said the Jewish leaders * about Christ, as they bitterly criticized Him."

peMA.Ode8C26.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C26.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C26.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C26.incipit = "Was it not Moses…"
peMA.Ode8C26.text = "“Was it not Moses himself, * who gave the Law, who orders you to circumcise? * And even on the Sabbath one is circumcised, * that the law of the fathers * not be broken.” So said Christ to the Jewish leaders."

peMA.Ode8C27.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C27.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C27.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C27.incipit = "So ungrateful all the time…"
peMA.Ode8C27.text = "So ungrateful all the time! * Long ago as sojourners in the wilderness, * they begrudged their Benefactor. And now consumed * with envy, they blaspheme Him * and wag unrighteous tongues and meditate on vain things."

peMA.Ode8C2Tr.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C2Tr.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C2Tr.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C2Tr.incipit = "The Trinity is one God…"
peMA.Ode8C2Tr.text = "The Trinity is one God, without the Father abdicating to be the Son, * nor the Son being converted to procession, * but to each their distinctness. * I glorify the three, light and God, unto the ages."

peMA.Ode8C2T.ode = miscellanea_en_US_goadedes.misc.Ode8
peMA.Ode8C2T.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode8C2T.melody = he.h.m8_en_US_goadedes.heHE.AngeloiKaiOuranoi.name
peMA.Ode8C2T.incipit = "Tell us: How did you give birth…"
peMA.Ode8C2T.text = "Tell us: How did you give birth to Him who shone forth from the Father before all time, * and with the Holy Spirit is extolled by all? * Only He who was well pleased to be born of you can explain, O Theotokos."

peMA.Ode9C1H.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C1H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode9C1H.greekmelody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.greekname
peMA.Ode9C1H.melody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.melody
peMA.Ode9C1H.incipit = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.incipit
peMA.Ode9C1H.text = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.text

peMA.Ode9C11.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C11.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode9C11.greekmelody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.greekname
peMA.Ode9C11.melody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.name
peMA.Ode9C11.incipit = "We have learned…"
peMA.Ode9C11.text = "We have learned from Christ our Teacher a new and holy way of living. * Let us be especially zealous to persevere and preserve it to the end, * so that we may experience the Holy Spirit when He comes to us."

peMA.Ode9C12.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C12.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode9C12.melody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.name
peMA.Ode9C12.incipit = "Life-giver, assuming …"
peMA.Ode9C12.text = "Life-giver, assuming my mortal nature, You rendered it immortal, * and by grace You made it imperishable, O Savior, and raised it with yourself; * ending the age old enmity, unto the Father You presented it."

peMA.Ode9C13.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C13.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode9C13.melody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.name
peMA.Ode9C13.incipit = "We have once again…"
peMA.Ode9C13.text = "We have once again been invited to lead a life worthy of heaven, * by the mediation of Him who emptied himself and assumed the form of a slave, * and who also exalted us. Let us now magnify Him worthily."

peMA.Ode9C1T.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C1T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode9C1T.melody = he.h.m4_en_US_goadedes.heHE.LithosAcheirotmitos.name
peMA.Ode9C1T.incipit = "Root, source, and cause…"
peMA.Ode9C1T.text = "Root, source, and cause of incorruption are You, O Virgin; and we faithful * honor you by aptly employing these words and images when extolling you. * For you poured out to all of us the hypostatic immortality."

peMA.Ode9C2H.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C2H.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C2H.greekmelody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.greekname
peMA.Ode9C2H.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.melody
peMA.Ode9C2H.incipit = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.incipit
peMA.Ode9C2H.text = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.text

peMA.Ode9C21.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C21.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C21.greekmelody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.greekname
peMA.Ode9C21.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C21.incipit = "About the middle…"
peMA.Ode9C21.text = "About the middle of the Jews’ feast of Tabernacles, * O Savior, You went up into Your holy Temple. * As You taught all the people, the Jews marveled at it, saying, * “This man has never studied, so how * is it that he has learning?” they asked."

peMA.Ode9C22.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C22.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C22.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C22.incipit = "As my Redeemer…"
peMA.Ode9C22.text = "As my Redeemer was performing great signs and wonders * and pouring out divine gifts, many cures and healings, * and was curing the people, relieving many of diseases, * the Jewish leaders became furious * about His many miracles."

peMA.Ode9C23.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C23.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C23.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C23.incipit = "When my Redeemer…"
peMA.Ode9C23.text = "When my Redeemer refuted the Jewish leaders, * who were disobedient, He answered them, saying, * “Do not judge by appearances, but judge with right judgment. * The Law commands that on the Sabbath day * a man ought to be circumcised.”"

peMA.Ode9C24.ode = miscellanea_en_US_goadedes.misc.Ode9
peMA.Ode9C24.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode9C24.melody = he.h.m8_en_US_goadedes.heHE.AllotrionTonMiteron.name
peMA.Ode9C24.incipit = "…"
peMA.Ode9C24.text = ""

