peMA.Ode3C13.text = ""

peMA.Ode3C1F1.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1F1.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C1F1.greekmelody = he.h.m4_en_US_goadedes.heHE.EstereothiIKardiaMouEnKyrio.greekname
peMA.Ode3C1F1.melody = he.h.m4_en_US_goadedes.heHE.EstereothiIKardiaMouEnKyrio.name
peMA.Ode3C1F1.incipit = "O life and well of immortality…"
peMA.Ode3C1F1.text = "O life and well of immortality, You, O Savior, * came and sat by the well, in Your compassion. * When the woman of Samaria asked for it, * You gave her the water of Your wisdom, and she sang Your praise"

