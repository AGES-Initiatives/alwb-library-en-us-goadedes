peMA.Ode3C1H.incipit = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.incipit
peMA.Ode3C1H.text = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.text

peMA.Ode3C11.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C11.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode3C11.greekmelody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.greekname
peMA.Ode3C11.melody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.name
peMA.Ode3C11.incipit = "Ascending to Your Father…"
peMA.Ode3C11.text = "Ascending to Your Father, O Christ, * You have exalted our humanity, * in Your ineffable love for us * and compassion, O Giver of life."

peMA.Ode3C12.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C12.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode3C12.melody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.name
peMA.Ode3C12.incipit = "The orders of the Angels…"
peMA.Ode3C12.text = "The orders of the Angels beheld * our human nature going up with You; * and they struck with astonishment, * and extolled You as the Savior of all."

peMA.Ode3C13.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C13.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode3C13.melody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.name
peMA.Ode3C13.incipit = "Angelic hosts…"
peMA.Ode3C13.text = "Angelic hosts, O Christ, were amazed, * as they beheld You being taken up * with Your body into the heavens; * and they praised Your Ascension, O Lord."

peMA.Ode3C14.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C14.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode3C14.melody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.name
peMA.Ode3C14.incipit = "Our human nature…"
peMA.Ode3C14.text = "Our human nature fell into death. * O Christ, You came and resurrected it * and by ascending exalted it, * glorifying us with You, O Lord."

peMA.Ode3C1T.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C1T.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode3C1T.melody = he.h.m5_en_US_goadedes.heHE.DynameiTouStavrouSou.name
peMA.Ode3C1T.incipit = "O Maiden pure…"
peMA.Ode3C1T.text = "O Maiden pure, unceasingly pray * to Christ, who came forth from your virgin womb, * that those who praise you as Mother of God * be delivered from the Devil’s deceit."

peMA.Ode3C2H.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C2H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C2H.greekmelody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.greekname
peMA.Ode3C2H.melody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.melody
peMA.Ode3C2H.incipit = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.incipit
peMA.Ode3C2H.text = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.text

peMA.Ode3C21.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C21.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C21.greekmelody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.greekname
peMA.Ode3C21.melody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.name
peMA.Ode3C21.incipit = "Behold, Christ the Lord…"
peMA.Ode3C21.text = "“Behold, Christ the Lord and King of glory is coming, now that He has put on * a body, which was made of earth. * Therefore lift up your heavenly gates.” * Thus did the lower bodiless orders call out to the higher ones."

peMA.Ode3C22.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C22.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C22.melody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.name
peMA.Ode3C22.incipit = "Adam was beguiled…"
peMA.Ode3C22.text = "Adam was beguiled by the serpent. O Christ, You came down to look for him, * assuming his humanity. * You found him and ascended and sat * at Your coequal Father’s right, while angels praised and exalted You."

peMA.Ode3C23.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C23.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C23.melody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.name
peMA.Ode3C23.incipit = "The feast of the glorious…"
peMA.Ode3C23.text = "The feast of the glorious Ascension of Christ the Creator of the world * is celebrated joyfully * in heaven and on earth today; * for by His will He visibly unites what used to be separate."

peMA.Ode3C2T.ode = miscellanea_en_US_goadedes.misc.Ode3
peMA.Ode3C2T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode3C2T.melody = he.h.m4_en_US_goadedes.heHE.TousSousYmnologous.name
peMA.Ode3C2T.incipit = "Lady, you gave birth…"
peMA.Ode3C2T.text = "Lady, you gave birth to the destroyer of death, yes the only deathless God. * O Virgin Mother wholly pure, * ever entreat Him earnestly * to kill the passions that will be the death of me, and to save my soul."

peMA.Ode3Kathisma1.mode = miscellanea_en_US_goadedes.misc.Mode8
peMA.Ode3Kathisma1.greekmelody = he.a.m8_en_US_goadedes.heAU.TinSofianKaiLogon.greekname
peMA.Ode3Kathisma1.melody = he.a.m8_en_US_goadedes.heAU.TinSofianKaiLogon.name
peMA.Ode3Kathisma1.incipit = "Leaving peace…"
peMA.Ode3Kathisma1.text = "Leaving peace to Your followers on the earth, * then embarking on clouds from heaven, O Lord, * You ascended and sat at the right hand of the Father, * one in essence with Him and the Holy Spirit, as You are. * For You appeared in flesh, but did not turn from what You were. * Now You are awaiting the present age’s conclusion; * then You will return to earth and will judge the entire world. * O righteous Judge of all, * spare our souls, we pray You, O God, * and forgive us, Your servants, all our sins, * for You are compassionate and merciful."

peMA.Ode4C1H.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C1H.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode4C1H.greekmelody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.greekname
peMA.Ode4C1H.melody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.melody
peMA.Ode4C1H.incipit = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.incipit
peMA.Ode4C1H.text = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.text

peMA.Ode4C11.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C11.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode4C11.greekmelody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.greekname
peMA.Ode4C11.melody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.name
peMA.Ode4C11.incipit = "You ascended in glory…"
peMA.Ode4C11.text = "You ascended in glory, O King of Angels and Lord, * in order to send to us the Comforter from the Father. * So we cry aloud: Glory be to Your Ascension, O Christ!"

peMA.Ode4C12.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C12.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode4C12.melody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.name
peMA.Ode4C12.incipit = "As the Savior…"
peMA.Ode4C12.text = "As the Savior was ascending to the Father with His flesh, * all the regiments of Angels were in wonderment of Him, * and they cried aloud: “Glory be to Your Ascension, O Christ!”"

peMA.Ode4C13.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C13.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode4C13.melody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.name
peMA.Ode4C13.incipit = "The celestial Powers…"
peMA.Ode4C13.text = "The celestial Powers called out to the orders above them, * “Lift up your gates for Christ! He is indeed of Master and King, * whom we extol together with the Father and the Spirit.”"

peMA.Ode4C1T.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C1T.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode4C1T.melody = he.h.m5_en_US_goadedes.heHE.EisakikoaTinAkoin.name
peMA.Ode4C1T.incipit = "The Virgin gave birth…"
peMA.Ode4C1T.text = "The Virgin gave birth without suffering maternal labor. * She is a mother indeed, while truly remaining virgin. * And we salute her and sing to her, “Mother of God, rejoice!”"

peMA.Ode4C2H.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C2H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode4C2H.greekmelody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.greekname
peMA.Ode4C2H.melody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.melody
peMA.Ode4C2H.incipit = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.incipit
peMA.Ode4C2H.text = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.text

peMA.Ode4C21.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C21.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode4C21.greekmelody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.greekname
peMA.Ode4C21.melody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.name
peMA.Ode4C21.incipit = "The life-giving Savior…"
peMA.Ode4C21.text = "The life-giving Savior Jesus * took His friends, whom He loved, with Him * up the Mount of Olives, * and He blessed them, lifting up both His hands. * And then He rode on a cloud and reached the bosom of * God the Father, from which He was not absent at any time."

peMA.Ode4C22.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C22.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode4C22.melody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.name
peMA.Ode4C22.incipit = "Celebrating…"
peMA.Ode4C22.text = "Celebrating is the whole world, * visible and invisible. * Everywhere, rejoicing. * Angels all exult with and humanity, * for the Ascension of the Christ, who in His goodness took flesh, * thus uniting with us; and they unceasingly sing His praise."

peMA.Ode4C23.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C23.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode4C23.melody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.name
peMA.Ode4C23.incipit = "O almighty Master…"
peMA.Ode4C23.text = "O almighty Master Jesus, * in Your love for humanity, * as You are immortal, * You have given all immortality; * for You destroyed death’s dominion and You gloriously * have ascended, with Your holy Disciples as witnesses."

peMA.Ode4C2T.ode = miscellanea_en_US_goadedes.misc.Ode4
peMA.Ode4C2T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode4C2T.melody = he.h.m4_en_US_goadedes.heHE.OKathimenosEnDoxi.name
peMA.Ode4C2T.incipit = "Blessed is your chosen…"
peMA.Ode4C2T.text = "Blessed is your chosen belly, * for it incomprehensibly * contained Him who emptied * Hades’ belly extraordinarily. * O all-immaculate and all-blameless Virgin, pray * to Your Son and God, to save the people who sing your praise."

peMA.Ode5C1H.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1H.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode5C1H.greekmelody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.greekname
peMA.Ode5C1H.melody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.melody
peMA.Ode5C1H.incipit = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.incipit
peMA.Ode5C1H.text = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.text

peMA.Ode5C11.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C11.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode5C11.greekmelody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.greekname
peMA.Ode5C11.melody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.name
peMA.Ode5C11.incipit = "O merciful Redeemer…"
peMA.Ode5C11.text = "O merciful Redeemer, when You had filled the world with joy, * You returned, incarnate now, * to Your celestial hosts on high."

peMA.Ode5C12.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C12.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode5C12.melody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.name
peMA.Ode5C12.incipit = "O Lord, the hosts…"
peMA.Ode5C12.text = "O Lord, the hosts of Angels were watching as You ascended, * and they cried aloud and said, * “Lift up the gates for our King and God!”"

peMA.Ode5C13.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C13.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode5C13.melody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.name
peMA.Ode5C13.incipit = "The host of the Apostles…"
peMA.Ode5C13.text = "The host of the Apostles beheld the Savior ascending. * They trembled and cried aloud, * “O Christ our King, we give glory to You!”"

peMA.Ode5C1T.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C1T.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode5C1T.melody = he.h.m5_en_US_goadedes.heHE.OrthrizontesVoomenSoi.name
peMA.Ode5C1T.incipit = "We praise you…"
peMA.Ode5C1T.text = "We praise you who are virgin after childbirth, O Theotokos. * You gave birth to God the Word * incarnate, for the sake of the world."

peMA.Ode5C2H.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C2H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode5C2H.greekmelody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.greekname
peMA.Ode5C2H.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.melody
peMA.Ode5C2H.incipit = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.incipit
peMA.Ode5C2H.text = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.text

peMA.Ode5C21.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C21.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode5C21.greekmelody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.greekname
peMA.Ode5C21.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.name
peMA.Ode5C21.incipit = "Once You had put death…"
peMA.Ode5C21.text = "Once You had put death to death by Your own death, O Christ our God, * You took Your beloved friends and went up * that holy mountain, the Mt. of Olives, O Lord. * From there You ascended and returned * to Your Father in the heights, carried up on a cloud of light."

peMA.Ode5C22.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C22.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode5C22.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.name
peMA.Ode5C22.incipit = "Your Birth was unusual…"
peMA.Ode5C22.text = "Your Birth was unusual, Your Resurrection also was * unusual, likewise was Your awesome, * divine Ascension, O Lord and Giver of life. * Elijah prefigured this of old: * a chariot took him up, as he praised You who love mankind."

peMA.Ode5C23.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C23.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode5C23.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.name
peMA.Ode5C23.incipit = "Two Angels addressed…"
peMA.Ode5C23.text = "Two Angels addressed the Lord’s Apostles as they gazed at Him, * “Men of Galilee, why are you startled * by the Ascension of Christ the Giver of life? * He will likewise come again to earth * to judge the entire world, as the most righteous Judge of all.”"

peMA.Ode5C2T.ode = miscellanea_en_US_goadedes.misc.Ode5
peMA.Ode5C2T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode5C2T.melody = he.h.m4_en_US_goadedes.heHE.ExestiTaSympanta.name
peMA.Ode5C2T.incipit = "Now Christ is ascending…"
peMA.Ode5C2T.text = "Now Christ is ascending to His Father whom He never left, * even when from you, O Theotokos, * He was incarnate with soul and intellect, * and kept your virginity intact, * even after childbirth, in His mercy ineffable."

peMA.Ode6C1H.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C1H.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode6C1H.greekmelody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.greekname
peMA.Ode6C1H.melody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.melody
peMA.Ode6C1H.incipit = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.incipit
peMA.Ode6C1H.text = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.text

peMA.Ode6C11.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C11.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode6C11.greekmelody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.greekname
peMA.Ode6C11.melody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.name
peMA.Ode6C11.incipit = "The host of the Apostles…"
peMA.Ode6C11.text = "The host of the Apostles watched as Christ the Creator was taken up, * exultant with hope about the Spirit He promised them. * They sang with fear and love, “Glory to Your ascent, O Lord!”"

peMA.Ode6C12.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C12.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode6C12.melody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.name
peMA.Ode6C12.incipit = "O Christ, Your angel messengers…"
peMA.Ode6C12.text = "O Christ, Your angel messengers appeared to Your Disciples and said to them, * “This Jesus, who was taken up will come in the same way * as you saw him go, in the flesh as the just Judge of all.”"

peMA.Ode6C13.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C13.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode6C13.melody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.name
peMA.Ode6C13.incipit = "O Savior, when the Powers…"
peMA.Ode6C13.text = "O Savior, when the Powers of the heavens witnessed Your bodily * ascent into heaven, they were awestruck and cried aloud, * “O Master, great indeed is Your love for humanity!”"

peMA.Ode6C1T.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C1T.mode = miscellanea_en_US_goadedes.misc.Mode5
peMA.Ode6C1T.melody = he.h.m5_en_US_goadedes.heHE.EkykloseMeAvyssos.name
peMA.Ode6C1T.incipit = "We glorify you…"
peMA.Ode6C1T.text = "We glorify you worthily, O glorious Mary, and call you gate * of heaven, and mountain, ladder, bush that was not consumed; * and for the Orthodox, Lady, you are pride and joy."

peMA.Ode6C2H.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C2H.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode6C2H.greekmelody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.greekname
peMA.Ode6C2H.melody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.melody
peMA.Ode6C2H.incipit = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.incipit
peMA.Ode6C2H.text = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.text

peMA.Ode6C21.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C21.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode6C21.greekmelody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.greekname
peMA.Ode6C21.melody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.name
peMA.Ode6C21.incipit = "Let clouds rain down…"
peMA.Ode6C21.text = "Let clouds rain down on us from on high * and sprinkle us on earth with eternal joy, for Christ ascends today * unto His Father upon a cloud, * as if upon the chariot of the Cherubim."

peMA.Ode6C22.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C22.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode6C22.melody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.name
peMA.Ode6C22.incipit = "Appearing in the likeness…"
peMA.Ode6C22.text = "Appearing in the likeness of flesh, * You gathered into one things that formerly were separate, O Christ; * and You ascended, as Your beloved * Disciples gazed toward heaven, O tender-loving Lord."

peMA.Ode6C23.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C23.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode6C23.melody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.name
peMA.Ode6C23.incipit = "The holy Angels…"
peMA.Ode6C23.text = "The holy Angels, beholding Christ * united with our physical flesh, remarked, “Why are His garments red?” * Because He bears on His flesh the marks * of His all-holy Passion that He endured for us."

peMA.Ode6C2T.ode = miscellanea_en_US_goadedes.misc.Ode6
peMA.Ode6C2T.mode = miscellanea_en_US_goadedes.misc.Mode4
peMA.Ode6C2T.melody = he.h.m4_en_US_goadedes.heHE.TinTheianTaftin.name
peMA.Ode6C2T.incipit = "O Virgin pure…"
peMA.Ode6C2T.text = "O Virgin pure, we extol and praise * your marvelous conceiving and childbirth, through which we have been saved * from death and ruin and suffering * and from the bleak imprisonment in the netherworld."

