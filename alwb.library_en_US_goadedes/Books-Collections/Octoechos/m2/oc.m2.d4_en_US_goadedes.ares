ocMA.OdeW8C21.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW8C21.ode = misc_en_US_goadedes.Ode8
ocMA.OdeW8C21.melody = he.h.m2_en_US_goadedes.KaminosPote.name
ocMA.OdeW8C21.incipit = "Zealously pursue…"
ocMA.OdeW8C21.text = "Zealously pursue the virtues, through divine acts, * and keep your distance from all wickedness, my soul, for you have as your * intercessor the immaculate and sympathetic Mother of Christ our God, * the benevolent, kind protectress who never fails."

ocMA.OdeW8C22.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW8C22.ode = misc_en_US_goadedes.Ode8
ocMA.OdeW8C22.melody = he.h.m2_en_US_goadedes.KaminosPote.name
ocMA.OdeW8C22.incipit = "Mother of our God…"
ocMA.OdeW8C22.text = "Mother of our God, you freed our human nature * from the ancestral bond and curse. And now, O immaculate * Virgin, I implore you, free my heart from everything that binds it to wickedness, * and bind me to the love of God who created me."

ocMA.OdeW8C23.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW8C23.ode = misc_en_US_goadedes.Ode8
ocMA.OdeW8C23.melody = he.h.m2_en_US_goadedes.KaminosPote.name
ocMA.OdeW8C23.incipit = "Dismal is my heart…"
ocMA.OdeW8C23.text = "Dismal is my heart, inglorious from sinning. * O Theotokos, who gave birth to Him who is the reflection of * the Father's glory, please make it shine. Let me partake of glory that never ends, * O all-blameless and blessed Virgin immaculate."

