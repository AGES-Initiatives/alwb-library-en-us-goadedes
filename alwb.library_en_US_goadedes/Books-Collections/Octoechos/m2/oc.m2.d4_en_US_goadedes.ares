ocMA.OdeW7C21.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW7C21.ode = misc_en_US_goadedes.Ode7
ocMA.OdeW7C21.melody = he.h.m2_en_US_goadedes.AntitheonProstagma.name
ocMA.OdeW7C21.incipit = "O Lady, my praise…"
ocMA.OdeW7C21.text = "O Lady, my praise are you and my salvation, * my surest assistance and my fortress unassailable. * I earnestly pray that you will fight for me against * demons who unceasingly attack me and are always seeking to murder me."

ocMA.OdeW7C22.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW7C22.ode = misc_en_US_goadedes.Ode7
ocMA.OdeW7C22.melody = he.h.m2_en_US_goadedes.AntitheonProstagma.name
ocMA.OdeW7C22.incipit = "When God with your blood…"
ocMA.OdeW7C22.text = "When God with your blood assumed a human body, * O Virgin, through you thereby humanity was deified. * Now I, who succumbing to the passions, lie here defiled * with wounds, pray you save me by your intercessions from the plots of the enemy."

ocMA.OdeW7C23.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW7C23.ode = misc_en_US_goadedes.Ode7
ocMA.OdeW7C23.melody = he.h.m2_en_US_goadedes.AntitheonProstagma.name
ocMA.OdeW7C23.incipit = "The furnace in Babylon…"
ocMA.OdeW7C23.text = "The furnace in Babylon of old prefigured * your childbirth, O Virgin pure; for the Servants were not consumed; * neither was your womb consumed by the unbearable fire. * Therefore we implore you to deliver us, your servants, from the eternal fire."

