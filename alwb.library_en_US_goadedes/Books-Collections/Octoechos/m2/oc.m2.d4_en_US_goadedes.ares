ocMA.OdeW4C21.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW4C21.ode = misc_en_US_goadedes.Ode4
ocMA.OdeW4C21.melody = he.h.m2_en_US_goadedes.ElilythasEkParthenou.name
ocMA.OdeW4C21.incipit = "O Lady, please…"
ocMA.OdeW4C21.text = "O Lady, please, let a drop of compunction rain down on me, * which will make the scorching heat subside inside of my aching heart, * and suppress iniquitous * recollections afflicting my intellect."

ocMA.OdeW4C22.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW4C22.ode = misc_en_US_goadedes.Ode4
ocMA.OdeW4C22.melody = he.h.m2_en_US_goadedes.ElilythasEkParthenou.name
ocMA.OdeW4C22.incipit = "Immaculate, holy Virgin…"
ocMA.OdeW4C22.text = "Immaculate, holy Virgin, I pray, do not pass me by, * as you see my lying here. The knife of pleasure has wounded me. *  Heal me by the spear and blood * of Christ your Son, who is our God who was crucified."

ocMA.OdeW4C23.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW4C23.ode = misc_en_US_goadedes.Ode4
ocMA.OdeW4C23.melody = he.h.m2_en_US_goadedes.ElilythasEkParthenou.name
ocMA.OdeW4C23.incipit = "Enriched are you…"
ocMA.OdeW4C23.text = "Enriched are you, with the mastery over creation all. * O Lady, my sins has made me poor in grace divine. Pity me, * so that I may magnify * you my benevolent patroness, all-blameless one."

