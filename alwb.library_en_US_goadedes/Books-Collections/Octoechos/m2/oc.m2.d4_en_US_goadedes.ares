ocMA.OdeW5C21.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW5C21.ode = misc_en_US_goadedes.Ode5
ocMA.OdeW5C21.melody = he.h.m2_en_US_goadedes.MesitisTheou.name
ocMA.OdeW5C21.incipit = "Immaculate Maid…"
ocMA.OdeW5C21.text = "Immaculate Maid, * who gave birth to Him who is way and the life, * I pray you to guide me straight on the narrow way, for I have senselessly * hurled myself to perdition and trackless lands of tragedy."

ocMA.OdeW5C22.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW5C22.ode = misc_en_US_goadedes.Ode5
ocMA.OdeW5C22.melody = he.h.m2_en_US_goadedes.MesitisTheou.name
ocMA.OdeW5C22.incipit = "I foolishly lived…"
ocMA.OdeW5C22.text = "I foolishly lived * a licentious life, estranging myself from God, * and I have been wandering far away in passions like a foreign land. * Bring be back, holy Virgin, and save me, by your prayers for me."

ocMA.OdeW5C23.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW5C23.ode = misc_en_US_goadedes.Ode5
ocMA.OdeW5C23.melody = he.h.m2_en_US_goadedes.MesitisTheou.name
ocMA.OdeW5C23.incipit = "I pray, let me drink…"
ocMA.OdeW5C23.text = "I pray, let me drink * from refreshing streams of life that flow from you, * O Mother and Virgin pure; for the flames of sinful deeds are burning me. * I your servant am threatened, for demons are assaulting me."

