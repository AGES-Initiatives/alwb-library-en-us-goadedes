ocMA.OdeW6C21.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW6C21.ode = misc_en_US_goadedes.Ode6
ocMA.OdeW6C21.melody = he.h.m2_en_US_goadedes.EnAvyssoPtaismaton.name
ocMA.OdeW6C21.incipit = "Let me not be…"
ocMA.OdeW6C21.text = "Let me not be the object of ridicule * by the wicked demons, O Lady, on Judgment Day, * but pray for me to Christ, the Judge * and your Son, to regard me with charity."

ocMA.OdeW6C22.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW6C22.ode = misc_en_US_goadedes.Ode6
ocMA.OdeW6C22.melody = he.h.m2_en_US_goadedes.EnAvyssoPtaismaton.name
ocMA.OdeW6C22.incipit = "I have moved You…"
ocMA.OdeW6C22.text = "I have moved You to wrath with my shameful thoughts * and my wicked deeds and transgressions and sins, O Lord. * Forgive me, by your Mother's prayers, * in Your mercy, and save me, O Christ my God."

ocMA.OdeW6C23.mode = misc_en_US_goadedes.Mode2
ocMA.OdeW6C23.ode = misc_en_US_goadedes.Ode6
ocMA.OdeW6C23.melody = he.h.m2_en_US_goadedes.EnAvyssoPtaismaton.name
ocMA.OdeW6C23.incipit = "You gave birth…"
ocMA.OdeW6C23.text = "You gave birth to the Lord of the universe, * who as God will Judge all. Alas, I am self-condemned * in many sins. O Lady, please, * from His just condemnation deliver me."

