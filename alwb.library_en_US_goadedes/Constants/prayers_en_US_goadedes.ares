pr.petCompline01.text = "Let us pray for the peace of the world."
pr.petCompline02.text = "For the pious and Orthodox Christians."
pr.petCompline03.text = "For our"
pr.petCompline04.text = ""
pr.petCompline05.text = ""
pr.petCompline06.text = "For our absent fathers, brothers and sisters."
pr.petCompline07.text = "For those who help us and those who serve us."
pr.petCompline08.text = "For those who hate us and those who love us."
pr.petCompline09.text = "For those who have asked us, the unworthy, to pray for them."
pr.petCompline10.text = "For the release of the captives."
pr.petCompline11.text = "For those sailing the seas."
pr.petCompline12.text = "For those laid up with illnesses."
pr.petCompline13.text = "Let us also pray for the abundance of the fruits of the earth."
pr.petCompline14.text = "And for all our fathers and brethren departed this life, those who lie here in peace, and the Orthodox everywhere."
pr.petCompline15.text = "Let us say for ourselves, also: Lord, have mercy; Lord, have mercy; Lord, have mercy."
