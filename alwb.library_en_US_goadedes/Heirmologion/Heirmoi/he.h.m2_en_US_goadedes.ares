AkikoaKyrieTinEndoxon.name = ""
AkikoaKyrieTinEndoxon.incipit = "…"
AkikoaKyrieTinEndoxon.mode = misc_en_US_goadedes.Mode2
AkikoaKyrieTinEndoxon.ode = misc_en_US_goadedes.Ode4
AkikoaKyrieTinEndoxon.text = ""

AkikoeKyrieFonisSou.notmetered = "N/M (When You thundered.)"
AkikoeKyrieFonisSou.name = "When You thundered."
AkikoeKyrieFonisSou.incipit = "When You thundered…"
AkikoeKyrieFonisSou.mode = misc_en_US_goadedes.Mode2
AkikoeKyrieFonisSou.ode = misc_en_US_goadedes.Ode4
AkikoeKyrieFonisSou.text = "When You thundered on the many waters, * O Lord, unto Your Son thus bearing witness, * then he whom You once designated as the voice of the one who cries out in the desert * heard Your voice; and filled with the Spirit there present, he cried aloud and said, * You are the Christ, the wisdom and the power of God."

AnarchouGennitoros.notmetered = "N/M (To us was revealed.)"
AnarchouGennitoros.name = "To us was revealed."
AnarchouGennitoros.incipit = "To us was revealed…"
AnarchouGennitoros.mode = misc_en_US_goadedes.Mode2
AnarchouGennitoros.ode = misc_en_US_goadedes.Ode9
AnarchouGennitoros.text = "To us was revealed the unoriginate Begetter's Son, * from a Virgin incarnated. He is God and Lord, * coming to those sitting in darkness to give them light, * to gather the scattered. * And therefore the all-extolled Theotokos should be magnified."

AntitheonProstagma.notmetered = "N/M (The godless decree.)"
AntitheonProstagma.name = "The godless decree."
AntitheonProstagma.incipit = "The godless decree…"
AntitheonProstagma.mode = misc_en_US_goadedes.Mode2
AntitheonProstagma.ode = misc_en_US_goadedes.Ode7
AntitheonProstagma.text = "The godless decree of the unlawful tyrant * commanded the stoking of the furnace's fiery flame. * But Christ joined His pious Servants, and He covered them * with the Spirit's dewy breeze. * He is praised and blessed and exceedingly glorious."

AporeiPasaGlossa.notmetered = "N/M (Every tongue.)"
AporeiPasaGlossa.name = "Every tongue."
AporeiPasaGlossa.incipit = "Every tongue…"
AporeiPasaGlossa.mode = misc_en_US_goadedes.Mode2
AporeiPasaGlossa.ode = misc_en_US_goadedes.Ode9
AporeiPasaGlossa.text = "Every tongue is dumbfounded to extol as is worthy; * a super-mundane mind is dizzied praising you with hymns, O Theotokos. * Nonetheless accept our faith, since you are full of goodness, * for you have seen our longing indeed inspired by God. * Lady, as the protectress of Christians, you do we magnify."

AsomenToKyrio.notmetered = "N/M (Unto the Lord.)"
AsomenToKyrio.name = "Unto the Lord."
AsomenToKyrio.incipit = "Unto the Lord…"
AsomenToKyrio.mode = misc_en_US_goadedes.Mode2
AsomenToKyrio.ode = misc_en_US_goadedes.Ode1
AsomenToKyrio.text = "Unto the Lord and Savior, * who led His chosen people * of Israel across the Sea * of old, and who therein submerged * all Pharaoh's hosts and chariots, * 

a victory song, for He is greatly glorified."

AtriptonAsynithi.notmetered = "N/M (When they had traversed.)"
AtriptonAsynithi.name = "When they had traversed."
AtriptonAsynithi.incipit = "When they had traversed…"
AtriptonAsynithi.mode = misc_en_US_goadedes.Mode2
AtriptonAsynithi.ode = misc_en_US_goadedes.Ode1
AtriptonAsynithi.text = "When they had traversed dry-shod * the sea path that was untrodden and unusual, * then chosen Israel did cry aloud: * Let us sing unto the Lord, for He is glorified."

AvyssosAmartimaton.notmetered = "N/M (As I am by sins encompassed.)"
AvyssosAmartimaton.name = "As I am by sins encompassed."
AvyssosAmartimaton.incipit = "As I am by sins encompassed…"
AvyssosAmartimaton.mode = misc_en_US_goadedes.Mode2
AvyssosAmartimaton.ode = misc_en_US_goadedes.Ode2
AvyssosAmartimaton.text = "As I am by sins encompassed, * even as Jonah the Prophet * was encircled by the lowest depth, * I pray You, as he did, O Lord my God: * Raise my life from corruption."

DefteLaoi.notmetered = "N/M (Peoples, now come.)"
DefteLaoi.name = "Peoples, now come."
DefteLaoi.incipit = "Peoples, now come…"
DefteLaoi.mode = misc_en_US_goadedes.Mode2
DefteLaoi.ode = misc_en_US_goadedes.Ode1
DefteLaoi.text = "Peoples, now come * and let us sing a song to Christ our God, * who by His prophet split the sea and through it led across * His people and possession, out of Egyptian bondage, * for He was glorified."

EchthrouZofodous.notmetered = "N/M (The Spirit's cleansing.)"
EchthrouZofodous.name = "The Spirit's cleansing."
EchthrouZofodous.incipit = "The Spirit's cleansing…"
EchthrouZofodous.mode = misc_en_US_goadedes.Mode2
EchthrouZofodous.ode = misc_en_US_goadedes.Ode5
EchthrouZofodous.text = "The Spirit's cleansing has washed away from us now * the dark and dirty enemy's poison venom. * Therefore we have landed on a new unerring * path, which leads to happiness that has no access, * save to them whom God to himself reconciled."
EchthrouZofodous1.text = "The Spirit's cleansing has washed away from us now"
EchthrouZofodous2.text = "the dark and dirty enemy's poison venom."
EchthrouZofodous3.text = "Therefore we have landed on a new unerring"
EchthrouZofodous4.text = "path, which leads to happiness that has no access,"
EchthrouZofodous5.text = "save to them whom God to himself reconciled."

EflexeReithro.notmetered = "N/M (He who retarded.)"
EflexeReithro.name = "He who retarded."
EflexeReithro.incipit = "He who retarded…"
EflexeReithro.mode = misc_en_US_goadedes.Mode2
EflexeReithro.ode = misc_en_US_goadedes.Ode7
EflexeReithro.text = "He who retarded once the towering fire * within the furnace that held the pious young men * burned the dragons' heads in the streams of the river, * and the stubborn stain of sin He has completely * washed away therein, with the dew of the Spirit."
EflexeReithro1.text = "He who retarded once the towering fire"
EflexeReithro2.text = "within the furnace that held the pious young men"
EflexeReithro3.text = "burned the dragons' heads in the streams of the river,"
EflexeReithro4.text = "and the stubborn stain of sin He has completely"
EflexeReithro5.text = "washed away therein, with the dew of the Spirit."

EfrixePaidonEvagon.notmetered = "N/M (Fed with unending fuel.)"
EfrixePaidonEvagon.name = "Fed with unending fuel."
EfrixePaidonEvagon.incipit = "Fed with unending fuel…"
EfrixePaidonEvagon.mode = misc_en_US_goadedes.Mode2
EfrixePaidonEvagon.ode = misc_en_US_goadedes.Ode8
EfrixePaidonEvagon.text = "Fed with unending fuel, the fire * that wearied not shuddered at the spotless bodies, * adorned as were the souls, * of the pious Servants and yielded to them. * When the ever-living flame had shrunken away, * they sang the hymn that henceforth was immortalized: * O bless the Lord, all you His works, and sing a hymn to Him, * and exalt Him beyond measure unto all the ages."

EikonosChrysis.notmetered = "N/M (The image of gold.)"
EikonosChrysis.name = "The image of gold."
EikonosChrysis.incipit = "The image of gold…"
EikonosChrysis.mode = misc_en_US_goadedes.Mode2
EikonosChrysis.ode = misc_en_US_goadedes.Ode7
EikonosChrysis.text = "The image of gold * was in Babylon on Dura being worshiped. * But Your three Servants showed complete disdain * for that abominable decree. * Thrown into the fiery furnace, * in the midst of it they were bedewed, * singing: Blessed are You, O God of our Fathers."

EisakikoaKyrieTinAkoin.notmetered = "N/M (I have heard.)"
EisakikoaKyrieTinAkoin.name = "I have heard the report."
EisakikoaKyrieTinAkoin.incipit = "I have heard the report…"
EisakikoaKyrieTinAkoin.mode = misc_en_US_goadedes.Mode2
EisakikoaKyrieTinAkoin.ode = misc_en_US_goadedes.Ode4
EisakikoaKyrieTinAkoin.text = "I have heard the report, O Lord, * of your eternal plan for our salvation, * and I glorified You, the only Friend of man."

EleftheraMenIKtisis.notmetered = "N/M (The created world.)"
EleftheraMenIKtisis.name = "The created world."
EleftheraMenIKtisis.incipit = "The created world…"
EleftheraMenIKtisis.mode = misc_en_US_goadedes.Mode2
EleftheraMenIKtisis.ode = misc_en_US_goadedes.Ode8
EleftheraMenIKtisis.text = "The created world exults in its liberty, * and sons of the light are they who once were darkened. * Only the champion of darkness is groaning. * Now let the formerly wretched inheritance * of the Nations bless eagerly Him who caused this."
EleftheraMenIKtisis1.text = "The created world exults in its liberty,"
EleftheraMenIKtisis2.text = "and sons of the light are they who once were darkened."
EleftheraMenIKtisis3.text = "Only the champion of darkness is groaning."
EleftheraMenIKtisis4.text = "Now let the formerly wretched inheritance"
EleftheraMenIKtisis5.text = "of the Nations bless eagerly Him who caused this."

ElilythasEkParthenou.notmetered = "N/M (You came, O Lord.)"
ElilythasEkParthenou.name = "You came, O Lord."
ElilythasEkParthenou.incipit = "You came, O Lord…"
ElilythasEkParthenou.mode = misc_en_US_goadedes.Mode2
ElilythasEkParthenou.ode = misc_en_US_goadedes.Ode4
ElilythasEkParthenou.text = "You came, O Lord, * not an elder nor angel, but You yourself, * God incarnate, being born of a pure Virgin, thus saving me * and all of humanity. * Therefore I sing to You: * Glory to Your strength, O Lord!"

EmegalynasChriste.notmetered = "N/M (You have magnified.)"
EmegalynasChriste.name = "You have magnified."
EmegalynasChriste.incipit = "You have magnified…"
EmegalynasChriste.mode = misc_en_US_goadedes.Mode2
EmegalynasChriste.ode = misc_en_US_goadedes.Ode9
EmegalynasChriste.text = "You have magnified, O Christ, * the Theotokos who bore You. * From her You donned a body * susceptible to passion like ours, O Maker; * it delivers us from the ignorance of our offenses. * All our generations call her blessed, and we magnify You."

EnAvyssoPtaismaton.notmetered = "N/M (Like a deep sea, my sins.)"
EnAvyssoPtaismaton.name = "Like a deep sea, my sins."
EnAvyssoPtaismaton.incipit = "Like a deep sea, my sins…"
EnAvyssoPtaismaton.mode = misc_en_US_goadedes.Mode2
EnAvyssoPtaismaton.ode = misc_en_US_goadedes.Ode6
EnAvyssoPtaismaton.text = "Like a deep sea, my sins have encircled me. * I invoke the deep sea of Your ineffable * and unfathomable tender love. * Raise me up from corruption, O Lord my God."

EnPetraMeTisPisteos.notmetered = "N/M (You strengthened me.)"
EnPetraMeTisPisteos.name = "You strengthened me."
EnPetraMeTisPisteos.incipit = "You strengthened me…"
EnPetraMeTisPisteos.mode = misc_en_US_goadedes.Mode2
EnPetraMeTisPisteos.ode = misc_en_US_goadedes.Ode3
EnPetraMeTisPisteos.text = "You strengthened me on the rock of faith, O Savior. * You made me smile at my enemies and deride them. * My spirit is rejoicing, as I am singing, * \"No one is holy like our God, * and no one is righteous but You, O Lord.\""

EnTiKamino.notmetered = "N/M (O Lord, Your Mother.)"
EnTiKamino.name = "O Lord, Your Mother."
EnTiKamino.incipit = "O Lord, Your Mother…"
EnTiKamino.mode = misc_en_US_goadedes.Mode2
EnTiKamino.ode = misc_en_US_goadedes.Ode8
EnTiKamino.text = "O Lord, Your Mother was prefigured * within the furnace of the three Servants in Babylon of old. * For the type delivered them from being consumed * by fire when they entered it. * Now the Virgin was manifested * to the whole wide world through You; and today * we extol and highly exalt her to the ages."

EnVythoKatestrose.notmetered = "N/M (Long ago the overwhelming Force.)"
EnVythoKatestrose.name = "Long ago the overwhelming Force."
EnVythoKatestrose.incipit = "Long ago the overwhelming Force…"
EnVythoKatestrose.mode = misc_en_US_goadedes.Mode2
EnVythoKatestrose.ode = misc_en_US_goadedes.Ode1
EnVythoKatestrose.text = "Long ago the overwhelming Force * utterly destroyed the chariots and army of Pharaoh within the Sea. * Now the Logos put on flesh * and He wiped out malignant sin and its burdens all. * He is Lord, forever glorious, and He is greatly glorified."

ExinthisenIErimos.notmetered = "N/M (The gentile church.)"
ExinthisenIErimos.name = "The gentile church."
ExinthisenIErimos.incipit = "The gentile church…"
ExinthisenIErimos.mode = misc_en_US_goadedes.Mode2
ExinthisenIErimos.ode = misc_en_US_goadedes.Ode3
ExinthisenIErimos.text = "The gentile church was formerly like the barren wilderness; * and then it blossomed as the rose *  and the lily, when You appeared on earth. * My heart is established in Your Advent, O Lord."

IEnOuranois.notmetered = "N/M ()"
IEnOuranois.name = ""
IEnOuranois.incipit = "…"
IEnOuranois.mode = misc_en_US_goadedes.Mode2
IEnOuranois.ode = misc_en_US_goadedes.Ode9
IEnOuranois.text = ""

IFoniTouLogou.notmetered = "N/M (The voice of the Logos.)"
IFoniTouLogou.name = "The voice of the Logos."
IFoniTouLogou.incipit = "The voice of the Logos…"
IFoniTouLogou.mode = misc_en_US_goadedes.Mode2
IFoniTouLogou.ode = misc_en_US_goadedes.Ode6
IFoniTouLogou.text = "The voice of the Logos, the lantern of the Light, the morning star that precedes the Sun, the Forerunner is in the desert, * crying to all the peoples, Repent and beforehand purify yourselves. * Behold, among us is the Christ, who out of corruption redeems the world."

ITonAchoritonTheon.notmetered = "N/M ()"
ITonAchoritonTheon.name = ""
ITonAchoritonTheon.incipit = "…"
ITonAchoritonTheon.mode = misc_en_US_goadedes.Mode2
ITonAchoritonTheon.ode = misc_en_US_goadedes.Ode9
ITonAchoritonTheon.text = "You have contained within your womb * God whom nothing can contain; * to the joy of the world you have given birth. * We sing praises to you, all-holy Virgin."

ITonProIliouFostira.notmetered = "N/M (Lady, from your virginal belly.)"
ITonProIliouFostira.name = "Lady, from your virginal belly."
ITonProIliouFostira.incipit = "Lady, from your virginal belly…"
ITonProIliouFostira.mode = misc_en_US_goadedes.Mode2
ITonProIliouFostira.ode = misc_en_US_goadedes.Ode9
ITonProIliouFostira.text = "Lady, from your virginal belly * you ineffably embodied God, * the luminary who arose before the sun, * and the One who came down to us * and dwelt among us bodily. * O blessed and all-holy one, * you, O Theotokos, do we magnify."

IchouRimatonDeiseos.notmetered = "N/M ()"
IchouRimatonDeiseos.name = ""
IchouRimatonDeiseos.incipit = "…"
IchouRimatonDeiseos.mode = misc_en_US_goadedes.Mode2
IchouRimatonDeiseos.ode = misc_en_US_goadedes.Ode6
IchouRimatonDeiseos.text = ""

