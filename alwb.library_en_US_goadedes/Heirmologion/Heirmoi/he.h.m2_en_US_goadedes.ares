TinAgninKaiAchranton.name = ""
TinAgninKaiAchranton.incipit = "…"
TinAgninKaiAchranton.mode = misc_en_US_goadedes.Mode2
TinAgninKaiAchranton.ode = misc_en_US_goadedes.Ode9
TinAgninKaiAchranton.text = ""

TinEkParthenou.notmetered = "N/M (When he foresaw.)"
TinEkParthenou.name = "When he foresaw."
TinEkParthenou.incipit = "When he foresaw…"
TinEkParthenou.mode = misc_en_US_goadedes.Mode2
TinEkParthenou.ode = misc_en_US_goadedes.Ode4
TinEkParthenou.text = "When he foresaw Your nativity * from the Virgin, the Prophet * crying out of old proclaimed: * I have heard Your report and I have become frightened; * for You have come to us * out of Teman and from the overshadowed * holy mountain, O my Christ."

TinMoseosOdin.notmetered = "N/M (O my soul, taking up.)"
TinMoseosOdin.name = "O my soul, taking up."
TinMoseosOdin.incipit = "O my soul, taking up…"
TinMoseosOdin.mode = misc_en_US_goadedes.Mode2
TinMoseosOdin.ode = misc_en_US_goadedes.Ode1
TinMoseosOdin.text = "O my soul, taking up * the ode of Moses, cry aloud and say: * He became for me a helper and shelter unto salvation. * He is my God and I will glorify Him."

TinParthenianSou.notmetered = "N/M (O undefiled Theotokos.)"
TinParthenianSou.name = "O undefiled Theotokos."
TinParthenianSou.incipit = "O undefiled Theotokos…"
TinParthenianSou.mode = misc_en_US_goadedes.Mode2
TinParthenianSou.ode = misc_en_US_goadedes.Ode9
TinParthenianSou.text = "O undefiled Theotokos, your virginity * was not consumed by the fire of Divinity. * So we magnify you with unceasing hymns."

TinYperfyosSarki.notmetered = "N/M ()"
TinYperfyosSarki.name = ""
TinYperfyosSarki.incipit = "…"
TinYperfyosSarki.mode = misc_en_US_goadedes.Mode2
TinYperfyosSarki.ode = misc_en_US_goadedes.Ode9
TinYperfyosSarki.text = ""

TisNyktosDielthousis.notmetered = "N/M (With the night being over.)"
TisNyktosDielthousis.name = "With the night being over."
TisNyktosDielthousis.incipit = "With the night being over…"
TisNyktosDielthousis.mode = misc_en_US_goadedes.Mode2
TisNyktosDielthousis.ode = misc_en_US_goadedes.Ode5
TisNyktosDielthousis.text = "With the night being over, * day has arrived, O Master, and the light has shone forth upon the world. * For this reason the orders * of Angels extol You, * and they glorify You, O Christ our God."

TisPisteosEnPetra.notmetered = "N/M (You strengthened me.)"
TisPisteosEnPetra.name = "You strengthened me."
TisPisteosEnPetra.incipit = "You strengthened me…"
TisPisteosEnPetra.mode = misc_en_US_goadedes.Mode2
TisPisteosEnPetra.ode = misc_en_US_goadedes.Ode3
TisPisteosEnPetra.text = "You strengthened me on the rock of faith, O Savior. * You made me smile at my enemies and deride them. * My spirit is rejoicing, as I am singing, * \"No one is holy like our God, * and no one is righteous but You, O Lord.\""

ToDogmatiToTyranniko.notmetered = "N/M ()"
ToDogmatiToTyranniko.name = ""
ToDogmatiToTyranniko.incipit = "…"
ToDogmatiToTyranniko.mode = misc_en_US_goadedes.Mode2
ToDogmatiToTyranniko.ode = misc_en_US_goadedes.Ode8
ToDogmatiToTyranniko.text = "The three righteous Servants did not obey * the impious decree of the tyrant. * And they were cast into the furnace's fire, * where they stood confessing God, * and they sang, \"Bless the Lord, all you works of the Lord.\""

ToTinAvaton.notmetered = "N/M (Let us sing to the Lord.)"
ToTinAvaton.name = "Let us sing to the Lord."
ToTinAvaton.incipit = "Let us sing to the Lord…"
ToTinAvaton.mode = misc_en_US_goadedes.Mode2
ToTinAvaton.ode = misc_en_US_goadedes.Ode1
ToTinAvaton.text = "Let us sing to the Lord, who dried up the impassable * surging waters of the Sea by His divine command, * and who also led the way * for the chosen people of Israel to cross on foot. * Come let us sing to Him, for He is greatly glorified."

TonArritoSofia.notmetered = "N/M ()"
TonArritoSofia.name = ""
TonArritoSofia.incipit = "…"
TonArritoSofia.mode = misc_en_US_goadedes.Mode2
TonArritoSofia.ode = misc_en_US_goadedes.Ode8
TonArritoSofia.text = ""

TonEkTheouTheonLogon.notmetered = "N/M (Being from God.)"
TonEkTheouTheonLogon.name = "Being from God."
TonEkTheouTheonLogon.incipit = "Being from God…"
TonEkTheouTheonLogon.mode = misc_en_US_goadedes.Mode2
TonEkTheouTheonLogon.ode = misc_en_US_goadedes.Ode9
TonEkTheouTheonLogon.text = "Being from God, God the Logos * with ineffable wisdom * came to make Adam new who fell through food * into corruption unhappily. * From an all-holy Virgin * ineffably He took on flesh for us. * We the faithful extol Him concordantly and magnify."

TonEnFonaisAngelikais.notmetered = "N/M ()"
TonEnFonaisAngelikais.name = ""
TonEnFonaisAngelikais.incipit = "…"
TonEnFonaisAngelikais.mode = misc_en_US_goadedes.Mode2
TonEnFonaisAngelikais.ode = misc_en_US_goadedes.Ode8
TonEnFonaisAngelikais.text = ""

TonEnKaminoTouPyros.notmetered = "N/M (When God went down.)"
TonEnKaminoTouPyros.name = "When God went down."
TonEnKaminoTouPyros.incipit = "When God went down…"
TonEnKaminoTouPyros.mode = misc_en_US_goadedes.Mode2
TonEnKaminoTouPyros.ode = misc_en_US_goadedes.Ode8
TonEnKaminoTouPyros.text = "When God went down into the fire * of the furnace with the holy Hebrew Servants, * He converted the flames there into dew refreshing them. * Extol Him as Lord, O you works of His, * and exalt Him beyond measure, * unto all the ages."

TonEnTiVatoMosi.notmetered = "N/M (Within the bush.)"
TonEnTiVatoMosi.name = "Within the bush."
TonEnTiVatoMosi.incipit = "Within the bush…"
TonEnTiVatoMosi.mode = misc_en_US_goadedes.Mode2
TonEnTiVatoMosi.ode = misc_en_US_goadedes.Ode8
TonEnTiVatoMosi.text = "Within the bush once, the Lord * on Mount Sinai prefigured * unto Moses the virgin Maiden's miracle of old.* Extol Him as you bless Him, * and exalt supremely, * unto all the ages."

TonGigenonTisIkouse.notmetered = "N/M (Among the earthborn.)"
TonGigenonTisIkouse.name = "Among the earthborn."
TonGigenonTisIkouse.incipit = "Among the earthborn…"
TonGigenonTisIkouse.mode = misc_en_US_goadedes.Mode2
TonGigenonTisIkouse.ode = misc_en_US_goadedes.Ode9
TonGigenonTisIkouse.text = "Among the earthborn who has ever heard or beheld a marvel such as this? * Namely, a Virgin was found to have conceived in the womb, * and she delivered the infant without a birth-pang. * Your miracle was so great! * And therefore you, O Mary, pure Mother of our God, we magnify."

TonPalaiDrosisanta.notmetered = "N/M ()"
TonPalaiDrosisanta.name = ""
TonPalaiDrosisanta.incipit = "…"
TonPalaiDrosisanta.mode = misc_en_US_goadedes.Mode2
TonPalaiDrosisanta.ode = misc_en_US_goadedes.Ode8
TonPalaiDrosisanta.text = ""

ToxonSynetriviDynaston.notmetered = "N/M ()"
ToxonSynetriviDynaston.name = ""
ToxonSynetriviDynaston.incipit = "…"
ToxonSynetriviDynaston.mode = misc_en_US_goadedes.Mode2
ToxonSynetriviDynaston.ode = misc_en_US_goadedes.Ode3
ToxonSynetriviDynaston.text = ""

VatosEnOrei.notmetered = "N/M (The burning bush.)"
VatosEnOrei.name = "The burning bush."
VatosEnOrei.incipit = "The burning bush…"
VatosEnOrei.mode = misc_en_US_goadedes.Mode2
VatosEnOrei.ode = misc_en_US_goadedes.Ode7
VatosEnOrei.text = "The burning bush that was not consumed * and the Chaldean furnace that emitted due * indeed prefigured you, O Bride of God. * You accepted in your womb, which was material, divine * immaterial fire * without being burnt. So to your Son we cry * and say, \"O God of our Fathers, You are blessed.\""

VythoAmartimaton.notmetered = "N/M (The gulf of my transgressions.)"
VythoAmartimaton.name = "The gulf of my transgressions."
VythoAmartimaton.incipit = "The gulf of my transgressions…"
VythoAmartimaton.mode = misc_en_US_goadedes.Mode2
VythoAmartimaton.ode = misc_en_US_goadedes.Ode6
VythoAmartimaton.text = "The gulf of my transgressions, O Savior, presses me, * and in the ocean of life I am foundering. * As once You extracted Jonah from the whale, * so now from the passions haul me up, * and bring me to safety."

VythouAnekalypse.notmetered = "N/M (The Lord who is powerful.)"
VythouAnekalypse.name = "The Lord who is powerful."
VythouAnekalypse.incipit = "The Lord who is powerful…"
VythouAnekalypse.mode = misc_en_US_goadedes.Mode2
VythouAnekalypse.ode = misc_en_US_goadedes.Ode1
VythouAnekalypse.text = "The Lord who is powerful in battle * once exposed the bottom of deep sea * and He drew His people through it as on dry land, * while there again He engulfed the enemies, * for He is glorified."

YmnoSeAkoi.notmetered = "N/M ()"
YmnoSeAkoi.name = ""
YmnoSeAkoi.incipit = "…"
YmnoSeAkoi.mode = misc_en_US_goadedes.Mode2
YmnoSeAkoi.ode = misc_en_US_goadedes.Ode4
YmnoSeAkoi.text = "I extol You, for I hear Your report, Lord, and I was amazed. For You have come down to me, seeking me the lost sheep. Therefore I glorify Your great condescension towards me, O very merciful Lord."

