IdeteIdeteOEnThalassi.name = ""
IdeteIdeteOEnThalassi.incipit = "…"
IdeteIdeteOEnThalassi.mode = misc_en_US_goadedes.Mode2
IdeteIdeteOEnThalassi.ode = misc_en_US_goadedes.Ode2
IdeteIdeteOEnThalassi.text = ""

IisousOZoisArchigos.notmetered = "N/M (He is Jesus.)"
IisousOZoisArchigos.name = "He is Jesus."
IisousOZoisArchigos.incipit = "He is Jesus…"
IisousOZoisArchigos.mode = misc_en_US_goadedes.Mode2
IisousOZoisArchigos.ode = misc_en_US_goadedes.Ode5
IisousOZoisArchigos.text = "He is Jesus, the Author of life. * He has come to loose the condemnation of first-created Adam. * Being God, and thus not needing to be purified, * in the Jordan He purifies the one who had fallen. * He put to death the enmity, and therefore He grants the peace that passes all understanding."

ImertonExefine.notmetered = "N/M (With the blessed utterance.)"
ImertonExefine.name = "With the blessed utterance."
ImertonExefine.incipit = "With the blessed utterance…"
ImertonExefine.mode = misc_en_US_goadedes.Mode2
ImertonExefine.ode = misc_en_US_goadedes.Ode6
ImertonExefine.text = "With the blessed utterance proclaimed the Father * Him He produced from the womb His beloved Son. * Yes, He said, this is My own connatural offspring, * splendor of My light, now also borne of mankind, * both My living Word, and by providence mortal."
ImertonExefine1.text = "With the blessed utterance proclaimed the Father"
ImertonExefine2.text = "Him He produced from the womb His beloved Son."
ImertonExefine3.text = "Yes, He said, this is My own connatural offspring,"
ImertonExefine4.text = "splendor of My light, now also borne of mankind,"
ImertonExefine5.text = "both My living Word, and by providence mortal."

IndalmatosChrysou.notmetered = "N/M ()"
IndalmatosChrysou.name = ""
IndalmatosChrysou.incipit = "…"
IndalmatosChrysou.mode = misc_en_US_goadedes.Mode2
IndalmatosChrysou.ode = misc_en_US_goadedes.Ode8
IndalmatosChrysou.text = ""

IschynODidous.notmetered = "N/M (The Lord who bestows.)"
IschynODidous.name = "The Lord who bestows."
IschynODidous.incipit = "The Lord who bestows…"
IschynODidous.mode = misc_en_US_goadedes.Mode2
IschynODidous.ode = misc_en_US_goadedes.Ode3
IschynODidous.text = "The Lord who bestows upon our rulers His might and exalts * the horn of His own anointed ones * was born of a pure virgin Maid, and now He goes to baptism. O faithful, let us cry aloud * and say that there is no one holy like our God, * and no one is righteous except for You, O Lord."

KaminosPote.notmetered = "N/M (Heeding the divine decree.)"
KaminosPote.name = "Heeding the divine decree."
KaminosPote.incipit = "Heeding the divine decree…"
KaminosPote.mode = misc_en_US_goadedes.Mode2
KaminosPote.ode = misc_en_US_goadedes.Ode8
KaminosPote.text = "Heeding the divine decree, the fiery furnace * of old in Babylon distributed its actions and energy; * for it burned up the Chaldean men, * but it refreshed the faithful young men who sang, * saying, \"Bless the Lord, all you works of the Lord. Sing a hymn to Him.\""

MesitisTheou.notmetered = "N/M (O Christ our God.)"
MesitisTheou.name = "O Christ our God."
MesitisTheou.incipit = "O Christ our God…"
MesitisTheou.mode = misc_en_US_goadedes.Mode2
MesitisTheou.ode = misc_en_US_goadedes.Ode5
MesitisTheou.text = "O Christ our God, * You are the mediator between God and men. * For through You, O Lord, we have access to Your Father, the Author of Light, * in the Spirit, as Paul said, out of the night of ignorance."

MystirionParadoxon.notmetered = "N/M (The furnace once.)"
MystirionParadoxon.name = "The furnace once."
MystirionParadoxon.incipit = "The furnace once…"
MystirionParadoxon.mode = misc_en_US_goadedes.Mode2
MystirionParadoxon.ode = misc_en_US_goadedes.Ode8
MystirionParadoxon.text = "The furnace once in Babylon displayed a paradoxical mystery by pouring out moisture: * that the Jordan was to receive in its streams the immaterial fire, * and hold the Creator when He was being baptized. * Him do the peoples all bless and exalt supremely unto all the ages."

NeousEfseveis.notmetered = "N/M (When the pious youths.)"
NeousEfseveis.name = "When the pious youths."
NeousEfseveis.incipit = "When the pious youths…"
NeousEfseveis.mode = misc_en_US_goadedes.Mode2
NeousEfseveis.ode = misc_en_US_goadedes.Ode7
NeousEfseveis.text = "When the pious youths were thrown in the furnace all aflame with fire, * the Angel of the Lord went down with them and drove out the fiery flame * and moist whistling wind preserved them free of harm. * Being thus refreshed in the flames, they gave glory, singing in gratitude. * O supremely praised and most exalted Lord and God of the fathers, You are blessed."

OAnthraxToIsaia.notmetered = "N/M ()"
OAnthraxToIsaia.name = ""
OAnthraxToIsaia.incipit = "…"
OAnthraxToIsaia.mode = misc_en_US_goadedes.Mode2
OAnthraxToIsaia.ode = misc_en_US_goadedes.Ode5
OAnthraxToIsaia.text = ""

OFotismos.notmetered = "N/M ()"
OFotismos.name = ""
OFotismos.incipit = "…"
OFotismos.mode = misc_en_US_goadedes.Mode2
OFotismos.ode = misc_en_US_goadedes.Ode5
OFotismos.text = ""

OSkiografonAchlyn.notmetered = "N/M (You dispelled.)"
OSkiografonAchlyn.name = "You dispelled the misty shadow."
OSkiografonAchlyn.incipit = "You dispelled the misty shadow…"
OSkiografonAchlyn.mode = misc_en_US_goadedes.Mode2
OSkiografonAchlyn.ode = misc_en_US_goadedes.Ode5
OSkiografonAchlyn.text = "You dispelled the misty shadow of prophetic enigmas, * and You illumined the hearts of the believers * by their true fulfillment in the all-holy Theotokos. * We entreat You to guide us, O Christ, by Your holy light."

OTinKaminonPalai.notmetered = "N/M ()"
OTinKaminonPalai.name = ""
OTinKaminonPalai.incipit = "…"
OTinKaminonPalai.mode = misc_en_US_goadedes.Mode2
OTinKaminonPalai.ode = misc_en_US_goadedes.Ode7
OTinKaminonPalai.text = ""

OTonYperNoun.notmetered = "N/M (O all spotless Bride.)"
OTonYperNoun.name = "O all spotless Bride."
OTonYperNoun.incipit = "O all spotless Bride…"
OTonYperNoun.mode = misc_en_US_goadedes.Mode2
OTonYperNoun.ode = misc_en_US_goadedes.Ode9
OTonYperNoun.text = "O all spotless Bride, surpassing comprehension * are the wonders of your childbirth, blessed Mother! * Through you obtaining comprehensive salvation, * we bring you a gift in striking up this worthy * hymn of gratitude to you our Benefactress."
OTonYperNoun1.text = "O all spotless Bride, surpassing comprehension"
OTonYperNoun2.text = "are the wonders of your childbirth, blessed Mother!"
OTonYperNoun3.text = "Through you obtaining comprehensive salvation,"
OTonYperNoun4.text = "we bring you a gift in striking up this worthy"
OTonYperNoun5.text = "hymn of gratitude to you our Benefactress."

OTouFotosChorigos.notmetered = "N/M (Lord, the provider of light.)"
OTouFotosChorigos.name = "Lord, the provider of light."
OTouFotosChorigos.incipit = "Lord, the provider of light…"
OTouFotosChorigos.mode = misc_en_US_goadedes.Mode2
OTouFotosChorigos.ode = misc_en_US_goadedes.Ode5
OTouFotosChorigos.text = "Lord, the provider of light, * and the Creator of the ages, we pray * that You will guide us in the light of Your all-holy commands. * Except for You, we do not know any other God."

OlosYparcheisEfesis.notmetered = "N/M (You are desire entirely.)"
OlosYparcheisEfesis.name = "You are desire entirely."
OlosYparcheisEfesis.incipit = "You are desire entirely…"
OlosYparcheisEfesis.mode = misc_en_US_goadedes.Mode2
OlosYparcheisEfesis.ode = misc_en_US_goadedes.Ode9
OlosYparcheisEfesis.text = "You are desire entirely, all sweetness are You, O Logos of God, the Virgin's Son, O God of gods, Lord of all, more holy than the holy ones. * We therefore maginfy You and the Maiden who gave birth to You."

OsoiPalaion.notmetered = "N/M (From the ancient snares.)"
OsoiPalaion.name = "From the ancient snares."
OsoiPalaion.incipit = "From the ancient snares…"
OsoiPalaion.mode = misc_en_US_goadedes.Mode2
OsoiPalaion.ode = misc_en_US_goadedes.Ode3
OsoiPalaion.text = "From the ancient snares of the gluttonous lions, * whose teeth are broken, we all have been delivered. * Let us exult, then, and our mouth let us widen, * weaving melody from words unto the Logos, * whose delight it is to bestow gifts upon us."
OsoiPalaion1.text = "From the ancient snares of the gluttonous lions,"
OsoiPalaion2.text = "whose teeth are broken, we all have been delivered."
OsoiPalaion3.text = "Let us exult, then, and our mouth let us widen,"
OsoiPalaion4.text = "weaving melody from words unto the Logos,"
OsoiPalaion5.text = "whose delight it is to bestow gifts upon us."

ProsKyrionEkKitous.notmetered = "N/M (The Prophet Jonah cried.)"
ProsKyrionEkKitous.name = "The Prophet Jonah cried."
ProsKyrionEkKitous.incipit = "The Prophet Jonah cried…"
ProsKyrionEkKitous.mode = misc_en_US_goadedes.Mode2
ProsKyrionEkKitous.ode = misc_en_US_goadedes.Ode6
ProsKyrionEkKitous.text = "The Prophet Jonah cried to the Lord from the sea creature, * \"I pray You, Lord my God, from the depth of Hades raise me up, * that with a voice of praise, I may sacrifice to You * in a spirit of truth, O Deliverer.\""

ProsecheteLaoi.notmetered = "N/M ()"
ProsecheteLaoi.name = ""
ProsecheteLaoi.incipit = "…"
ProsecheteLaoi.mode = misc_en_US_goadedes.Mode2
ProsecheteLaoi.ode = misc_en_US_goadedes.Ode2
ProsecheteLaoi.text = ""

ProsecheteLaosMou.notmetered = "N/M ()"
ProsecheteLaosMou.name = "N/M ()"
ProsecheteLaosMou.incipit = "N/M ()…"
ProsecheteLaosMou.mode = misc_en_US_goadedes.Mode2
ProsecheteLaosMou.ode = misc_en_US_goadedes.Ode2
ProsecheteLaosMou.text = "Take heed, O my people, to my law. Incline your ears to the words of my mouth, for I invoke the Lord's name."

PsychaisKatharais.notmetered = "N/M (With souls that are pure.)"
PsychaisKatharais.name = "With souls that are pure."
PsychaisKatharais.incipit = "With souls that are pure…"
PsychaisKatharais.mode = misc_en_US_goadedes.Mode2
PsychaisKatharais.ode = misc_en_US_goadedes.Ode9
PsychaisKatharais.text = "With souls that are pure, let us open unpolluted lips, * and come, let us magnify the all-immaculate * and most pure, all-holy Mother of Emmanuel, * as through her we offer up * intercession to Him, the Son she bore. * \"Spare our souls, O Christ our God, and save us, we pray.\""

PyrsoKathartheis.notmetered = "N/M (Once purged.)"
PyrsoKathartheis.name = "Once purged."
PyrsoKathartheis.incipit = "Once purged…"
PyrsoKathartheis.mode = misc_en_US_goadedes.Mode2
PyrsoKathartheis.ode = misc_en_US_goadedes.Ode4
PyrsoKathartheis.text = "Once purged by the fire of mystical vision, * the Prophet, hymning the renewal of mortals, * cried in a loud voice energized by the Spirit, * telling the ineffable Word's incarnation. * He it is who crushed the power of the princes."
PyrsoKathartheis1.text = "Once purged by the fire of mystical vision,"
PyrsoKathartheis2.text = "the Prophet, hymning the renewal of mortals,"
PyrsoKathartheis3.text = "cried in a loud voice energized by the Spirit,"
PyrsoKathartheis4.text = "telling the ineffable Word's incarnation."
PyrsoKathartheis5.text = "He it is who crushed the power of the princes."

RimaTyrannou.notmetered = "N/M (When the decree.)"
RimaTyrannou.name = "When the decree."
RimaTyrannou.incipit = "When the decree…"
RimaTyrannou.mode = misc_en_US_goadedes.Mode2
RimaTyrannou.ode = misc_en_US_goadedes.Ode8
RimaTyrannou.text = "When the decree of the tyrant prevailed of old, * the fiery furnace was heated to sevenfold strength. * The three Servants did not burn therein, * when they defied the king's pronouncement; * but they cried out and sang, \"All you works of the Lord, sing a hymn to the Lord, * and exalt Him beyond measure unto all the ages.\""

RitoresAnedeichthisanPaides.notmetered = "N/M ()"
RitoresAnedeichthisanPaides.name = ""
RitoresAnedeichthisanPaides.incipit = "…"
RitoresAnedeichthisanPaides.mode = misc_en_US_goadedes.Mode2
RitoresAnedeichthisanPaides.ode = misc_en_US_goadedes.Ode7
RitoresAnedeichthisanPaides.text = "Those Servants of old proved to be most wise orators, for with their lips they uttered theology from a God-inspired soul, and they sang: Blessed are You, the super-divine God of us and of our Fathers."

SeTinNoitin.notmetered = "N/M ()"
SeTinNoitin.name = ""
SeTinNoitin.incipit = "…"
SeTinNoitin.mode = misc_en_US_goadedes.Mode2
SeTinNoitin.ode = misc_en_US_goadedes.Ode9
SeTinNoitin.text = ""

SteirothentaMouTonNoun.notmetered = "N/M (Barren has my mind.)"
SteirothentaMouTonNoun.name = "Barren has my mind."
SteirothentaMouTonNoun.incipit = "Barren has my mind…"
SteirothentaMouTonNoun.mode = misc_en_US_goadedes.Mode2
SteirothentaMouTonNoun.ode = misc_en_US_goadedes.Ode3
SteirothentaMouTonNoun.text = "Barren has my mind become. * Make me fruitful, O my God, in Your compassion, * as the Husbandman of good, * and the gardener of all * that is virtuous."

StereosonImasEnSoiKyrie.notmetered = "N/M (Establish us in You.)"
StereosonImasEnSoiKyrie.name = "Establish us in You."
StereosonImasEnSoiKyrie.incipit = "Establish us in You…"
StereosonImasEnSoiKyrie.mode = misc_en_US_goadedes.Mode2
StereosonImasEnSoiKyrie.ode = misc_en_US_goadedes.Ode3
StereosonImasEnSoiKyrie.text = "Establish us in You, Lord, we pray to You, * who mortified sin by Your crucifixion, * and implant in us the fear of You * within the hearts of Your servants who sing Your praise."

SteiveiThalassis.notmetered = "N/M (Israel treaded.)"
SteiveiThalassis.name = "Israel treaded."
SteiveiThalassis.incipit = "Israel treaded…"
SteiveiThalassis.mode = misc_en_US_goadedes.Mode2
SteiveiThalassis.ode = misc_en_US_goadedes.Ode1
SteiveiThalassis.text = "Israel treaded on the sea's swelling billow, * which had been rendered once again into dry land. * Then the dark waters concealed all the Egyptian * riders together, as a tomb laid in water, * by the mighty strength of the right of the Master."
SteiveiThalassis1.text = "Israel treaded on the sea's swelling billow,"
SteiveiThalassis2.text = "which had been rendered once again into dry land."
SteiveiThalassis3.text = "Then the dark waters concealed all the Egyptian"
SteiveiThalassis4.text = "riders together, as a tomb laid in water,"
SteiveiThalassis5.text = "by the mighty strength of the right of the Master."

SyToLogoSouKyrie.notmetered = "N/M (You established.)"
SyToLogoSouKyrie.name = "You established."
SyToLogoSouKyrie.incipit = "You established…"
SyToLogoSouKyrie.mode = misc_en_US_goadedes.Mode2
SyToLogoSouKyrie.ode = misc_en_US_goadedes.Ode3
SyToLogoSouKyrie.text = "You established the heavens by Your divine word, almighty God. * Now I pray You also establish my heart in the faith, O Lord."

TaCherouvimMimoumenoi.notmetered = "N/M (The Servants in the furnace.)"
TaCherouvimMimoumenoi.name = "The Servants in the furnace."
TaCherouvimMimoumenoi.incipit = "The Servants in the furnace…"
TaCherouvimMimoumenoi.mode = misc_en_US_goadedes.Mode2
TaCherouvimMimoumenoi.ode = misc_en_US_goadedes.Ode7
TaCherouvimMimoumenoi.text = "The Servants in the furnace fire * danced as they imitated * the Cherubim, and cried out: * Blessed are You, O Lord our God, * for You have caused all this to befall us and Your holy city, in truth and in judgment, on account of our sins. * You are supremely extolled and You are glorified, unto all the ages."

TinAchlynTisPsychisMou.notmetered = "N/M ()"
TinAchlynTisPsychisMou.name = ""
TinAchlynTisPsychisMou.incipit = "…"
TinAchlynTisPsychisMou.mode = misc_en_US_goadedes.Mode2
TinAchlynTisPsychisMou.ode = misc_en_US_goadedes.Ode5
TinAchlynTisPsychisMou.text = ""

